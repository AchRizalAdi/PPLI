ents/section/ForgotSection.vue","sourceRoot":"","sourcesContent":["<template>\r\n    <section class=\"login-section section-space-b pt-4 pt-md-5 mt-md-3\">\r\n        <div class=\"container\">\r\n                <div class=\"row align-items-center justify-content-center\">\r\n                    <div class=\"col-lg-6 mb-5 mb-lg-0 d-none d-lg-block\">\r\n                        <img :src=\"SectionData.forgot.img\" alt=\"\" class=\"img-fluid\">\r\n                    </div><!-- end col-lg-6 -->\r\n                    <div class=\"col-lg-6\">\r\n                        <div class=\"section-head-sm\">\r\n                            <h2 class=\"mb-1\">{{ SectionData.forgot.title }}</h2>\r\n                            <p>{{ SectionData.forgot.subTitle }}</p>\r\n                        </div>\r\n                        <form @submit.prevent=\"forget()\">\r\n                            <div class=\"form-floating mb-4 mt-4\">\r\n                                <input type=\"text\" class=\"form-control\" id=\"email\" placeholder=\"Email\">\r\n                                <label for=\"email\">Email</label>\r\n                            </div><!-- end form-floating -->\r\n                            <button class=\"btn btn-dark w-100\" type=\"submit\">kirim</button>\r\n                        </form>\r\n                    </div><!-- end col-lg-6 -->\r\n                </div><!-- end row -->\r\n            </div><!-- end container -->\r\n    </section>\r\n</template>\r\n<script>\r\n\r\nimport SectionData from '@/store/store.js'\r\nimport axios from 'axios';\r\nexport default {\r\n  name: 'ForgotSection',\r\n  data () {\r\n    return {\r\n      SectionData,\r\n      email:[],\r\n    }\r\n  },\r\n  methods:{\r\n     makeToast(append = false) {\r\n        this.toastCount++\r\n        this._vm.$bvToast(`This is toast number ${this.toastCount}`, {\r\n          title: 'BootstrapVue Toast',\r\n          autoHideDelay: 5000,\r\n          appendToast: append\r\n        })\r\n      },\r\n        reset(){\r\n            this.email=null;\r\n        },\r\n        forget(){\r\n            // alert('a');\r\n            axios.post('http://127.0.0.1:8000/api/forgot',{\r\n                email:this.email,\r\n            }).then(function (response) {\r\n                this.reset()\r\n                this.$toast.show(response.data.message);\r\n            }.bind(this));\r\n        }\r\n  },\r\n  mounted () {\r\n    /*  ======== Show/Hide passoword ======== */\r\n    function showHidePassword(selector){\r\n        let elem = document.querySelectorAll(selector);\r\n        if(elem.length > 0){\r\n            elem.forEach(item => {\r\n            item.addEventListener(\"click\", function(e){\r\n                e.preventDefault();\r\n                let target = document.getElementById(item.getAttribute(\"href\"));\r\n                if(target.type == \"password\") {\r\n                target.type = \"text\";\r\n                item.classList.add(\"is-shown\");\r\n                }else{\r\n                target.type = \"password\";\r\n                item.classList.remove(\"is-shown\");\r\n                }\r\n            });\r\n\r\n            });\r\n        }\r\n    }\r\n\r\n    showHidePassword(\".password-toggle\");\r\n\r\n  }\r\n}\r\n</script>"]}]}