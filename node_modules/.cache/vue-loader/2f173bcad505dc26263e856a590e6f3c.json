        <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"kode\"\r\n                  placeholder=\"kode\"\r\n                  v-model=\"kode\"\r\n                  required\r\n                />\r\n                <label for=\"kode\">Kode</label>\r\n              </div>\r\n              <div class=\"form-floating mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"nama\"\r\n                  placeholder=\"Nama\"\r\n                  v-model=\"nama\"\r\n                  required\r\n                />\r\n                <label for=\"nama\">Nama</label>\r\n              </div>\r\n              <div class=\"form-floating mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"alamat_kantor\"\r\n                  placeholder=\"alamat_kantor\"\r\n                  v-model=\"alamat_kantor\"\r\n                  required\r\n                />\r\n                <label for=\"alamat_kantor\">Alamat Kantor</label>\r\n              </div>\r\n              <div class=\"form-floating mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"email\"\r\n                  placeholder=\"email\"\r\n                  v-model=\"email\"\r\n                  required\r\n                />\r\n                <label for=\"email\">Email</label>\r\n              </div>\r\n              <div class=\"form-floating mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"nomor\"\r\n                  placeholder=\"nomor\"\r\n                  v-model=\"nomor\"\r\n                  required\r\n                />\r\n                <label for=\"nomor\">Nomor</label>\r\n              </div>\r\n              <