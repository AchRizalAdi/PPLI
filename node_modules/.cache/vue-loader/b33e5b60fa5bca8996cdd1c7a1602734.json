nData.contactsData.contactsTableHead\"\r\n                :key=\"i\"\r\n              >\r\n                {{ list }}\r\n              </th>\r\n            </tr>\r\n          </thead>\r\n          <tbody class=\"fs-13\">\r\n            <tr v-for=\"(item, index) in kontak.data\" :key=\"item.id\">\r\n              <td scope=\"row\">\r\n                <a href=\"#\">{{ index + 1 }}</a>\r\n              </td>\r\n              <td>{{ item.nama }}</td>\r\n              <td>{{ item.alamat }}</td>\r\n              <td>{{ item.email }}</td>\r\n              <td>{{ item.nomor }}</td>\r\n              <td>{{ item.status }}</td>\r\n              <td class=\"row\">\r\n                <router-link\r\n                v-if=\"checkPrivilege('kontak-show')\"\r\n                  :to=\"{ name: 'show-contact', params: { id: item.id } }\"\r\n                  class=\"col- icon-btn p-0 m-0\"\r\n                  title=\"Show\"\r\n                  ><em class=\"fa fa-eye\"></em\r\n                ></router-link>\r\n                <!-- <button  @click=\"showCities(item.id)\" class=\"col-sm-1 icon-btn \" title=\"Show\" data-bs-toggle=\"modal\"  data-bs-target=\"#editModal\"><em class=\"fa fa-eye\" ></em></button> -->\r\n                <router-link\r\n                v-if=\"checkPrivilege('kontak-update')\"\r\n                  :to=\"{ name: 'edit-contact', params: { id: item.id } }\"\r\n                  class=\"col- p-0 m-0 icon-btn\"\r\n                  title=\"Edit\"\r\n                  ><em class=\"fa fa-pencil-square-o\"></em\r\n                ></router-link>\r\n                <button\r\n                v-if=\"checkPrivilege('kontak-delete')\"\r\n                  @click=\"deleteKontak(item.id)\"\r\n                  class=\"col- p-0 m-0 icon-btn\"\r\n                  title=\"Delete\"\r\n                >\r\n                  <em class=\"ni ni-trash\"></em>\r\n                </button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <!-- end table-responsive -->\r\n      <!-- pagination -->\r\n      <div class=\"text-center mt-4 mt-md-5\">\r\n        <Pagination\r\n          :records=\"records.length\"\r\n          v-model=\"page\"\r\n          :per-page=\"perPage\"\r\n        ></Pagination>\r\n      </div>\r\n    </div>\r\n    <!-- end profile-setting-panel-wrap-->\r\n  </div>\r\n  <!-- end col-lg-8 -->\r\n</template>\r\n\r\n<script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\nimport SectionData from \"@/store/store.js\";\r\nimport Pagination from \"v-pagination-3\";\r\nimport axios from \"axios\";\r\nimport $ from \"jquery\";\r\n// import { reactive } from 'vue';\r\n// import { onMounted, ref } from 'vue';\r\n\r\nexport default {\r\n  components: {\r\n    Pagination,\r\n  },\r\n  data() {\r\n    return {\r\n      SectionData,\r\n      page: 1,\r\n      i: 1,\r\n      perPage: 6,\r\n      records: [],\r\n      kontak: [],\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    resetnama() {\r\n      this.name = null;\r\n    },\r\n    getKontak() {\r\n      axios.get(\"http://127.0.0.1:8000/api/kontak\").then(\r\n        function (response) {\r\n          this.kontak = response.data;\r\n          $(document).ready(function () {\r\n            $(\"#dataTable\").DataTable();\r\n          });\r\n        }.bind(this)\r\n      );\r\n    },\r\n    deleteKontak(id) {\r\n      if (confirm(\"Apakah Kamu Yakin?\")) {\r\n        axios.delete(\"http://127.0.0.1:8000/api/kontak/\" + id).then(\r\n          function () {\r\n            this.getKontak();\r\n            this.$toast.error(\"Berhasil delete\");\r\n          }.bind(this)\r\n        );\r\n      }\r\n    },\r\n    checkPrivilege(privilege) {\r\n      const permission = localStorage.getItem(\"permission\");\r\n      let status = false;\r\n      JSON.parse(permission).forEach((data) => {\r\n        if (data === privilege) {\r\n          status = true;\r\n        }\r\n      });\r\n      return status;\r\n    },\r\n  },\r\n  created: function () {\r\n    this.getKontak();\r\n  },\r\n  computed: {\r\n    displayedRecords() {\r\n      const startIndex = this.perPage * (this.page - 1);\r\n      const endIndex = startIndex + this.perPage;\r\n      return this.records.slice(startIndex, endIndex);\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}