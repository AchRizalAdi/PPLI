e-box\">\r\n                            <h3>{{SectionData.wilayahData.mainTitle }}</h3>\r\n                        </div><!-- end user-panel-title-box -->\r\n                        \r\n                <!-- {{ cities}} -->\r\n                        <div class=\"d-grid gap-2 d-md-block\">\r\n                            <button @click=\"resetnama()\" type=\"button\" class=\"btn btn-primary btn-sm mb-2\" data-bs-toggle=\"modal\" data-bs-target=\"#messageModal\">Add Wilayah</button>\r\n                        </div>\r\n                        <div class=\"profile-setting-panel-wrap\">\r\n                            <div class=\"table-responsive\">\r\n                                <table class=\"table mb-0 table-s2\" id=\"dataTable\">\r\n                                    <thead class=\"fs-14\">\r\n                                        <tr>\r\n                                            <th scope=\"col\" v-for=\"(list, i) in SectionData.wilayahData.wilayahTableHead\" :key=\"i\">{{ list }}</th>\r\n                                        </tr>\r\n                                    </thead>\r\n                                    <tbody class=\"fs-13\">\r\n                                        <tr v-for=\"item in wilayah.data\"  :key=\"item.id\">\r\n                                            <td scope=\"row\"  ><a href=\"#\">{{ item.id }}</a>  </td>\r\n                                            <td>{{ item.name }}</td>\r\n                                            <td>{{ item.email }}</td>\r\n                                            <td>{{ item.kota }}</td>\r\n                                            <td>{{ item.alamat }}</td>\r\n                                            <td>{{ item.nomor }}</td>\r\n                                            <td class=\"row\">\r\n                                                <button  @click=\"showWilayah(item.id)\" class=\"col icon-btn\" title=\"Edit\" data-bs-toggle=\"modal\"  data-bs-target=\"#editModal\"><em class=\"fa fa-pencil-square-o\"></em></button>\r\n                                                <button   @click=\"deleteWilayah(item.id)\" class=\"col icon-btn \" title=\"Delete\" ><em class=\"ni ni-trash\"></em></button>\r\n                                            </td>\r\n                                        </tr> \r\n                                    </tbody>\r\n                                </table>\r\n                            </div><!-- end table-responsive -->\r\n                            <!-- pagination -->\r\n                            <div class=\"text-center mt-4 mt-md-5\">\r\n                                <Pagination :records=\"records.length\" v-model=\"page\" :per-page=\"perPage\"></Pagination>\r\n                            </div>\r\n                        </div><!-- end profile-setting-panel-wrap-->\r\n            <!-- Modal store -->\r\n             <form @submit.prevent=\"postWilayah()\">\r\n                <div class=\"modal fade\" id=\"messageModal\" tabindex=\"-1\" aria-labelledby=\"reportModalLabel\" aria-hidden=\"true\" >\r\n                    <div class=\"modal-dialog modal-dialog-centered\">\r\n                        <div class=\"modal-content\">\r\n                            <div class=\"modal-header\">\r\n                                <h4 class=\"modal-title\" id=\"reportModalLabel\">Add Wilayah</h4>\r\n                                <button type=\"button\" class=\"btn-close icon-btn\" data-bs-dismiss=\"modal\" aria-label=\"Close\">\r\n                                    <em class=\"ni ni-cross\"></em>\r\n                                </button>\r\n                            </div>\r\n                            <div class=\"modal-body\">\r\n                               <div class=\"form-floating mb-3\">\r\n                                <input type=\"text\" class=\"form-control\" id=\"nama\" placeholder=\"name\"  v-model=\"name\"  required>\r\n                                <label for=\"nama\">name</label>\r\n                            </div><!-- end form-floating -->  \r\n                             <div class=\"form-floating mb-3 mt-4\">\r\n                                <input type=\"text\" class=\"form-control\" id=\"email\" placeholder=\"email\"  v-model=\"email\"  required>\r\n                                <label for=\"email\">email</label>\r\n                            </div><!-- end form-floating --> \r\n                                <div class=\"form-group mb-2\">\r\n                                  <label>Kota </label>\r\n                                    <select v-model=\"kota\" class='form-control' required>\r\n                                        <option  v-for='data in cities.data' :value='data.name' :key='data.id' >{{ data.name }}</option>\r\n                                    </select>\r\n                            </div><!-- end form-floating --> \r\n                             <div class=\"form-floating mb-3 mt-4\">\r\n                                <input type=\"text\" class=\"form-control\" id=\"alamat\" placeholder=\"alamat\"  v-model=\"alamat\"  required>\r\n                                <label for=\"alamat\">alamat</label>\r\n                            </div><!-- end form-floating --> \r\n                             <div class=\"form-floating mb-3 mt-4\">\r\n                                <input type=\"text\" class=\"form-control\" id=\"nomor\" placeholder=\"Nomor\"  v-model=\"nomor\"  required>\r\n                                <label for=\"nomor\">Nomor</label>\r\n                            </div><!-- end form-floating --> \r\n                             <div class=\"form-check mb-3 mt-2\">\r\n                                 <input v-model=\"hq\" class=\"form-check-input\" type=\"checkbox\"  id=\"checkbox\">\r\n                                         <label class=\"form-check-label\" for=\"checkbox\">\r\n                                             HQ\r\n                                         </label>\r\n                            </div><!-- end form-floating --> \r\n                                <button class=\"btn btn-dark w-100\" data-bs-dismiss=\"modal\" type=\"submit\">Add</button>\r\n                            </div><!-- end modal-body -->\r\n                        </div><!-- end modal-content -->\r\n                    </div><!-- end modal-dialog -->\r\n                </div><!-- end modal-->\r\n            </form>\r\n               <form @submit.prevent=\"putWilayah(id)\">\r\n                <div class=\"modal fade\" id=\"editModal\" tabindex=\"-1\" aria-labelledby=\"reportModalLabel\" aria-hidden=\"true\" >\r\n                    <div class=\"modal-dialog modal-dialog-centered\">\r\n                        <div class=\"modal-content\">\r\n                            <div class=\"modal-header\">\r\n                                <!-- {{name}} -->\r\n                                <h4 class=\"modal-title\" id=\"reportModalLabel\">edit Wilayah</h4>\r\n                                <button type=\"button\" class=\"btn-close icon-btn\" data-bs-dismiss=\"modal\" aria-label=\"Close\">\r\n                                    <em class=\"ni ni-cross\"></em>\r\n                                </button>\r\n                            </div>\r\n                            <div class=\"modal-body\">\r\n                               <div class=\"form-floating mb-3\">\r\n                                <input type=\"text\" class=\"form-control\" id=\"nama\" placeholder=\"name\"  v-model=\"name\"  required>\r\n                                <label for=\"nama\">name</label>\r\n                            </div><!-- end form-floating -->  \r\n                             <div class=\"form-floating mb-3 mt-4\">\r\n                                <input type=\"text\" class=\"form-control\" id=\"email\" placeholder=\"email\"  v-model=\"email\"  required>\r\n                                <label for=\"email\">email</label>\r\n                            </div><!-- end form-floating --> \r\n                                <div class=\"form-group mb-2\">\r\n                                  <label>Kota  </label>\r\n                                    <select v-model=\"kota\" class='form-control' required>\r\n                                        <option  v-for='data in cities.data' :value='data.name' :key='data.id' >{{ data.name }}</option>\r\n                                    </select>\r\n                            </div><!-- end form-floating --> \r\n                             <div class=\"form-floating mb-3 mt-4\">\r\n                                <input type=\"text\" class=\"form-control\" id=\"alamat\" placeholder=\"alamat\"  v-model=\"alamat\"  required>\r\n                                <label for=\"alamat\">alamat</label>\r\n                            </div><!-- end form-floating --> \r\n                             <div class=\"form-floating mb-3 mt-4\">\r\n                                <input type=\"text\" class=\"form-control\" id=\"nomor\" placeholder=\"Nomor\"  v-model=\"nomor\"  required>\r\n                                <label for=\"nomor\">Nomor</label>\r\n                            </div><!-- end form-floating --> \r\n                           <div class=\"form-check mb-3 mt-2\">\r\n                                <input v-model=\"hq\" class=\"form-check-input\" type=\"checkbox\"  id=\"checkbox\">\r\n                                         <label class=\"form-check-label\" for=\"checkbox\">\r\n                                             HQ\r\n                                         </label>\r\n                            </div><!-- end form-floating --> \r\n                                <button class=\"btn btn-dark w-100\" data-bs-dismiss=\"modal\" type=\"submit\">edit</button>\r\n                            </div><!-- end modal-body -->\r\n                        </div><!-- end modal-content -->\r\n                    </div><!-- end modal-dialog -->\r\n                </div><!-- end modal-->\r\n            </form>\r\n             \r\n        </div><!-- end col-lg-8 -->          \r\n</template>\r\n\r\n<script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\nimport SectionData from '@/store/store.js'\r\nimport Pagination from 'v-pagination-3';\r\nimport axios from 'axios';\r\nimport $ from \"jquery\";\r\n// import { thisTypeAnnotation } from '@babel/types';\r\n// import { reactive } from 'vue';\r\n// import { onMounted, ref } from 'vue';\r\n\r\nexport default {\r\n    components: {\r\n    Pagination\r\n  },\r\n  data () {\r\n    return {\r\n      SectionData,\r\n      page: 1,\r\n      perPage: 6,\r\n      records: \"\",\r\n      wilayah: \"\",\r\n      cities:\"\",\r\n      kota:\"\",\r\n      name:\"\",\r\n      email:\"\",\r\n      alamat:\"\",\r\n      nomor:\"\",\r\n      hq:\"\",\r\n    //   data:\"\"\r\n\r\n    }\r\n  },\r\n  \r\n  methods: {\r\n    isChecked(data){\r\n        if(data){\r\n            return this.hq=true;\r\n        }else{\r\n            return this.hq=false;\r\n        }\r\n    },\r\n    resetnama() {\r\n        this.name=null;\r\n        this.email=null;\r\n        this.kota=null;\r\n        this.alamat=null;\r\n        this.nomor=null;\r\n\r\n    },\r\n     deleteWilayah (id){\r\n        if(confirm(\"Apakah Kamu Yakin?\")){\r\n        axios.delete('http://127.0.0.1:8000/api/wilayah/'+id)\r\n        .then(function () {\r\n                this.getWilayah()\r\n                this.$toast.show(\"berhasil delete\");\r\n        }.bind(this));\r\n        }\r\n        },\r\n    postWilayah(){\r\n        alert(this.hq);\r\n            axios.post('http://127.0.0.1:8000/api/wilayah', {\r\n                name: this.name,\r\n                email:this.email,\r\n                kota:this.kota,\r\n                alamat:this.alamat,\r\n                nomor:this.nomor,\r\n                HQ:this.hq\r\n            }).then(response => {\r\n                \r\n            this.getWilayah()\r\n            this.$toast.show(\"berhasil ditambahkan\");\r\n            console.log(response);\r\n            }).catch(error => {\r\n            this.$toast.show(\"gagal menambahkan\");\r\n            console.log(error);\r\n            })\r\n            this.name = '';\r\n            this.provinsi='';\r\n        },\r\n    putWilayah(id){\r\n            axios.post('http://127.0.0.1:8000/api/wilayah/'+id, {\r\n                name: this.name,\r\n                email:this.email,\r\n                kota:this.kota,\r\n                alamat:this.alamat,\r\n                nomor:this.nomor,\r\n                HQ:this.hq\r\n            }).then(response => {\r\n                \r\n            this.getWilayah()\r\n            this.$toast.show(\"berhasil diupdate\");\r\n            console.log(response);\r\n            }).catch(error => {\r\n            this.$toast.show(\"update gagal\");\r\n            console.log(error);\r\n            })\r\n            this.name = '';\r\n            this.provinsi='';\r\n        },\r\n    getCities : function(){\r\n            axios.get('http://127.0.0.1:8000/api/cities')\r\n            .then(function (response) {\r\n                        this.cities = response.data;\r\n             }.bind(this));\r\n    },\r\n     getWilayah : function(){\r\n        // alert(this.hq);\r\n        axios.get('http://127.0.0.1:8000/api/wilayah')\r\n        .then(function (response) {\r\n            this.wilayah = response.data;\r\n            $(document).ready(function () {\r\n            $('#dataTable').DataTable();\r\n        });\r\n        }.bind(this));\r\n    },\r\n    showWilayah(id){\r\n     axios.get('http://127.0.0.1:8000/api/wilayah/'+id)\r\n        .then(function (response) {\r\n            this.id = response.data.data.id;\r\n            this.name=response.data.data.name;\r\n            this.email=response.data.data.email;\r\n            this.kota=response.data.data.kota;\r\n            this.alamat=response.data.data.alamat;\r\n            this.nomor=response.data.data.nomor;\r\n            this.data=response.data.data.HQ;\r\n            this.hq = (response.data.data.HQ === 1)? true : false;\r\n            // alert(this.data);\r\n        }.bind(this));\r\n  },\r\n    \r\n \r\n  },\r\n   created: function(){\r\n            this.getWilayah(),\r\n             this.getCities()\r\n  },\r\n  computed: {\r\n      displayedRecords() {\r\n      const startIndex = this.perPage * (this.page - 1);\r\n      const endIndex = startIndex + this.perPage;\r\n      return this.records.slice(startIndex, endIndex);\r\n    }\r\n  },\r\n\r\n}\r\n</script>"]}]}