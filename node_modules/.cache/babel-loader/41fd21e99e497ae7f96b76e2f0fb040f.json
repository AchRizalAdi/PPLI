Ke,IAAI,CAACE,KALV,EAKc;IAAA;IALd,EAKcE,WALd;GAMS,CANT;;EAAA,GADF;;EAAA,oBAAsCjB,cASlC,CAXN,GAYAR,aAKCC,sBALD,EAKC;IAJCC,EAAE,EAAC,aAIJ;IAHCL,IAAI,EAAC,QAGN;IAFCL,KAAK,EAAC;EAEP,CALD;sBAIG,MAAI;;;;GAJP,GAMAkC,YA7EF;;EAAA,CAiFI,CAlFN,CAoFI,EADJ3B,6CACI,CArFN,CAuFI,EADJA,sDACI,CArGN,GAsGAA","names":["class","_createElementVNode","for","href","title","type","_hoisted_2","_createCommentVNode","_createVNode","_component_router_link","to","_hoisted_7","onSubmit","$options","id","placeholder","$data","$event","required","_hoisted_11","_hoisted_13","_hoisted_15","_hoisted_16","_hoisted_18","_hoisted_20","_createElementBlock","_Fragment","_renderList","data","item","value","name","key","_hoisted_21","_hoisted_23"],"sourceRoot":"","sources":["E:\\Codeee\\PPLI FE BE\\PPLI\\src\\components\\section\\UserManaSection.vue"],"sourcesContent":["<template>\r\n  <div class=\"col-lg-9 ps-xl-5\">\r\n    <div class=\"user-panel-title-box\">\r\n      <h3>Tambah User</h3>\r\n    </div>\r\n    <!-- end user-panel-title-box -->\r\n    <nav aria-label=\"breadcrumb\">\r\n      <ol class=\"breadcrumb breadcrumb-s1 justify-content-left mb-3\">\r\n        <li class=\"breadcrumb-item\">\r\n          <router-link to=\"/pengaturan\">User Manajemen</router-link>\r\n        </li>\r\n        <li class=\"breadcrumb-item\">Tambah User</li>\r\n      </ol>\r\n    </nav>\r\n\r\n    <div class=\"profile-setting-panel-wrap\">\r\n      <div class=\"table-responsive\">\r\n        <form @submit.prevent=\"postUser()\">\r\n          <!-- {{ registers }} -->\r\n\r\n          <div class=\"form-floating mb-3\">\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              id=\"name\"\r\n              placeholder=\"Nama\"\r\n              v-model=\"name\"\r\n              required\r\n            />\r\n            <label for=\"name\">Nama</label>\r\n          </div>\r\n          <!-- end form-floating -->\r\n          <div class=\"form-floating mb-3\">\r\n            <input\r\n              type=\"email\"\r\n              class=\"form-control\"\r\n              id=\"email\"\r\n              placeholder=\"Email\"\r\n              v-model=\"email\"\r\n              required\r\n            />\r\n            <label for=\"email\">Email</label>\r\n          </div>\r\n          <!-- end form-floating -->\r\n          <div class=\"form-floating mb-3\">\r\n            <input\r\n              type=\"password\"\r\n              class=\"form-control password\"\r\n              id=\"password\"\r\n              placeholder=\"Password\"\r\n              v-model=\"password\"\r\n              required\r\n            />\r\n            <label for=\"password\">Password</label>\r\n            <a\r\n              href=\"password\"\r\n              class=\"password-toggle\"\r\n              title=\"Toggle show/hide pasword\"\r\n            >\r\n              <em class=\"password-shown ni ni-eye-off\"></em>\r\n              <em class=\"password-hidden ni ni-eye\"></em>\r\n            </a>\r\n          </div>\r\n          <!-- end form-floating -->\r\n          <div class=\"form-floating mb-3\">\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              id=\"username\"\r\n              placeholder=\"Username\"\r\n              v-model=\"Username\"\r\n              required\r\n            />\r\n            <label for=\"username\">Username</label>\r\n          </div>\r\n          <!-- end form-floating -->\r\n          <div class=\"form-group mb-3\">\r\n            <label>Roles</label>\r\n            <select class=\"form-control\" v-model=\"roles\" required>\r\n              <option\r\n                v-for=\"item in roless.data\"\r\n                :value=\"item.name\"\r\n                :key=\"item.id\"\r\n              >\r\n                {{ item.name }}\r\n              </option>\r\n            </select>\r\n          </div>\r\n          <router-link\r\n            to=\"/pengaturan\"\r\n            type=\"button\"\r\n            class=\"btn btn-sm btn-dark\"\r\n            >Back</router-link\r\n          >\r\n          <button class=\"btn btn-sm btn-dark w-20 ms-2\" type=\"submit\">\r\n            Create User\r\n          </button>\r\n        </form>\r\n      </div>\r\n      <!-- end table-responsive -->\r\n    </div>\r\n    <!-- end profile-setting-panel-wrap-->\r\n  </div>\r\n  <!-- end col-lg-8 -->\r\n</template>\r\n\r\n<script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\nimport SectionData from \"@/store/store.js\";\r\nimport axios from \"axios\";\r\n// import $ from \"jquery\";\r\n// import { reactive } from 'vue';\r\n// import { onMounted, ref } from 'vue';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      SectionData,\r\n      page: 1,\r\n      i: 1,\r\n      perPage: 6,\r\n      users: [],\r\n      roless: [],\r\n      name: \"\",\r\n      email: \"\",\r\n      password: \"\",\r\n      Username: \"\",\r\n      roles: \"\",\r\n    };\r\n  },\r\n  //   http://127.0.0.1:8000/api/admin/adduser\r\n  methods: {\r\n    resetnama() {\r\n      this.name = null;\r\n    },\r\n    postUser() {\r\n      axios\r\n        .post(\"http://127.0.0.1:8000/api/admin/adduser\", {\r\n          name: this.name,\r\n          email: this.email,\r\n          password: this.password,\r\n          Username: this.Username,\r\n          roles: this.roles,\r\n        })\r\n        .then((response) => {\r\n          // this.getRoles()\r\n          this.$toast.success(\"berhasil ditambahkan\");\r\n          console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$toast.error(\"create user gagal\");\r\n          console.log(error);\r\n        });\r\n      this.name = \"\";\r\n      this.email = \"\";\r\n      this.password = \"\";\r\n      this.username = \"\";\r\n      this.roles = \"\";\r\n    },\r\n    getRoles() {\r\n      axios.get(\"http://127.0.0.1:8000/api/role/show\").then(\r\n        function (response) {\r\n          this.roless = response.data;\r\n        }.bind(this)\r\n      );\r\n    },\r\n  },\r\n  created: function () {\r\n    this.getRoles();\r\n  },\r\n  computed: {\r\n    displayedRecords() {\r\n      const startIndex = this.perPage * (this.page - 1);\r\n      const endIndex = startIndex + this.perPage;\r\n      return this.records.slice(startIndex, endIndex);\r\n    },\r\n  },\r\n  mounted() {\r\n    /*  ======== Show/Hide passoword ======== */\r\n    function showHidePassword(selector) {\r\n      let elem = document.querySelectorAll(selector);\r\n      if (elem.length > 0) {\r\n        elem.forEach((item) => {\r\n          item.addEventListener(\"click\", function (e) {\r\n            e.preventDefault();\r\n            let target = document.getElementById(item.getAttribute(\"href\"));\r\n            if (target.type == \"password\") {\r\n              target.type = \"text\";\r\n              item.classList.add(\"is-shown\");\r\n            } else {\r\n              target.type = \"password\";\r\n              item.classList.remove(\"is-shown\");\r\n            }\r\n          });\r\n        });\r\n      }\r\n    }\r\n\r\n    showHidePassword(\".password-toggle\");\r\n  },\r\n};\r\n</script>\r\n"]}]}