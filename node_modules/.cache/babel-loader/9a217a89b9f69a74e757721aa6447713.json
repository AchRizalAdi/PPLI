{ \r\n    Products\r\n  },\r\n  data () {\r\n    return {\r\n      SectionData,\r\n      filterMenu: [\r\n        {\r\n            id: 1,\r\n            title: 'All',\r\n            class: 'active',\r\n            options: [\r\n                {\r\n                    category: 'freight forwarding'\r\n                },\r\n                {\r\n                    category: 'trucking'\r\n                },\r\n                {\r\n                    category: 'warehouse'\r\n                },\r\n                {\r\n                    category: 'more'\r\n                },\r\n            ]\r\n        },\r\n        {\r\n            id: 2,\r\n            title: 'Freight Forwarding',\r\n            class: '',\r\n            options: [\r\n                {\r\n                    category: 'freight forwarding'\r\n                }\r\n            ]\r\n        },\r\n        {\r\n            id: 3,\r\n            title: 'Trucking',\r\n            class: '',\r\n            options: [\r\n                {\r\n                    category: 'trucking'\r\n                }\r\n            ]\r\n        },\r\n        {\r\n            id: 4,\r\n            title: 'Warehouse',\r\n            class: '',\r\n            options: [\r\n                {\r\n                    category: 'warehouse'\r\n                }\r\n            ]\r\n        },\r\n        {\r\n            id: 5,\r\n            title: 'More',\r\n            class: '',\r\n            options: [\r\n                {\r\n                    category: 'more'\r\n                }\r\n            ]\r\n        },\r\n      ],\r\n      selectedTab: null,\r\n      previous_active_id: 1\r\n    }\r\n  },\r\n  methods: {\r\n    setTab(tab, id) {\r\n      this.selectedTab = tab;\r\n      if (this.previous_active_id === id) return;\r\n      this.filterMenu.find(menu => menu.id === this.previous_active_id).class = '';\r\n      this.filterMenu.find(menu => menu.id === id).class = 'active';\r\n      this.previous_active_id = id;\r\n    },\r\n  },\r\n  computed: {\r\n      filteredData () {\r\n        return this.SectionData.productData.products.filter(data => {\r\n        if (this.selectedTab === null) return true\r\n        const opts = this.selectedTab.options.map(opt => opt.category)\r\n        return opts.includes(data.category)\r\n      })\r\n    },\r\n  }\r\n  \r\n}\r\n</script>\r\n\r\n<style lang=\"css\" scoped>\r\n .details {\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    z-index: 1;\r\n }\r\n .author-link {\r\n   z-index: 2;\r\n   position: relative;\r\n }\r\n</style>"]}]}