;UAC1B1B,IAAI,CAACW,IAAL,CAAU,iBAAV,EAA6B,EAA7B,EAAiC,MAAjC;QACF;MACD,CAdD;IAeD,CA1BM;;IA2BPgB,SAAS,GAAG;MACV,KAAKnB,IAAL,GAAY,IAAZ;IACD,CA7BM;;IA8BPoB,YAAY,EAAE,YAAY;MACxB9B,KAAK,CAAC+B,GAAN,CAAU,oCAAV,EAAgDP,IAAhD,CACE,UAAUQ,QAAV,EAAoB;QAClB,KAAKxB,SAAL,GAAiBwB,QAAQ,CAAC5B,IAA1B;QACA6B,UAAU,CAAC,MAAM;UACfhC,CAAC,CAAC,eAAD,CAAD,CAAmBiC,SAAnB;QACD,CAFS,EAEP,IAFO,CAAV;MAGD,CALD,CAKEC,IALF,CAKO,IALP,CADF;IAQD,CAvCM;IAwCPC,YAAY,EAAE,YAAY;MACxBpC,KAAK,CAAC+B,GAAN,CAAU,oCAAV,EAAgDP,IAAhD,CACE,UAAUQ,QAAV,EAAoB;QAClB,KAAKvB,SAAL,GAAiBuB,QAAQ,CAAC5B,IAA1B;QACA6B,UAAU,CAAC,MAAM;UACfhC,CAAC,CAAC,gBAAD,CAAD,CAAoBiC,SAApB;QACD,CAFS,EAEP,IAFO,CAAV;MAGD,CALD,CAKEC,IALF,CAKO,IALP,CADF;IAQD,CAjDM;;IAkDPR,cAAc,CAACP,EAAD,EAAK;MACjB;MAEApB,KAAK,CAACqC,MAAN,CAAa,wCAAwCjB,EAArD,EAAyDI,IAAzD,CACE,YAAY;QACV;QACA,KAAKM,YAAL;MACD,CAHD,CAGEK,IAHF,CAGO,IAHP,CADF;IAMD,CA3DM;;IA4DPG,YAAY,CAAClB,EAAD,EAAK;MACf;MACApB,KAAK,CAAC+B,GAAN,CAAU,wCAAwCX,EAAlD,EAAsDI,IAAtD,CACE,UAAUQ,QAAV,EAAoB;QAClB,KAAKO,IAAL,GAAYP,QAAQ,CAAC5B,IAAT,CAAcA,IAAd,CAAmBgB,EAA/B;QACA,KAAKV,IAAL,GAAYsB,QAAQ,CAAC5B,IAAT,CAAcA,IAAd,CAAmBM,IAA/B;MACD,CAHD,CAGEyB,IAHF,CAGO,IAHP,CADF;IAMD,CApEM;;IAqEPK,YAAY,CAACpB,EAAD,EAAK;MACf;MACApB,KAAI,CACDyC,IADH,CACQ,wCAAwCrB,EADhD,EACoD;QAChDV,IAAI,EAAE,KAAKA;MADqC,CADpD,EAIGc,IAJH,CAISQ,QAAD,IAAc;QAClB,KAAKpB,QAAL;QACA,KAAKkB,YAAL,GAFkB,CAGlB;;QACAY,OAAO,CAACC,GAAR,CAAYX,QAAZ;MACD,CATH,EAUGY,KAVH,CAUUC,KAAD,IAAW;QAChB,KAAKC,MAAL,CAAYD,KAAZ,CAAkB,cAAlB;QACAH,OAAO,CAACC,GAAR,CAAYE,KAAZ;MACD,CAbH;MAcA,KAAKnC,IAAL,GAAY,EAAZ;IACD,CAtFM;;IAuFPqC,aAAa,GAAG;MACd/C,KAAI,CACDyC,IADH,CACQ,oCADR,EAC8C;QAC1C/B,IAAI,EAAE,KAAKA;MAD+B,CAD9C,EAIGc,IAJH,CAISQ,QAAD,IAAc;QAClB,KAAKpB,QAAL;QACA,KAAKkB,YAAL;QACAY,OAAO,CAACC,GAAR,CAAYX,QAAZ;MACD,CARH,EASGY,KATH,CASUC,KAAD,IAAW;QAChBG,KAAK,CAAC,oBAAD,CAAL;QACAN,OAAO,CAACC,GAAR,CAAYE,KAAZ;MACD,CAZH;MAaA,KAAKnC,IAAL,GAAY,EAAZ;IACD,CAtGM;;IAuGPuC,cAAc,CAACC,SAAD,EAAY;MACxB,MAAMC,UAAS,GAAIC,YAAY,CAACC,OAAb,CAAqB,YAArB,CAAnB;MACA,IAAIC,MAAK,GAAI,KAAb;MACAC,IAAI,CAACC,KAAL,CAAWL,UAAX,EAAuBM,OAAvB,CAAgCrD,IAAD,IAAU;QACvC,IAAIA,IAAG,KAAM8C,SAAb,EAAwB;UACtBI,MAAK,GAAI,IAAT;QACF;MACD,CAJD;MAKA,OAAOA,MAAP;IACD;;EAhHM,CAjBI;EAmIbI,OAAO,EAAE,YAAY;IACnB,KAAK5B,YAAL;IACA,KAAKM,YAAL;EACD,CAtIY;EAuIbuB,QAAQ,EAAE;IACRC,gBAAgB,GAAG;MACjB,MAAMC,UAAS,GAAI,KAAKvD,OAAL,IAAgB,KAAKD,IAAL,GAAY,CAA5B,CAAnB;MACA,MAAMyD,QAAO,GAAID,UAAS,GAAI,KAAKvD,OAAnC;MACA,OAAO,KAAKC,OAAL,CAAawD,KAAb,CAAmBF,UAAnB,EAA+BC,QAA/B,CAAP;IACD;;EALO;AAvIG,CAAf","names":["SectionData","Pagination","axios","$","Swal","components","data","page","perPage","records","provinsis","transaksi","name","methods","showPost","fire","position","icon","title","showConfirmButton","timer","showDelete","id","showDenyButton","confirmButtonText","denyButtonText","then","result","isConfirmed","deleteProvinsi","isDenied","resetnama","getProvinsis","get","response","setTimeout","DataTable","bind","getTransaksi","delete","showProvinsi","edit","putProvinsis","post","console","log","catch","error","$toast","postProvinsis","alert","checkPrivilege","privilege","permission","localStorage","getItem","status","JSON","parse","forEach","created","computed","displayedRecords","startIndex","endIndex","slice"],"sourceRoot":"","sources":["E:\\Codeee\\PPLI FE BE\\PPLI\\src\\components\\section\\TransaksiSection.vue"],"sourcesContent":["<template>\r\n  <div class=\"col-lg-9 ps-xl-5\">\r\n    <div class=\"user-panel-title-box\">\r\n      <h3>Transaksi</h3>\r\n    </div>\r\n    <!-- end user-panel-title-box -->\r\n\r\n    {{ transaksi }}\r\n    <div class=\"d-flex flex-row\">\r\n      <div\r\n        v-if=\"checkPrivilege('provinsi-store')\"\r\n        class=\"d-grid gap-2 d-md-block\"\r\n      >\r\n        <router-link\r\n          to=\"/add-transaksi\"\r\n          @click=\"resetnama()\"\r\n          type=\"button\"\r\n          class=\"btn btn-sm btn-dark mb-2\"\r\n        >\r\n          <span class=\"ni ni-plus m-0 p-0\"></span>\r\n        </router-link>\r\n      </div>\r\n      <div\r\n        v-if=\"checkPrivilege('provinsi-store')\"\r\n        class=\"d-grid ms-1 gap-2 d-md-block\"\r\n      >\r\n        <button\r\n          @click=\"resetnama()\"\r\n          type=\"button\"\r\n          class=\"btn btn-sm btn-dark mb-2\"\r\n          data-bs-toggle=\"modal\"\r\n          data-bs-target=\"#messageModal\"\r\n        >\r\n          Rekap Bulanan\r\n        </button>\r\n      </div>\r\n      <div\r\n        v-if=\"checkPrivilege('provinsi-store')\"\r\n        class=\"d-grid ms-1 gap-2 d-md-block\"\r\n      >\r\n        <button\r\n          @click=\"resetnama()\"\r\n          type=\"button\"\r\n          class=\"btn btn-sm btn-dark mb-2\"\r\n          data-bs-toggle=\"modal\"\r\n          data-bs-target=\"#messageModal\"\r\n        >\r\n          Laporan Pembukuan\r\n        </button>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"profile-setting-panel-wrap\">\r\n      <div class=\"table\">\r\n        <table class=\"table mb-0 table-s2\" id=\"dataTransaksi\">\r\n          <thead class=\"fs-14\">\r\n            <tr>\r\n              <th\r\n                scope=\"col\"\r\n                v-for=\"(list, i) in SectionData.transaksiData\r\n                  .transaksiTableHead\"\r\n                :key=\"i\"\r\n              >\r\n                {{ list }}\r\n              </th>\r\n            </tr>\r\n          </thead>\r\n          <tbody class=\"fs-13\">\r\n            <tr v-for=\"item in transaksi.data\" :key=\"item.id\">\r\n              <td scope=\"row\">\r\n                <a href=\"#\">{{ item.id }}</a>\r\n              </td>\r\n              <td>{{ item.name }}</td>\r\n              <td class=\"row\">\r\n                <button\r\n                  v-if=\"checkPrivilege('provinsi-update')\"\r\n                  @click=\"showProvinsi(item.id)\"\r\n                  class=\"col- icon-btn p-0 m-0\"\r\n                  title=\"Edit\"\r\n                  data-bs-toggle=\"modal\"\r\n                  data-bs-target=\"#updateModal\"\r\n                >\r\n                  <em class=\"fa fa-pencil-square-o\"></em>\r\n                </button>\r\n                <button\r\n                  v-if=\"checkPrivilege('provinsi-delete')\"\r\n                  @click=\"showDelete(item.id)\"\r\n                  class=\"col- icon-btn p-0 m-0\"\r\n                  title=\"Delete\"\r\n                >\r\n                  <em class=\"ni ni-trash\"></em>\r\n                </button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <!-- end table-responsive -->\r\n      <!-- pagination -->\r\n      <div class=\"text-center mt-4 mt-md-5\">\r\n        <Pagination\r\n          :records=\"records.length\"\r\n          v-model=\"page\"\r\n          :per-page=\"perPage\"\r\n        ></Pagination>\r\n      </div>\r\n    </div>\r\n    <!-- end profile-setting-panel-wrap-->\r\n    <!-- Modal store -->\r\n    <form @submit.prevent=\"postProvinsis()\">\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"messageModal\"\r\n        tabindex=\"-1\"\r\n        aria-labelledby=\"reportModalLabel\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog modal-dialog-centered\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h4 class=\"modal-title\" id=\"reportModalLabel\">Add Provinsi</h4>\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn-close icon-btn\"\r\n                data-bs-dismiss=\"modal\"\r\n                aria-label=\"Close\"\r\n              >\r\n                <em class=\"ni ni-cross\"></em>\r\n              </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"form-floating mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"nama\"\r\n                  placeholder=\"name\"\r\n                  v-model=\"name\"\r\n                  required\r\n                />\r\n                <label for=\"nama\">Nama</label>\r\n              </div>\r\n              <!-- end form-floating -->\r\n              <button\r\n                class=\"btn btn-dark w-100\"\r\n                data-bs-dismiss=\"modal\"\r\n                type=\"submit\"\r\n              >\r\n                Add\r\n              </button>\r\n            </div>\r\n            <!-- end modal-body -->\r\n          </div>\r\n          <!-- end modal-content -->\r\n        </div>\r\n        <!-- end modal-dialog -->\r\n      </div>\r\n      <!-- end modal-->\r\n    </form>\r\n    <form @submit.prevent=\"putProvinsis(edit)\">\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"updateModal\"\r\n        tabindex=\"-1\"\r\n        aria-labelledby=\"reportModalLabel\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog modal-dialog-centered\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h4 class=\"modal-title\" id=\"reportModalLabel\">update provinsi</h4>\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn-close icon-btn\"\r\n                data-bs-dismiss=\"modal\"\r\n                aria-label=\"Close\"\r\n              >\r\n                <em class=\"ni ni-cross\"></em>\r\n              </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"form-floating mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"name\"\r\n                  placeholder=\"name\"\r\n                  v-model=\"name\"\r\n                  required\r\n                />\r\n                <!-- <label for=\"nama\">{{edit.data.name}}</label> -->\r\n              </div>\r\n              <!-- end form-floating -->\r\n              <button\r\n                class=\"btn btn-dark w-100\"\r\n                data-bs-dismiss=\"modal\"\r\n                type=\"submit\"\r\n              >\r\n                update\r\n              </button>\r\n            </div>\r\n            <!-- end modal-body -->\r\n          </div>\r\n          <!-- end modal-content -->\r\n        </div>\r\n        <!-- end modal-dialog -->\r\n      </div>\r\n      <!-- end modal-->\r\n    </form>\r\n  </div>\r\n  <!-- end col-lg-8 -->\r\n</template>\r\n\r\n<script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\nimport SectionData from \"@/store/store.js\";\r\nimport Pagination from \"v-pagination-3\";\r\nimport axios from \"axios\";\r\nimport $ from \"jquery\";\r\nimport Swal from \"sweetalert2\";\r\n// import { reactive } from 'vue';\r\n// import { onMounted, ref } from 'vue';\r\n\r\nexport default {\r\n  components: {\r\n    Pagination,\r\n  },\r\n  data() {\r\n    return {\r\n      SectionData,\r\n      page: 1,\r\n      perPage: 6,\r\n      records: [],\r\n      provinsis: [],\r\n      transaksi: [],\r\n      //   edit:'',\r\n      name: \"\",\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    showPost() {\r\n      Swal.fire({\r\n        position: \"top-end\",\r\n        icon: \"success\",\r\n        title: \"Data telah tersimpan!\",\r\n        showConfirmButton: false,\r\n        timer: 1500,\r\n      });\r\n    },\r\n    showDelete(id) {\r\n      Swal.fire({\r\n        title: \"Apakah anda ingin menghapus data ini?\",\r\n        showDenyButton: true,\r\n        // showCancelButton: true,\r\n        confirmButtonText: \"Iya\",\r\n        denyButtonText: `Tidak`,\r\n      }).then((result) => {\r\n        /* Read more about isConfirmed, isDenied below */\r\n        if (result.isConfirmed) {\r\n          this.deleteProvinsi(id);\r\n          Swal.fire(\"Tersimpan!\", \"\", \"success\");\r\n        } else if (result.isDenied) {\r\n          Swal.fire(\"Tidak Tersimpan\", \"\", \"info\");\r\n        }\r\n      });\r\n    },\r\n    resetnama() {\r\n      this.name = null;\r\n    },\r\n    getProvinsis: function () {\r\n      axios.get(\"http://127.0.0.1:8000/api/provinsi\").then(\r\n        function (response) {\r\n          this.provinsis = response.data;\r\n          setTimeout(() => {\r\n            $(\"#dataProvinsi\").DataTable();\r\n          }, 3000);\r\n        }.bind(this)\r\n      );\r\n    },\r\n    getTransaksi: function () {\r\n      axios.get(\"127.0.0.1:8000/api/transaksi/index\").then(\r\n        function (response) {\r\n          this.transaksi = response.data;\r\n          setTimeout(() => {\r\n            $(\"#dataTransaksi\").DataTable();\r\n          }, 2000);\r\n        }.bind(this)\r\n      );\r\n    },\r\n    deleteProvinsi(id) {\r\n      // alert(id);\r\n\r\n      axios.delete(\"http://127.0.0.1:8000/api/provinsi/\" + id).then(\r\n        function () {\r\n          // alert(\"delete succes\");\r\n          this.getProvinsis();\r\n        }.bind(this)\r\n      );\r\n    },\r\n    showProvinsi(id) {\r\n      // alert(id);\r\n      axios.get(\"http://127.0.0.1:8000/api/provinsi/\" + id).then(\r\n        function (response) {\r\n          this.edit = response.data.data.id;\r\n          this.name = response.data.data.name;\r\n        }.bind(this)\r\n      );\r\n    },\r\n    putProvinsis(id) {\r\n      // alert(id);\r\n      axios\r\n        .post(\"http://127.0.0.1:8000/api/provinsi/\" + id, {\r\n          name: this.name,\r\n        })\r\n        .then((response) => {\r\n          this.showPost();\r\n          this.getProvinsis();\r\n          // this.$toast.show(\"berhasil update\");\r\n          console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$toast.error(\"gagal update\");\r\n          console.log(error);\r\n        });\r\n      this.name = \"\";\r\n    },\r\n    postProvinsis() {\r\n      axios\r\n        .post(\"http://127.0.0.1:8000/api/provinsi\", {\r\n          name: this.name,\r\n        })\r\n        .then((response) => {\r\n          this.showPost();\r\n          this.getProvinsis();\r\n          console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          alert(\"provinsi sudah ada\");\r\n          console.log(error);\r\n        });\r\n      this.name = \"\";\r\n    },\r\n    checkPrivilege(privilege) {\r\n      const permission = localStorage.getItem(\"permission\");\r\n      let status = false;\r\n      JSON.parse(permission).forEach((data) => {\r\n        if (data === privilege) {\r\n          status = true;\r\n        }\r\n      });\r\n      return status;\r\n    },\r\n  },\r\n  created: function () {\r\n    this.getProvinsis();\r\n    this.getTransaksi();\r\n  },\r\n  computed: {\r\n    displayedRecords() {\r\n      const startIndex = this.perPage * (this.page - 1);\r\n      const endIndex = startIndex + this.perPage;\r\n      return this.records.slice(startIndex, endIndex);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n// setup(){ // const provinsi = reactive({ // name : '', // }); // function\r\nstore() { // axios.post( // 'http://127.0.0.1:8000/api/provinsi', // provinsi //\r\n) // .then((response)=> { // console.log(response); // }).catch((err) => { //\r\nconsole.log(err); // }); // } // return { // provinsi, // store // } // },\r\n"]}]}