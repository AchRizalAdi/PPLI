wal","VueEditor","components","data","page","perPage","records","provinsis","name","wilayahs","pengumuman","wilayah","judul","keterangan","status","tanggal","methods","resetnama","showPost","fire","position","icon","title","showConfirmButton","timer","showDelete","id","showDenyButton","confirmButtonText","denyButtonText","then","result","isConfirmed","deleteProvinsi","isDenied","getWilayahs","get","response","map","value","text","bind","getProvinsis","getPengumuman","document","ready","DataTable","delete","showProvinsi","edit","showPengumuman","putProvinsis","post","console","log","catch","error","$toast","postPengumuman","alert","postProvinsis","checkPrivilege","privilege","permission","localStorage","getItem","JSON","parse","forEach","created","computed","displayedRecords","startIndex","endIndex","slice"],"sourceRoot":"","sources":["E:\\Codeee\\PPLI FE BE\\PPLI\\src\\components\\section\\PengumumanSection.vue"],"sourcesContent":["<template>\r\n  <div class=\"col-lg-9 ps-xl-5\">\r\n    <div class=\"user-panel-title-box\">\r\n      <h3>Pengumuman</h3>\r\n    </div>\r\n    <!-- end user-panel-title-box -->\r\n\r\n    <!-- {{ provinsi }} -->\r\n    <div\r\n      v-if=\"checkPrivilege('provinsi-store')\"\r\n      class=\"d-grid gap-2 d-md-block\"\r\n    >\r\n      <button\r\n        @click=\"resetnama()\"\r\n        type=\"button\"\r\n        class=\"btn btn-dark btn-sm mb-2\"\r\n        data-bs-toggle=\"modal\"\r\n        data-bs-target=\"#messageModal\"\r\n      >\r\n        Tambah Pengumuman\r\n      </button>\r\n    </div>\r\n    <!-- {{ tanggal }} -->\r\n    <!-- {{ ket }} -->\r\n    <div class=\"profile-setting-panel-wrap\">\r\n      <div class=\"table\">\r\n        <table class=\"table mb-0 table-s2\" id=\"example\">\r\n          <thead class=\"fs-14\">\r\n            <tr>\r\n              <th\r\n                scope=\"col\"\r\n                v-for=\"(list, i) in SectionData.pengumumansData\r\n                  .pengumumansTableHead\"\r\n                :key=\"i\"\r\n              >\r\n                {{ list }}\r\n              </th>\r\n            </tr>\r\n          </thead>\r\n          <tbody class=\"fs-13\">\r\n            <tr v-for=\"item in pengumuman.data\" :key=\"item.id\">\r\n              <td>{{ item.wilayah.name }}</td>\r\n              <td>{{ item.tanggal }}</td>\r\n              <td>{{ item.judul }}</td>\r\n              <td>{{ item.status }}</td>\r\n              <!-- <td>{{ item.keterangan }}</td> -->\r\n              <td class=\"row\">\r\n                <button\r\n                  v-if=\"checkPrivilege('provinsi-update')\"\r\n                  @click=\"showPengumuman(item.id)\"\r\n                  class=\"col- icon-btn p-0 m-0\"\r\n                  title=\"Edit\"\r\n                  data-bs-toggle=\"modal\"\r\n                  data-bs-target=\"#updateModal\"\r\n                >\r\n                  <em class=\"fa fa-pencil-square-o\"></em>\r\n                </button>\r\n                <button\r\n                  v-if=\"checkPrivilege('provinsi-delete')\"\r\n                  @click=\"showDelete(item.id)\"\r\n                  class=\"col- icon-btn p-0 m-0\"\r\n                  title=\"Delete\"\r\n                >\r\n                  <em class=\"ni ni-trash\"></em>\r\n                </button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <!-- end table-responsive -->\r\n      <!-- pagination -->\r\n      <div class=\"text-center mt-4 mt-md-5\">\r\n        <Pagination\r\n          :records=\"records.length\"\r\n          v-model=\"page\"\r\n          :per-page=\"perPage\"\r\n        ></Pagination>\r\n      </div>\r\n    </div>\r\n    <!-- end profile-setting-panel-wrap-->\r\n    <!-- Modal store -->\r\n    <form @submit.prevent=\"postPengumuman()\">\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"messageModal\"\r\n        tabindex=\"-1\"\r\n        aria-labelledby=\"reportModalLabel\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog modal-dialog-centered\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h4 class=\"modal-title\" id=\"reportModalLabel\">\r\n                Tambah Pengumuman\r\n              </h4>\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn-close icon-btn\"\r\n                data-bs-dismiss=\"modal\"\r\n                aria-label=\"Close\"\r\n              >\r\n                <em class=\"ni ni-cross\"></em>\r\n              </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"form-group mb-3\">\r\n                <label>Wilayah </label>\r\n                <v-select\r\n                  v-model=\"wilayah\"\r\n                  required\r\n                  :options=\"wilayahs\"\r\n                  :reduce=\"(wilayahs) => wilayahs.value\"\r\n                  label=\"text\"\r\n                >\r\n                </v-select>\r\n              </div>\r\n              <div class=\"form-floating mt-3 mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"judul\"\r\n                  placeholder=\"judul\"\r\n                  v-model=\"judul\"\r\n                  required\r\n                />\r\n                <label for=\"judul\">Judul</label>\r\n              </div>\r\n              <label>Keterangan</label>\r\n              <div class=\"form-floating mb-3\">\r\n                <vue-editor v-model=\"keterangan\" />\r\n              </div>\r\n              <!-- <textarea\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"keterangan\"\r\n                  placeholder=\"Keterangan\"\r\n                  v-model=\"keterangan\"\r\n                  required\r\n                ></textarea>\r\n                <label for=\"keterangan\">Keterangan</label> -->\r\n              <label for=\"status\">Status</label>\r\n              <select\r\n                class=\"form-select h-2 mb-3\"\r\n                v-model=\"status\"\r\n                aria-label=\"Default select example\"\r\n              >\r\n                <!-- <label>I am i</label> -->\r\n                <option value=\"Tampil\">Tampil</option>\r\n                <option value=\"Tidak Tampil\">Tidak Tampil</option>\r\n              </select>\r\n              <!-- end form-floating -->\r\n              <button class=\"btn btn-dark w-100\" type=\"submit\">Add</button>\r\n            </div>\r\n            <!-- end modal-body -->\r\n          </div>\r\n          <!-- end modal-content -->\r\n        </div>\r\n        <!-- end modal-dialog -->\r\n      </div>\r\n      <!-- end modal-->\r\n    </form>\r\n    <!-- Modal store -->\r\n    <form @submit.prevent=\"putPengumuman(edit)\">\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"updateModal\"\r\n        tabindex=\"-1\"\r\n        aria-labelledby=\"reportModalLabel\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog modal-dialog-centered\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h4 class=\"modal-title\" id=\"reportModalLabel\">\r\n                Pengumuman\r\n              </h4>\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn-close icon-btn\"\r\n                data-bs-dismiss=\"modal\"\r\n                aria-label=\"Close\"\r\n              >\r\n                <em class=\"ni ni-cross\"></em>\r\n              </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"form-group mb-3\">\r\n                <label>Wilayah </label>\r\n                <v-select\r\n                  v-model=\"wilayah\"\r\n                  required\r\n                  :options=\"wilayahs\"\r\n                  :reduce=\"(wilayahs) => wilayahs.value\"\r\n                  label=\"text\"\r\n                >\r\n                </v-select>\r\n              </div>\r\n              <div class=\"form-floating mt-3 mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"judul\"\r\n                  placeholder=\"judul\"\r\n                  v-model=\"judul\"\r\n                  required\r\n                />\r\n                <label for=\"judul\">Judul</label>\r\n              </div>\r\n              <label>Keterangan</label>\r\n              <div class=\"form-floating mb-3\">\r\n                <QuillEditor\r\n                  v-model:content=\"keterangan\"\r\n                  contentType=\"html\"\r\n                  theme=\"snow\"\r\n                />\r\n              </div>\r\n              <!-- <textarea\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"keterangan\"\r\n                  placeholder=\"Keterangan\"\r\n                  v-model=\"keterangan\"\r\n                  required\r\n                ></textarea>\r\n                <label for=\"keterangan\">Keterangan</label> -->\r\n              <label for=\"status\">Status</label>\r\n              <select\r\n                class=\"form-select h-2 mb-3\"\r\n                v-model=\"status\"\r\n                aria-label=\"Default select example\"\r\n              >\r\n                <!-- <label>I am i</label> -->\r\n                <option value=\"Tampil\">Tampil</option>\r\n                <option value=\"Tidak Tampil\">Tidak Tampil</option>\r\n              </select>\r\n              <!-- end form-floating -->\r\n              <button class=\"btn btn-dark w-100\" type=\"submit\">Add</button>\r\n            </div>\r\n            <!-- end modal-body -->\r\n          </div>\r\n          <!-- end modal-content -->\r\n        </div>\r\n        <!-- end modal-dialog -->\r\n      </div>\r\n      <!-- end modal-->\r\n    </form>\r\n    <form @submit.prevent=\"putProvinsis(edit)\">\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"updateModal\"\r\n        tabindex=\"-1\"\r\n        aria-labelledby=\"reportModalLabel\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog modal-dialog-centered\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h4 class=\"modal-title\" id=\"reportModalLabel\">update provinsi</h4>\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn-close icon-btn\"\r\n                data-bs-dismiss=\"modal\"\r\n                aria-label=\"Close\"\r\n              >\r\n                <em class=\"ni ni-cross\"></em>\r\n              </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"form-floating mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"name\"\r\n                  placeholder=\"name\"\r\n                  v-model=\"name\"\r\n                  required\r\n                />\r\n              </div>\r\n              <!-- end form-floating -->\r\n              <button\r\n                class=\"btn btn-dark w-100\"\r\n                data-bs-dismiss=\"modal\"\r\n                type=\"submit\"\r\n              >\r\n                update\r\n              </button>\r\n            </div>\r\n            <!-- end modal-body -->\r\n          </div>\r\n          <!-- end modal-content -->\r\n        </div>\r\n        <!-- end modal-dialog -->\r\n      </div>\r\n      <!-- end modal-->\r\n    </form>\r\n  </div>\r\n  <!-- end col-lg-8 -->\r\n</template>\r\n\r\n<script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\n// import RichTextEditor from \"@/components/RichTextEditor.vue\";\r\nimport SectionData from \"@/store/store.js\";\r\nimport Pagination from \"v-pagination-3\";\r\nimport axios from \"axios\";\r\nimport $ from \"jquery\";\r\nimport Swal from \"sweetalert2\";\r\n// Basic Use - Covers most scenarios\r\nimport { VueEditor } from \"vue2-editor\";\r\n\r\n// import { reactive } from 'vue';\r\n// import { onMounted, ref } from 'vue';\r\n\r\nexport default {\r\n  components: {\r\n    Pagination,\r\n    // RichTextEditor,\r\n  },\r\n  data() {\r\n    return {\r\n      SectionData,\r\n      page: 1,\r\n      perPage: 6,\r\n      records: [],\r\n      provinsis: [],\r\n      name: \"\",\r\n      wilayahs: [],\r\n      pengumuman: [],\r\n      wilayah: \"\",\r\n      judul: \"\",\r\n      keterangan: \"\",\r\n      status: \"\",\r\n      tanggal: \"\",\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    resetnama() {\r\n      this.wilayah = null;\r\n      this.judul = null;\r\n      this.keterangan = null;\r\n      this.status = null;\r\n    },\r\n    showPost() {\r\n      Swal.fire({\r\n        position: \"top-end\",\r\n        icon: \"success\",\r\n        title: \"Data telah tersimpan!\",\r\n        showConfirmButton: false,\r\n        timer: 1500,\r\n      });\r\n    },\r\n    showDelete(id) {\r\n      Swal.fire({\r\n        title: \"Apakah anda ingin menghapus data ini?\",\r\n        showDenyButton: true,\r\n        // showCancelButton: true,\r\n        confirmButtonText: \"Iya\",\r\n        denyButtonText: `Tidak`,\r\n      }).then((result) => {\r\n        /* Read more about isConfirmed, isDenied below */\r\n        if (result.isConfirmed) {\r\n          this.deleteProvinsi(id);\r\n          Swal.fire(\"Tersimpan!\", \"\", \"success\");\r\n        } else if (result.isDenied) {\r\n          Swal.fire(\"Tidak Tersimpan\", \"\", \"info\");\r\n        }\r\n      });\r\n    },\r\n    getWilayahs: function () {\r\n      axios.get(\"http://127.0.0.1:8000/api/select/wilayah\").then(\r\n        function (response) {\r\n          this.wilayahs = response.data.map((wilayahs) => ({\r\n            value: wilayahs.id,\r\n            text: wilayahs.name,\r\n          }));\r\n        }.bind(this)\r\n      );\r\n    },\r\n    getProvinsis: function () {\r\n      axios.get(\"http://127.0.0.1:8000/api/provinsi\").then(\r\n        function (response) {\r\n          this.provinsis = response.data;\r\n          // $(document).ready(function () {\r\n          //   $(\"#dataTable\").DataTable();\r\n          // });\r\n        }.bind(this)\r\n      );\r\n    },\r\n    getPengumuman: function () {\r\n      axios.get(\"http://127.0.0.1:8000/api/pengumuman\").then(\r\n        function (response) {\r\n          this.pengumuman = response.data;\r\n          $(document).ready(function () {\r\n            $(\"#dataTable\").DataTable();\r\n          });\r\n          this.tanggal = response.data.tanggal;\r\n        }.bind(this)\r\n      );\r\n    },\r\n    deleteProvinsi(id) {\r\n      // alert(id);\r\n\r\n      axios.delete(\"http://127.0.0.1:8000/api/provinsi/\" + id).then(\r\n        function () {\r\n          // alert(\"delete succes\");\r\n          this.getProvinsis();\r\n        }.bind(this)\r\n      );\r\n    },\r\n    showProvinsi(id) {\r\n      // alert(id);\r\n      axios.get(\"http://127.0.0.1:8000/api/provinsi/\" + id).then(\r\n        function (response) {\r\n          this.edit = response.data.data.id;\r\n          this.name = response.data.data.name;\r\n        }.bind(this)\r\n      );\r\n    },\r\n    showPengumuman(id) {\r\n      // alert(id);\r\n      axios.get(\"http://127.0.0.1:8000/api/pengumuman/\" + id).then(\r\n        function (response) {\r\n          this.id = response.data.data.id;\r\n          this.wilayah = response.data.data.wilayah.name;\r\n          this.judul = response.data.data.judul; \r\n          this.keterangan = response.data.data.keterangan; \r\n          this.status = response.data.data.status; \r\n        }.bind(this)\r\n      );\r\n    },\r\n    putProvinsis(id) {\r\n      // alert(id);\r\n      axios\r\n        .post(\"http://127.0.0.1:8000/api/provinsi/\" + id, {\r\n          name: this.name,\r\n        })\r\n        .then((response) => {\r\n          this.showPost();\r\n          this.getProvinsis();\r\n          // this.$toast.show(\"berhasil update\");\r\n          console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$toast.error(\"gagal update\");\r\n          console.log(error);\r\n        });\r\n      this.name = \"\";\r\n    },\r\n    postPengumuman() {\r\n      axios\r\n        .post(\"http://127.0.0.1:8000/api/pengumuman\", {\r\n          wilayah: this.wilayah,\r\n          judul: this.judul,\r\n          keterangan: this.keterangan,\r\n          status: this.status,\r\n        })\r\n        .then((response) => {\r\n          this.showPost();\r\n          this.getPengumuman();\r\n          console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          alert(\"pengumuman sudah ada\");\r\n          console.log(error);\r\n        });\r\n    },\r\n    postProvinsis() {\r\n      axios\r\n        .post(\"http://127.0.0.1:8000/api/provinsi\", {\r\n          name: this.name,\r\n        })\r\n        .then((response) => {\r\n          this.showPost();\r\n          this.getProvinsis();\r\n          console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          alert(\"pengumuman sudah ada\");\r\n          console.log(error);\r\n        });\r\n      this.name = \"\";\r\n    },\r\n    checkPrivilege(privilege) {\r\n      const permission = localStorage.getItem(\"permission\");\r\n      let status = false;\r\n      JSON.parse(permission).forEach((data) => {\r\n        if (data === privilege) {\r\n          status = true;\r\n        }\r\n      });\r\n      return status;\r\n    },\r\n  },\r\n  created: function () {\r\n    this.getProvinsis();\r\n    this.getPengumuman();\r\n    this.getWilayahs();\r\n  },\r\n  computed: {\r\n    displayedRecords() {\r\n      const startIndex = this.perPage * (this.page - 1);\r\n      const endIndex = startIndex + this.perPage;\r\n      return this.records.slice(startIndex, endIndex);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n// setup(){ // const provinsi = reactive({ // name : '', // }); // function\r\nstore() { // axios.post( // 'http://127.0.0.1:8000/api/provinsi', // provinsi //\r\n) // .then((response)=> { // console.log(response); // }).catch((err) => { //\r\nconsole.log(err); // }); // } // return { // provinsi, // store // } // },\r\n"]}]}