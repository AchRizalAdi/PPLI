tri\">\r\n          <thead class=\"fs-14\">\r\n            <tr>\r\n              <th\r\n                scope=\"col\"\r\n                v-for=\"(list, i) in SectionData.industrysData\r\n                  .industrysTableHead\"\r\n                :key=\"i\"\r\n              >\r\n                {{ list }}\r\n              </th>\r\n            </tr>\r\n          </thead>\r\n          <tbody class=\"fs-13\">\r\n            <tr v-for=\"(item, index) in industri.data\" :key=\"item.id\">\r\n              <td scope=\"row\">\r\n                <a href=\"#\">{{ index + 1 }}</a>\r\n              </td>\r\n              <td>{{ item.name }}</td>\r\n              <td class=\"row\">\r\n                <button\r\n                  v-if=\"checkPrivilege('industri-edit')\"\r\n                  @click=\"showIndustri(item.id)\"\r\n                  class=\"col- icon-btn p-0 m-0\"\r\n                  title=\"Edit\"\r\n                  data-bs-toggle=\"modal\"\r\n                  data-bs-target=\"#updateModal\"\r\n                >\r\n                  <em class=\"fa fa-pencil-square-o\"></em>\r\n                </button>\r\n                <button\r\n                  v-if=\"checkPrivilege('industri-delete')\"\r\n                  @click=\"showDelete(item.id)\"\r\n                  class=\"col- icon-btn p-0 m-0\"\r\n                  title=\"Delete\"\r\n                >\r\n                  <em class=\"ni ni-trash\"></em>\r\n                </button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <!-- end table-responsive -->\r\n      <!-- pagination -->\r\n      <div class=\"text-center mt-4 mt-md-5\">\r\n        <Pagination\r\n          :records=\"records.length\"\r\n          v-model=\"page\"\r\n          :per-page=\"perPage\"\r\n        ></Pagination>\r\n      </div>\r\n    </div>\r\n    <!-- end profile-setting-panel-wrap-->\r\n    <!-- Modal store -->\r\n    <form @submit.prevent=\"postIndustri()\">\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"messageModal\"\r\n        tabindex=\"-1\"\r\n        aria-labelledby=\"reportModalLabel\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog modal-dialog-centered\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h4 class=\"modal-title\" id=\"reportModalLabel\">Tambah Industri</h4>\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn-close icon-btn\"\r\n                data-bs-dismiss=\"modal\"\r\n                aria-label=\"Close\"\r\n              >\r\n                <em class=\"ni ni-cross\"></em>\r\n              </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"form-floating mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"nama\"\r\n                  placeholder=\"name\"\r\n                  v-model=\"name\"\r\n                  required\r\n                />\r\n                <label for=\"nama\">Nama</label>\r\n              </div>\r\n              <!-- end form-floating -->\r\n              <button\r\n                class=\"btn btn-dark w-100\"\r\n                data-bs-dismiss=\"modal\"\r\n                type=\"submit\"\r\n              >\r\n                Add\r\n              </button>\r\n            </div>\r\n            <!-- end modal-body -->\r\n          </div>\r\n          <!-- end modal-content -->\r\n        </div>\r\n        <!-- end modal-dialog -->\r\n      </div>\r\n      <!-- end modal-->\r\n    </form>\r\n    <form @submit.prevent=\"putIndustri(edit)\">\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"updateModal\"\r\n        tabindex=\"-1\"\r\n        aria-labelledby=\"reportModalLabel\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog modal-dialog-centered\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h4 class=\"modal-title\" id=\"reportModalLabel\">update provinsi</h4>\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn-close icon-btn\"\r\n                data-bs-dismiss=\"modal\"\r\n                aria-label=\"Close\"\r\n              >\r\n                <em class=\"ni ni-cross\"></em>\r\n              </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"form-floating mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"name\"\r\n                  placeholder=\"name\"\r\n                  v-model=\"name\"\r\n                  required\r\n                />\r\n                <!-- <label for=\"nama\">{{edit.data.name}}</label> -->\r\n              </div>\r\n              <!-- end form-floating -->\r\n              <button\r\n                class=\"btn btn-dark w-100\"\r\n                data-bs-dismiss=\"modal\"\r\n                type=\"submit\"\r\n              >\r\n                update\r\n              </button>\r\n            </div>\r\n            <!-- end modal-body -->\r\n          </div>\r\n          <!-- end modal-content -->\r\n        </div>\r\n        <!-- end modal-dialog -->\r\n      </div>\r\n      <!-- end modal-->\r\n    </form>\r\n  </div>\r\n  <!-- end col-lg-8 -->\r\n</template>\r\n\r\n<script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\nimport SectionData from \"@/store/store.js\";\r\nimport Pagination from \"v-pagination-3\";\r\nimport axios from \"axios\";\r\nimport $ from \"jquery\";\r\nimport Swal from \"sweetalert2\";\r\nimport mitt from \"mitt\";\r\nconst emitter = mitt();\r\n// import { reactive } from 'vue';\r\n// import { onMounted, ref } from 'vue';\r\n\r\nexport default {\r\n  components: {\r\n    Pagination,\r\n  },\r\n  data() {\r\n    return {\r\n      SectionData,\r\n      page: 1,\r\n      perPage: 6,\r\n      records: [],\r\n      // provinsis: [],\r\n      industri: [],\r\n      //   edit:'',\r\n      name: \"\",\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    showPost() {\r\n      Swal.fire({\r\n        position: \"top-end\",\r\n        icon: \"success\",\r\n        title: \"Data telah tersimpan!\",\r\n        showConfirmButton: false,\r\n        timer: 1500,\r\n      });\r\n    },\r\n    showDelete(id) {\r\n      Swal.fire({\r\n        title: \"Apakah anda ingin menghapus data ini?\",\r\n        showDenyButton: true,\r\n        // showCancelButton: true,\r\n        confirmButtonText: \"Iya\",\r\n        denyButtonText: `Tidak`,\r\n      }).then((result) => {\r\n        /* Read more about isConfirmed, isDenied below */\r\n        if (result.isConfirmed) {\r\n          this.deleteIndustri(id);\r\n          Swal.fire(\"Tersimpan!\", \"\", \"success\");\r\n        } else if (result.isDenied) {\r\n          Swal.fire(\"Tidak Tersimpan\", \"\", \"info\");\r\n        }\r\n      });\r\n    },\r\n    resetnama() {\r\n      this.name = null;\r\n    },\r\n    getIndustri: function () {\r\n      axios.get(\"http://127.0.0.1:8000/api/industry\").then(\r\n        function (response) {\r\n          this.industri = response.data;\r\n          setTimeout(() => {\r\n            $(\"#dataIndustri\").DataTable();\r\n          }, 100);\r\n        }.bind(this)\r\n      );\r\n    },\r\n    deleteIndustri(id) {\r\n      axios.delete(\"http://127.0.0.1:8000/api/industry/\" + id).then(\r\n        function () {\r\n          $(\"#dataIndustri\").DataTable().destroy();\r\n          emitter.emit(\"refreshPage\");\r\n        }.bind(this)\r\n      );\r\n    },\r\n    showIndustri(id) {\r\n      // alert(id);\r\n      axios.get(\"http://127.0.0.1:8000/api/industry/\" + id).then(\r\n        function (response) {\r\n          this.edit = response.data.data.id;\r\n          this.name = response.data.data.name;\r\n        }.bind(this)\r\n      );\r\n    },\r\n    putIndustri(id) {\r\n      // alert(id);\r\n      axios\r\n        .post(\"http://127.0.0.1:8000/api/industry/\" + id, {\r\n          name: this.name,\r\n        })\r\n        .then((response) => {\r\n          this.showPost();\r\n          this.getIndustri();\r\n          // this.$toast.show(\"berhasil update\");\r\n          console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$toast.error(\"gagal update\");\r\n          console.log(error);\r\n        });\r\n      this.name = \"\";\r\n    },\r\n    postIndustri() {\r\n      axios\r\n        .post(\"http://127.0.0.1:8000/api/industry\", {\r\n          name: this.name,\r\n        })\r\n        .then((response) => {\r\n          this.showPost();\r\n          $(\"#dataIndustri\").DataTable().destroy();\r\n          emitter.emit(\"refreshPage\");\r\n          console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          alert(\"industri sudah ada\");\r\n          console.log(error);\r\n        });\r\n    },\r\n    checkPrivilege(privilege) {\r\n      const permission = localStorage.getItem(\"permission\");\r\n      let status = false;\r\n      JSON.parse(permission).forEach((data) => {\r\n        if (data === privilege) {\r\n          status = true;\r\n        }\r\n      });\r\n      return status;\r\n    },\r\n  },\r\n  created: function () {\r\n    this.getIndustri();\r\n    emitter.on(\"refreshPage\", () => {\r\n      this.getIndustri();\r\n    });\r\n  },\r\n  computed: {\r\n    displayedRecords() {\r\n      const startIndex = this.perPage * (this.page - 1);\r\n      const endIndex = startIndex + this.perPage;\r\n      return this.records.slice(startIndex, endIndex);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n// setup(){ // const provinsi = reactive({ // name : '', // }); // function\r\nstore() { // axios.post( // 'http://127.0.0.1:8000/api/provinsi', // provinsi //\r\n) // .then((response)=> { // console.log(response); // }).catch((err) => { //\r\nconsole.log(err); // }); // } // return { // provinsi, // store // } // },\r\n"]}]}