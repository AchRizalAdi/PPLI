AAmBC,EAAG,EAF9E,EAGI;QAAEe,UAAU,EAAE,KAAKA;MAAnB,CAHJ,EAKGd,IALH,CAKSgB,QAAD,IAAc;QAClB;QACA,KAAKb,QAAL;QACA,KAAKc,MAAL,CAAYC,OAAZ,CAAoB,kBAApB;QACAC,OAAO,CAACC,GAAR,CAAYJ,QAAZ;MACD,CAVH,EAWGK,KAXH,CAWUC,KAAD,IAAW;QAChB,KAAKL,MAAL,CAAYK,KAAZ,CAAkB,kCAAlB;QACAH,OAAO,CAACC,GAAR,CAAYE,KAAZ;MACD,CAdH;IAeD;EAhCM,CAlBI;EAoDbC,QAAQ,EAAE;IACRC,gBAAgB,GAAG;MACjB,MAAMC,UAAS,GAAI,KAAKjC,OAAL,IAAgB,KAAKF,IAAL,GAAY,CAA5B,CAAnB;MACA,MAAMoC,QAAO,GAAID,UAAS,GAAI,KAAKjC,OAAnC;MACA,OAAO,KAAKC,OAAL,CAAakC,KAAb,CAAmBF,UAAnB,EAA+BC,QAA/B,CAAP;IACD;;EALO;AApDG,CAAf","names":["SectionData","axios","$","Swal","data","page","i","perPage","records","privilege","created","get","$route","params","id","then","res","methods","showPost","fire","position","icon","title","showConfirmButton","timer","postPrivilege","searchIDs","map","val","permission","post","response","$toast","success","console","log","catch","error","computed","displayedRecords","startIndex","endIndex","slice"],"sourceRoot":"","sources":["E:\\Codeee\\PPLI FE BE\\PPLI\\src\\components\\section\\PrivilageSection.vue"],"sourcesContent":["<template>\r\n  <div class=\"col-lg-9 ps-xl-5\">\r\n    <div class=\"user-panel-title-box\">\r\n      <!-- {{ privilege }} -->\r\n      <h3>Privilege</h3>\r\n    </div>\r\n    <!-- end user-panel-title-box -->\r\n    <nav aria-label=\"breadcrumb\">\r\n      <ol class=\"breadcrumb breadcrumb-s1 justify-content-left mb-3\">\r\n        <li class=\"breadcrumb-item\">\r\n          <router-link to=\"/pengaturan\">Pengaturan</router-link>\r\n        </li>\r\n        <li class=\"breadcrumb-item\">Privilege</li>\r\n      </ol>\r\n    </nav>\r\n    <!-- {{ cities }} -->\r\n    <!-- <div class=\"d-grid gap-2 d-md-block\">\r\n                            <button @click=\"resetnama()\" type=\"button\" class=\"btn btn-primary btn-sm mb-2\" data-bs-toggle=\"modal\" data-bs-target=\"#addModal\">Add Group</button>\r\n                        </div> -->\r\n    <form @submit.prevent=\"postPrivilege\">\r\n      <div class=\"profile-setting-panel-wrap\">\r\n        <div class=\"table\">\r\n          <table class=\"table mb-0 table-s2\" id=\"dataTable\">\r\n            <thead class=\"fs-14\">\r\n              <tr>\r\n                <th\r\n                  scope=\"col\"\r\n                  v-for=\"(list, i) in SectionData.privilegeData\r\n                    .privilegeTableHead\"\r\n                  :key=\"i\"\r\n                >\r\n                  {{ list }}\r\n                </th>\r\n              </tr>\r\n            </thead>\r\n            <tbody class=\"fs-13\">\r\n              <tr v-for=\"item in privilege\" :key=\"item.id\">\r\n                <td>{{ item.permission_name }}</td>\r\n                <td class=\"row\">\r\n                  <div class=\"form-check form-switch form-switch-s1 ms-4\">\r\n                    <input\r\n                      v-model=\"item.has_permission\"\r\n                      name=\"permission[]\"\r\n                      class=\"form-check-input checkbox-switcher\"\r\n                      :value=\"item.permission_id\"\r\n                      data-target=\"switch-content-unlock\"\r\n                      type=\"checkbox\"\r\n                    />\r\n                  </div>\r\n                  <!-- end form-check -->\r\n                  <!-- <button  @click=\"showCities(item.id)\" class=\"col-sm-1 icon-btn\" title=\"Show\" data-bs-toggle=\"modal\"  data-bs-target=\"#editModal\"><em class=\"fa fa-eye\" ></em></button> -->\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n          <router-link\r\n            to=\"/pengaturan\"\r\n            type=\"button\"\r\n            class=\"btn btn-sm btn-dark\"\r\n            >Kembali</router-link\r\n          >\r\n          <button type=\"submit\" class=\"btn-sm btn btn-success ms-2\">\r\n            Simpan\r\n          </button>\r\n        </div>\r\n        <!-- end table-responsive -->\r\n      </div>\r\n      <!-- end profile-setting-panel-wrap-->\r\n      <!-- BUTTON -->\r\n    </form>\r\n  </div>\r\n  <!-- end col-lg-8 -->\r\n</template>\r\n\r\n<script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\nimport SectionData from \"@/store/store.js\";\r\nimport axios from \"axios\";\r\nimport $ from \"jquery\";\r\nimport Swal from \"sweetalert2\";\r\n// import { reactive } from 'vue';\r\n// import { onMounted, ref } from 'vue';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      SectionData,\r\n      page: 1,\r\n      i: 1,\r\n      perPage: 6,\r\n      records: [],\r\n      privilege: [],\r\n    };\r\n  },\r\n  created() {\r\n    axios\r\n      .get(`http://127.0.0.1:8000/api/user_permission/${this.$route.params.id}`)\r\n      .then((res) => {\r\n        this.privilege = res.data;\r\n      });\r\n  },\r\n  methods: {\r\n    showPost() {\r\n      Swal.fire({\r\n        position: \"top-end\",\r\n        icon: \"success\",\r\n        title: \"Data telah tersimpan!\",\r\n        showConfirmButton: false,\r\n        timer: 1500,\r\n      });\r\n    },\r\n    postPrivilege: function () {\r\n      var searchIDs = $(\"input:checkbox:checked\")\r\n        .map(function () {\r\n          return $(this).val();\r\n        })\r\n        .get();\r\n      this.permission = searchIDs;\r\n      axios\r\n        .post(\r\n          `http://127.0.0.1:8000/api/update_user_permission/${this.$route.params.id}`,\r\n          { permission: this.permission }\r\n        )\r\n        .then((response) => {\r\n          // localStorage.setItem('itemKey', JSON.stringify());\r\n          this.showPost();\r\n          this.$toast.success(\"berhasil di ubah\");\r\n          console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$toast.error(\"Permission tidak berhasil diubah\");\r\n          console.log(error);\r\n        });\r\n    },\r\n  },\r\n  computed: {\r\n    displayedRecords() {\r\n      const startIndex = this.perPage * (this.page - 1);\r\n      const endIndex = startIndex + this.perPage;\r\n      return this.records.slice(startIndex, endIndex);\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}