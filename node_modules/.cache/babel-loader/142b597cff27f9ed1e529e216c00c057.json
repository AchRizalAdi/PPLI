,QAAX,CAAV,EAAuB,WAAvB;EA6DN,CA7DP,GACEP,oBA0DM,KA1DN,eA0DM,CAnDJA,oBAiDM,KAjDN,eAiDM,CAhDJA,oBA8CM,KA9CN,eA8CM,CA7CJsC,WA6CI,EAlCJtC,oBAgCM,KAhCN,eAgCM,CA/BJA,oBAUM,KAVN,eAUM,iBATJA,oBAOE,OAPF,EAOE;IANAI,IAAI,EAAC,MAML;IALAR,KAAK,EAAC,cAKN;IAJAC,EAAE,EAAC,MAIH;IAHAoC,WAAW,EAAC,MAGZ;+DAFS1B,aAAIuB,OAEb;IADAI,QAAQ,EAAR;EACA,CAPF;;EAAA,kBAKW3B,aAIP,EADJgC,WACI,CAVN,CA+BI,EApBJvC,oBAUM,KAVN,eAUM,iBATJA,oBAOE,OAPF,EAOE;IANAI,IAAI,EAAC,MAML;IALAR,KAAK,EAAC,cAKN;IAJAC,EAAE,EAAC,OAIH;IAHAoC,WAAW,EAAC,OAGZ;+DAFS1B,cAAKuB,OAEd;IADAI,QAAQ,EAAR;EACA,CAPF;;EAAA,kBAKW3B,cAIP,EADJiC,WACI,CAVN,CAoBI,EARJ9B,0CAQI,EAPJ+B,WAOI,CAhCN,CAkCI,EADJ/B,uCACI,CA9CN,CAgDI,EADJA,0CACI,CAjDN,CAmDI,EADJA,yCACI,CA1DN,GA2DAA,kCA5DF;;EAAA,CA8DI,CA1MN,GA2MAA","names":["class","id","scope","href","_createElementVNode","_hoisted_13","_hoisted_16","tabindex","type","for","_toDisplayString","$data","jabatanData","mainTitle","_createCommentVNode","$options","_createElementBlock","onClick","_Fragment","_renderList","jabatanTableHead","list","i","key","data","item","index","name","level","title","_createVNode","_component_Pagination","records","length","$event","onSubmit","_hoisted_22","placeholder","required","_hoisted_25","_hoisted_27","_hoisted_28","_hoisted_32","_hoisted_35","_hoisted_37","_hoisted_38"],"sourceRoot":"","sources":["E:\\Codeee\\PPLI FE BE\\PPLI\\src\\components\\section\\JabatanSection.vue"],"sourcesContent":["<template>\r\n  <div class=\"col-lg-9 ps-xl-5\">\r\n    <div class=\"user-panel-title-box\">\r\n      <h3>{{ SectionData.jabatanData.mainTitle }}</h3>\r\n    </div>\r\n    <!-- end user-panel-title-box -->\r\n\r\n    <!-- {{ jabatan}} -->\r\n    <div\r\n      v-if=\"checkPrivilege('provinsi-store')\"\r\n      class=\"d-grid gap-2 d-md-block\"\r\n    >\r\n      <button\r\n        @click=\"resetnama()\"\r\n        type=\"button\"\r\n        class=\"btn btn-dark btn-sm mb-2\"\r\n        data-bs-toggle=\"modal\"\r\n        data-bs-target=\"#messageModal\"\r\n      >\r\n        Add jabatan\r\n      </button>\r\n    </div>\r\n    <div class=\"profile-setting-panel-wrap\">\r\n      <div class=\"table\">\r\n        <table class=\"table mb-0 table-s2\" id=\"dataJabatan\">\r\n          <thead class=\"fs-14\">\r\n            <tr>\r\n              <th\r\n                scope=\"col\"\r\n                v-for=\"(list, i) in SectionData.jabatanData.jabatanTableHead\"\r\n                :key=\"i\"\r\n              >\r\n                {{ list }}\r\n              </th>\r\n            </tr>\r\n          </thead>\r\n          <tbody class=\"fs-13\">\r\n            <tr v-for=\"(item, index) in jabatan.data\" :key=\"item.id\">\r\n              <td scope=\"row\">\r\n                <a href=\"#\">{{ index + 1 }}</a>\r\n              </td>\r\n              <td>{{ item.name }}</td>\r\n              <td>{{ item.level }}</td>\r\n              <td class=\"row\">\r\n                <button\r\n                  v-if=\"checkPrivilege('provinsi-update')\"\r\n                  @click=\"showJabatan(item.id)\"\r\n                  class=\"col- icon-btn p-0 m-0\"\r\n                  title=\"Edit\"\r\n                  data-bs-toggle=\"modal\"\r\n                  data-bs-target=\"#updateModal\"\r\n                >\r\n                  <em class=\"fa fa-pencil-square-o\"></em>\r\n                </button>\r\n                <button\r\n                  v-if=\"checkPrivilege('provinsi-delete')\"\r\n                  @click=\"showDelete(item.id)\"\r\n                  class=\"col- icon-btn p-0 m-0\"\r\n                  title=\"Delete\"\r\n                >\r\n                  <em class=\"ni ni-trash\"></em>\r\n                </button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <!-- end table-responsive -->\r\n      <!-- pagination -->\r\n      <div class=\"text-center mt-4 mt-md-5\">\r\n        <Pagination\r\n          :records=\"records.length\"\r\n          v-model=\"page\"\r\n          :per-page=\"perPage\"\r\n        ></Pagination>\r\n      </div>\r\n    </div>\r\n    <!-- end profile-setting-panel-wrap-->\r\n    <!-- Modal store -->\r\n    <form @submit.prevent=\"postJabatan()\">\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"messageModal\"\r\n        tabindex=\"-1\"\r\n        aria-labelledby=\"reportModalLabel\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog modal-dialog-centered\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h4 class=\"modal-title\" id=\"reportModalLabel\">Add Jabatan</h4>\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn-close icon-btn\"\r\n                data-bs-dismiss=\"modal\"\r\n                aria-label=\"Close\"\r\n              >\r\n                <em class=\"ni ni-cross\"></em>\r\n              </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"form-floating mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"nama\"\r\n                  placeholder=\"name\"\r\n                  v-model=\"name\"\r\n                  required\r\n                />\r\n                <label for=\"nama\">name</label>\r\n              </div>\r\n              <div class=\"form-floating mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"level\"\r\n                  placeholder=\"level\"\r\n                  v-model=\"level\"\r\n                  required\r\n                />\r\n                <label for=\"level\">level</label>\r\n              </div>\r\n\r\n              <!-- end form-floating -->\r\n              <button\r\n                class=\"btn btn-dark w-100\"\r\n                data-bs-dismiss=\"modal\"\r\n                type=\"submit\"\r\n              >\r\n                Add\r\n              </button>\r\n            </div>\r\n            <!-- end modal-body -->\r\n          </div>\r\n          <!-- end modal-content -->\r\n        </div>\r\n        <!-- end modal-dialog -->\r\n      </div>\r\n      <!-- end modal-->\r\n    </form>\r\n    <form @submit.prevent=\"putJabatan(id)\">\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"updateModal\"\r\n        tabindex=\"-1\"\r\n        aria-labelledby=\"reportModalLabel\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog modal-dialog-centered\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h4 class=\"modal-title\" id=\"reportModalLabel\">Edit Jabatan</h4>\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn-close icon-btn\"\r\n                data-bs-dismiss=\"modal\"\r\n                aria-label=\"Close\"\r\n              >\r\n                <em class=\"ni ni-cross\"></em>\r\n              </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"form-floating mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"nama\"\r\n                  placeholder=\"name\"\r\n                  v-model=\"name\"\r\n                  required\r\n                />\r\n                <label for=\"nama\">name</label>\r\n              </div>\r\n              <div class=\"form-floating mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"level\"\r\n                  placeholder=\"level\"\r\n                  v-model=\"level\"\r\n                  required\r\n                />\r\n                <label for=\"level\">level</label>\r\n              </div>\r\n\r\n              <!-- end form-floating -->\r\n              <button\r\n                class=\"btn btn-dark w-100\"\r\n                data-bs-dismiss=\"modal\"\r\n                type=\"submit\"\r\n              >\r\n                Update\r\n              </button>\r\n            </div>\r\n            <!-- end modal-body -->\r\n          </div>\r\n          <!-- end modal-content -->\r\n        </div>\r\n        <!-- end modal-dialog -->\r\n      </div>\r\n      <!-- end modal-->\r\n    </form>\r\n  </div>\r\n  <!-- end col-lg-8 -->\r\n</template>\r\n\r\n<script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\nimport SectionData from \"@/store/store.js\";\r\nimport Pagination from \"v-pagination-3\";\r\nimport axios from \"axios\";\r\nimport $ from \"jquery\";\r\nimport Swal from \"sweetalert2\";\r\nimport mitt from \"mitt\";\r\nconst emitter = mitt();\r\n// import { reactive } from 'vue';\r\n// import { onMounted, ref } from 'vue';\r\n\r\nexport default {\r\n  components: {\r\n    Pagination,\r\n  },\r\n  data() {\r\n    return {\r\n      SectionData,\r\n      page: 1,\r\n      perPage: 6,\r\n      records: [],\r\n      id: [],\r\n      jabatan: [],\r\n      name: [],\r\n      level: [],\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    checkPrivilege(privilege) {\r\n      const permission = localStorage.getItem(\"permission\");\r\n      let status = false;\r\n      JSON.parse(permission).forEach((data) => {\r\n        if (data === privilege) {\r\n          status = true;\r\n        }\r\n      });\r\n      return status;\r\n    },\r\n    showPost() {\r\n      Swal.fire({\r\n        position: \"center\",\r\n        icon: \"success\",\r\n        title: \"Data telah tersimpan!\",\r\n        showConfirmButton: false,\r\n        timer: 1500,\r\n      });\r\n    },\r\n    showDelete(id) {\r\n      Swal.fire({\r\n        title: \"Apakah anda ingin menghapus data ini?\",\r\n        showDenyButton: true,\r\n        // showCancelButton: true,\r\n        confirmButtonText: \"Iya\",\r\n        denyButtonText: `Tidak`,\r\n      }).then((result) => {\r\n        /* Read more about isConfirmed, isDenied below */\r\n        if (result.isConfirmed) {\r\n          this.deleteJabatan(id);\r\n          Swal.fire(\"Berhasil Terhapus!\", \"\", \"success\");\r\n        } else if (result.isDenied) {\r\n          Swal.fire(\"Tidak Tersimpan\", \"\", \"info\");\r\n        }\r\n      });\r\n    },\r\n    resetnama() {\r\n      this.name = null;\r\n      this.level = null;\r\n    },\r\n    getJabatan: function () {\r\n      axios.get(\"http://127.0.0.1:8000/api/jabatan\").then(\r\n        function (response) {\r\n          this.jabatan = response.data;\r\n          setTimeout(() => {\r\n            $(\"#dataJabatan\").DataTable();\r\n          }, 100);\r\n        }.bind(this)\r\n      );\r\n    },\r\n    postJabatan() {\r\n      axios\r\n        .post(\"http://127.0.0.1:8000/api/jabatan\", {\r\n          name: this.name,\r\n          level: this.level,\r\n        })\r\n        .then((response) => {\r\n          this.showPost();\r\n          $(\"#dataJabatan\").DataTable().destroy();\r\n          emitter.emit(\"refreshPage\");\r\n          console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$toast.show(\"gagal menambahkan\");\r\n          console.log(error);\r\n        });\r\n    },\r\n    putJabatan(id) {\r\n      axios\r\n        .post(\"http://127.0.0.1:8000/api/jabatan/\" + id, {\r\n          name: this.name,\r\n          level: this.level,\r\n        })\r\n        .then((response) => {\r\n          this.showPost();\r\n          this.getJabatan();\r\n          this.$toast.show(\"berhasil update\");\r\n          console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$toast.show(\"gagal update\");\r\n          console.log(error);\r\n        });\r\n      this.name = \"\";\r\n      this.level = \"\";\r\n    },\r\n    showJabatan(id) {\r\n      axios.get(\"http://127.0.0.1:8000/api/jabatan/\" + id).then(\r\n        function (response) {\r\n          this.id = response.data.data.id;\r\n          this.name = response.data.data.name;\r\n          this.level = response.data.data.level;\r\n        }.bind(this)\r\n      );\r\n    },\r\n    deleteJabatan(id) {\r\n      axios.delete(\"http://127.0.0.1:8000/api/jabatan/\" + id).then(\r\n        function () {\r\n          $(\"#dataJabatan\").DataTable().destroy();\r\n          emitter.emit(\"refreshPage\");\r\n        }.bind(this)\r\n      );\r\n    },\r\n  },\r\n  created: function () {\r\n    this.getJabatan();\r\n    emitter.on(\"refreshPage\", () => {\r\n      this.getJabatan();\r\n    });\r\n  },\r\n  computed: {\r\n    displayedRecords() {\r\n      const startIndex = this.perPage * (this.page - 1);\r\n      const endIndex = startIndex + this.perPage;\r\n      return this.records.slice(startIndex, endIndex);\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}