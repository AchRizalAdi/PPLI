;MACAR,KAAK,CAACuB,IAAN,CAAW,6CAA2Cf,EAAtD,EAA0D;QACtDgB,MAAM,EAAE,KAAKA;MADyC,CAA1D,EAEGT,IAFH,CAEQC,QAAO,IAAK;QACpB,KAAKE,YAAL;QACAO,OAAO,CAACC,GAAR,CAAYV,QAAZ;QACA,KAAKW,MAAL,CAAYC,IAAZ,CAAiBZ,QAAQ,CAACb,IAAT,CAAcqB,MAA/B;MACC,CAND,EAMGK,KANH,CAMSC,KAAI,IAAK;QAClB,KAAKH,MAAL,CAAYC,IAAZ,CAAiB,cAAjB;QACAH,OAAO,CAACC,GAAR,CAAYI,KAAZ;MACC,CATD;MAUA,KAAKN,MAAL,GAAc,EAAd;IACP,CAhCM;;IAiCPO,eAAc,CAAGvB,EAAH,EAAM;MACpB;MACA,IAAGwB,OAAO,CAAC,oBAAD,CAAV,EAAiC;QACzBhC,KAAK,CAACiC,MAAN,CAAa,+CAA6CzB,EAA1D,EACCO,IADD,CACM,YAAY;UAEL,KAAKG,YAAL;UACC,KAAKS,MAAL,CAAYC,IAAZ,CAAiB,iBAAjB;QACL,CAJH,CAIIX,IAJJ,CAIS,IAJT,CADN;MAMC;IACJ,CA3CE;;IA4CPiB,SAAS,CAAC1B,EAAD,EAAI;MACT,KAAKA,EAAL,GAAQA,EAAR;IACH,CA9CM;;IA+CP2B,KAAK,CAAC3B,EAAD,EAAI;MACLR,KAAK,CAACuB,IAAN,CAAW,8CAA4Cf,EAAvD,EAA0D;QACtD2B,KAAK,EAAC,KAAKxB;MAD2C,CAA1D,EAEGI,IAFH,CAEQC,QAAO,IAAK;QAChB,KAAKE,YAAL;QACA,KAAKS,MAAL,CAAYC,IAAZ,CAAiB,gBAAjB;QACAH,OAAO,CAACC,GAAR,CAAYV,QAAZ;MACC,CANL,EAMOa,KANP,CAMaC,KAAI,IAAK;QAClB,KAAKH,MAAL,CAAYC,IAAZ,CAAiB,sBAAjB;QACAH,OAAO,CAACC,GAAR,CAAYI,KAAZ;MACC,CATL;MAUI,KAAKnB,SAAL,GAAiB,EAAjB;IAER;;EA5DO,CAlBI;EAkFPyB,OAAO,EAAE,YAAU;IACf,KAAKlB,YAAL;EAGT,CAtFY;EAuFbmB,QAAQ,EAAE;IACRC,gBAAgB,GAAG;MACjB,MAAMC,UAAS,GAAI,KAAKjC,OAAL,IAAgB,KAAKD,IAAL,GAAY,CAA5B,CAAnB;MACA,MAAMmC,QAAO,GAAID,UAAS,GAAI,KAAKjC,OAAnC;MACA,OAAO,KAAKC,OAAL,CAAakC,KAAb,CAAmBF,UAAnB,EAA+BC,QAA/B,CAAP;IACF;;EALQ;AAvFG,CAAf","names":["SectionData","Pagination","axios","$","components","data","name","page","perPage","records","id","status_register","registers","isi_pesan","methods","update","get","then","response","bind","getRegisters","document","ready","DataTable","updateStatus","post","status","console","log","$toast","show","catch","error","deleteRegisters","confirm","delete","sendEmail","pesan","created","computed","displayedRecords","startIndex","endIndex","slice"],"sourceRoot":"","sources":["E:\\Codeee\\PPLI FE BE\\PPLI\\src\\components\\section\\OfferSection.vue"],"sourcesContent":["<template>\r\n   <div class=\"col-lg-9 ps-xl-5\">\r\n                        <div class=\"user-panel-title-box\">\r\n                            <h3>{{SectionData.registersData.mainTitle }}</h3>\r\n                        </div><!-- end user-panel-title-box -->\r\n                        <div class=\"profile-setting-panel-wrap\">\r\n                            <div class=\"table-responsive\">\r\n                                <table class=\"table mb-0 table-s2\" id=\"dataTable\">\r\n                                    <thead class=\"fs-14\">\r\n                                        <tr>\r\n                                            <th scope=\"col\" v-for=\"(list, i) in SectionData.registersData.registersTableHead\" :key=\"i\">{{ list }}</th>\r\n                                        </tr>\r\n                                    </thead>\r\n                                    <tbody class=\"fs-13\">\r\n                                        <tr v-for=\"(item,index) in registers.data\" :value='item.id' :key=\"item.id\">\r\n                                            <th scope=\"row\" id=\"id\"><a href=\"#\">{{ index+1 }}</a></th>\r\n                                            <td>{{ item.name }}</td>\r\n                                            <td>{{ item.email }}</td>\r\n                                            <td>{{ item.username }}</td>\r\n                                            <td>{{ item.NamaPerushaan }}</td>\r\n                                            <td>{{ item.PhoneNumber }}</td>\r\n                                            <td>{{ item.company_industry.name }}</td>\r\n                                            <td>{{ item.wilayah.name }}</td>\r\n                                            <td>{{ item.cities.name }}</td>\r\n                                            <td>{{ item.BentukBadanUsaha }}</td>\r\n                                            <td>{{ item.AlasanBergabung }}</td>\r\n                                             <td><a class=\"btn btn-warning btn-sm\" @click=\"update(item.id)\" id=\"statusss\" data-bs-toggle=\"modal\"  data-bs-target=\"#statussModal\" >{{item.status}}</a></td>\r\n                                            <td><a class=\"btn btn-primary btn-sm\" @click=\"sendEmail(item.id)\" id=\"pesan\" data-bs-toggle=\"modal\"  data-bs-target=\"#pesanModal\" >Kirim Pesan</a></td>\r\n                                            <td><a type=\"button\" class=\"btn icon-btn ms-auto\" title=\"Delete\" @click=\"deleteRegisters(item.id)\"><em class=\"ni ni-trash\"></em></a></td>\r\n                                        </tr>\r\n                                    </tbody>\r\n                                </table>\r\n                            </div><!-- end table-responsive -->\r\n                            <!-- pagination -->\r\n                            <div class=\"text-center mt-4 mt-md-2\">\r\n                                <Pagination :records=\"records.length\" v-model=\"page\" :per-page=\"perPage\"></Pagination>\r\n                            </div>\r\n                             <!-- Modal store -->\r\n                            <form @submit.prevent=\"updateStatus(id)\">\r\n                                <div class=\"modal fade\" id=\"statussModal\" tabindex=\"-1\" aria-labelledby=\"reportModalLabel\" aria-hidden=\"true\" >\r\n                                    <div class=\"modal-dialog modal-dialog-centered\">\r\n                                        <div class=\"modal-content\">\r\n                                            <div class=\"modal-header\">\r\n                                                <h4 class=\"modal-title\" id=\"reportModalLabel\">Permission</h4>\r\n                                                <button type=\"button\" class=\"btn-close icon-btn\" data-bs-dismiss=\"modal\" aria-label=\"Close\">\r\n                                                    <em class=\"ni ni-cross\"></em>\r\n                                                </button>\r\n                                            </div>\r\n                                            <div class=\"modal-body\">\r\n                                                 <div class=\"form-group mb-2\">\r\n                                                    <label>status </label>\r\n                                                    <select v-model=\"status\" class='form-control' required>\r\n                                                        <option  v-for='data in status_register.data' :value='data.name' :key='data.id' >{{ data.name }}</option>\r\n                                                    </select>\r\n                                                </div>\r\n                                                <button class=\"btn btn-dark w-100\" type=\"submit\">Select</button>\r\n                                            </div><!-- end modal-body -->\r\n                                        </div><!-- end modal-content -->\r\n                                    </div><!-- end modal-dialog -->\r\n                                </div><!-- end modal-->\r\n                            </form>\r\n                            <!-- Modal Pesan -->\r\n                            <form @submit.prevent=\"pesan(id)\">\r\n                                <div class=\"modal fade\" id=\"pesanModal\" tabindex=\"-1\" aria-labelledby=\"reportModalLabel\" aria-hidden=\"true\" >\r\n                                    <div class=\"modal-dialog modal-dialog-centered\">\r\n                                        <div class=\"modal-content\">\r\n                                            <div class=\"modal-header\">\r\n                                                <h4 class=\"modal-title\" id=\"reportModalLabel\">Mail</h4>\r\n                                                <button type=\"button\" class=\"btn-close icon-btn\" data-bs-dismiss=\"modal\" aria-label=\"Close\">\r\n                                                    <em class=\"ni ni-cross\"></em>\r\n                                                </button>\r\n                                            </div>\r\n                                            <div class=\"modal-body\">\r\n                                                <div class=\"form-group mb-4\">\r\n                                                    <label>Isi Pesan DIbawah</label>\r\n                                                    <div class=\"form-floating mb-3\">\r\n                                                        <textarea type=\"input\" class=\"form-control\" id=\"nama\" v-model=\"isi_pesan\"  required></textarea>\r\n                                                    </div><!-- end form-floating -->                       \r\n                                                </div>\r\n                                                <button class=\"btn btn-dark w-100\" type=\"submit\">Send</button>\r\n                                            </div><!-- end modal-body -->\r\n                                        </div><!-- end modal-content -->\r\n                                    </div><!-- end modal-dialog -->\r\n                                </div><!-- end modal-->\r\n                            </form>\r\n                        </div><!-- end profile-setting-panel-wrap-->\r\n                    </div><!-- end col-lg-8 --> \r\n</template>\r\n\r\n<script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\nimport SectionData from '@/store/store.js'\r\nimport Pagination from 'v-pagination-3';\r\nimport axios from 'axios';\r\nimport $ from \"jquery\";\r\n\r\nexport default {\r\n    components: {\r\n    Pagination\r\n  },\r\n  data () {\r\n    return {\r\n    name: 'OfferSection',\r\n      SectionData,\r\n      page: 1,\r\n      perPage: 6,\r\n      records: [],\r\n      id:[],\r\n      status_register: [],\r\n      registers: [],\r\n      isi_pesan:[],\r\n    }\r\n  },\r\n  \r\n  methods: {    \r\n    update (id) { \r\n            // alert(document.getElementById('registers').value)\r\n                axios.get('http://127.0.0.1:8000/api/statusRegister')\r\n                .then(function(response){\r\n                    this.id=id;\r\n                    this.status_register = response.data;\r\n                }.bind(this));\r\n        },\r\n        getRegisters : function() { \r\n           \r\n                axios.get('http://127.0.0.1:8000/api/userRegister')\r\n                .then(function(response){\r\n                    this.registers = response.data;\r\n                    $(document).ready(function () {\r\n                                        $('#dataTable').DataTable();\r\n                                    });\r\n                }.bind(this));\r\n        },\r\n    updateStatus(id){\r\n            // alert(id)\r\n            axios.post('http://127.0.0.1:8000/api/update/member/'+id, { \r\n                status: this.status,\r\n            }).then(response => {\r\n            this.getRegisters()\r\n            console.log(response);\r\n            this.$toast.show(response.data.status);\r\n            }).catch(error => {\r\n            this.$toast.show(\"gagal update\");\r\n            console.log(error);\r\n            })\r\n            this.status = '';  \r\n    },\r\n    deleteRegisters (id){\r\n    // alert(id);\r\n    if(confirm(\"Apakah Kamu Yakin?\")){\r\n            axios.delete('http://127.0.0.1:8000/api/register/delete/'+id)\r\n            .then(function () {\r\n                     \r\n                         this.getRegisters()\r\n                          this.$toast.show(\"berhasil delete\");\r\n                    }.bind(this));\r\n             }\r\n        },\r\n    sendEmail(id){\r\n        this.id=id;\r\n    },\r\n    pesan(id){\r\n        axios.post('http://127.0.0.1:8000/api/register/email/'+id,{\r\n            pesan:this.isi_pesan,\r\n        }).then(response => {     \r\n            this.getRegisters()\r\n            this.$toast.show(\"pesan terkirim\");\r\n            console.log(response);\r\n            }).catch(error => {\r\n            this.$toast.show(\"gagal mengirim pesan\");\r\n            console.log(error);\r\n            })\r\n            this.isi_pesan = ''; \r\n\r\n    }\r\n\r\n\r\n  },\r\n        created: function(){\r\n            this.getRegisters()\r\n            \r\n\r\n  },\r\n  computed: {\r\n    displayedRecords() {\r\n      const startIndex = this.perPage * (this.page - 1);\r\n      const endIndex = startIndex + this.perPage;\r\n      return this.records.slice(startIndex, endIndex);\r\n    }\r\n  },\r\n}\r\n</script>\r\n// <!-- <select v-model=\"status\" id=\"statusss\" class='form-control w-100' @change=\"postStatus(item.id, $event)\" >\r\n//                                                 <option :value=\"null\" disabled>Mail Verified</option>\r\n//                                                 <option value=\"Approved by DPP\">Approved by DPP</option>\r\n//                                                 <option value=\"Approved by DPW\">Approved by DPW</option>\r\n//                                                 <option value=\"Rejected by DPP\">Rejected by DPP</option>\r\n//                                                 <option value=\"Rejected by DPW\">Rejected by DPW</option>\r\n//                                             </select> -->\r\n                                           \r\n//                                             <!-- <td><button class=\"icon-btn ms-auto\" title=\"Accept\" for=\"status\" ><em class=\"ni ni-check\"></em></button></td> -->"]}]}