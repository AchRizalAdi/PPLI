,UAAU,EAAE,EARW;MASvBC,MAAM,EAAE,EATe;MAUvBC,SAAS,EAAC,EAVa;MAWvBC,WAAW,EAAE,EAXU;MAYvBC,MAAM,EAAE;IAZe,CAAD,CAA1B;IAeA,MAAMC,UAAS,GAAI/B,GAAE,CAAG,EAAH,CAArB;;IAEA,SAASgC,KAAT,GAAiB;MACb/B,KAAK,CAACgC,IAAN,CACI,oCADJ,EAEId,SAFJ,EAICT,IAJD,CAIM,MAAK;QACPwB,OAAO,CAACC,GAAR,CAAY,aAAZ;MACH,CAND,EAMGC,KANH,CAMUC,GAAD,IAAS;QACdN,UAAU,CAACO,KAAX,GAAmBD,GAAG,CAAC1B,QAAJ,CAAaR,IAAhC;MACP,CARG;IAUJ;;IACA,OAAO;MACHoC,SAAS,EAAE,KADR;MAEHpB,SAFG;MAGHY,UAHG;MAIHC;IAJG,CAAP;EAMH,CArEU;;EAsEbQ,OAAM,GAAK;IACT;IACA,SAASC,gBAAT,CAA0BC,QAA1B,EAAmC;MAC/B,IAAIC,IAAG,GAAIC,QAAQ,CAACC,gBAAT,CAA0BH,QAA1B,CAAX;;MACA,IAAGC,IAAI,CAACG,MAAL,GAAc,CAAjB,EAAmB;QACfH,IAAI,CAACI,OAAL,CAAaC,IAAG,IAAK;UACrBA,IAAI,CAACC,gBAAL,CAAsB,OAAtB,EAA+B,UAASC,CAAT,EAAW;YACtCA,CAAC,CAACC,cAAF;YACA,IAAIC,MAAK,GAAIR,QAAQ,CAACS,cAAT,CAAwBL,IAAI,CAACM,YAAL,CAAkB,MAAlB,CAAxB,CAAb;;YACA,IAAGF,MAAM,CAACG,IAAP,IAAe,UAAlB,EAA8B;cAC9BH,MAAM,CAACG,IAAP,GAAc,MAAd;cACAP,IAAI,CAACQ,SAAL,CAAeC,GAAf,CAAmB,UAAnB;YACC,CAHD,MAGK;cACLL,MAAM,CAACG,IAAP,GAAc,UAAd;cACAP,IAAI,CAACQ,SAAL,CAAeE,MAAf,CAAsB,UAAtB;YACA;UACH,CAVD;QAYC,CAbD;MAcJ;IACJ;;IAEAjB,gBAAgB,CAAC,kBAAD,CAAhB;EAEF;;AA9Fa,CAAf","names":["SectionData","reactive","ref","axios","name","data","checked","provinsis","cities","methods","getProvinsis","get","then","response","bind","getCities","params","provinsiId_id","country","created","setup","registers","email","password","username","namaPerushaan","nomor","companyindustry","provinsiId","KotaId","WilayahId","bentukusaha","alasan","validation","store","post","console","log","catch","err","value","isSuccess","mounted","showHidePassword","selector","elem","document","querySelectorAll","length","forEach","item","addEventListener","e","preventDefault","target","getElementById","getAttribute","type","classList","add","remove"],"sourceRoot":"","sources":["C:\\Coding\\jumat\\PPLI\\src\\components\\section\\RegisterSection.vue"],"sourcesContent":["<template>\r\n    <section class=\"register-section section-space-b pt-4 pt-md-5 mt-md-3\">\r\n            <div class=\"container\">\r\n                <div class=\"row align-items-center justify-content-center\">\r\n                    <div class=\"col-lg-6 mb-lg-0 d-none d-lg-block\">\r\n                        <img :src=\"SectionData.registerData.img\" alt=\"\" class=\"img-fluid\">\r\n                    </div><!-- end col-lg-6 -->\r\n                    <div class=\"col-lg-6 col-md-9\">\r\n                        <div class=\"section-head-sm\">\r\n                            <h2 class=\"mb-2\" v-html=\" SectionData.registerData.title\"></h2>\r\n                            <p>{{ SectionData.registerData.subTitle }}</p>\r\n                        </div>\r\n                        <form @submit.prevent=\"store()\">\r\n                            {{ registers }}\r\n                            <br>\r\n                            {{ provinsis }}\r\n                             <br>\r\n                            {{ cities }}\r\n                             <div class=\"form-floating mb-3\">\r\n                                <input type=\"text\" class=\"form-control\" id=\"name\" placeholder=\"Nama\" v-model=\"registers.name\">\r\n                                <label for=\"name\">Nama</label>\r\n                                <div v-if=\"validation.name\" class=\"text-danger\">\r\n                                   {{validation.name}}\r\n                                </div>\r\n                            </div><!-- end form-floating -->  \r\n                            <div class=\"form-floating mb-3\">\r\n                                <input type=\"email\" class=\"form-control\" id=\"email\" placeholder=\"Email\" v-model=\"registers.email\">\r\n                                <label for=\"email\">Email</label>\r\n                                 <div v-if=\"validation.email\" class=\"text-danger\">\r\n                                   {{validation.email}}\r\n                                </div>\r\n                            </div><!-- end form-floating -->\r\n                            <div class=\"form-floating mb-3\">\r\n                                <input type=\"password\" class=\"form-control password\" id=\"password\" placeholder=\"Password\" v-model=\"registers.password\">\r\n                                <label for=\"password\">Password</label>\r\n                                <a href=\"password\" class=\"password-toggle\" title=\"Toggle show/hide pasword\">\r\n                                    <em class=\"password-shown ni ni-eye-off\"></em>\r\n                                    <em class=\"password-hidden ni ni-eye\"></em>\r\n                                </a>\r\n                                 <div v-if=\"validation.password\" class=\"text-danger\">\r\n                                   {{validation.password}}\r\n                                </div>\r\n                            </div><!-- end form-floating -->\r\n                            <div class=\"form-floating mb-3\">\r\n                                <input type=\"text\" class=\"form-control\" id=\"username\" placeholder=\"Username\" v-model=\"registers.username\">\r\n                                <label for=\"username\">Username</label>\r\n                                 <div v-if=\"validation.username\" class=\"text-danger\">\r\n                                   {{validation.username}}\r\n                                </div>\r\n                            </div><!-- end form-floating -->\r\n                            <div class=\"form-floating mb-3\">\r\n                                <input type=\"text\" class=\"form-control\" id=\"NamaPerusahaan\" placeholder=\"Nama Perusahaan\" v-model=\"registers.namaPerushaan\">\r\n                                <label for=\"NamaPerusahaan\">Nama Perusahaan</label>\r\n                                 <div v-if=\"validation.namaPerushaan\" class=\"text-danger\">\r\n                                   {{validation.namaPerushaan}}\r\n                                </div>\r\n                            </div><!-- end form-floating -->\r\n                            <!-- <select class=\"form-select mb-3\" aria-label=\"Default select example\">\r\n                                <label>I am i</label>\r\n                                <option value=\"shipper\">Shipper</option>\r\n                                <option value=\"provider\">Logistic Provider</option>\r\n                                </select> -->\r\n                             <div class=\"form-floating mb-3\">\r\n                                <input type=\"text\" class=\"form-control\" id=\"PhoneNumber\" placeholder=\"Phone\" v-model=\"registers.nomor\">\r\n                                <label for=\"PhoneNumber\">Phone</label>\r\n                                <div v-if=\"validation.nomor\" class=\"text-danger\">\r\n                                   {{validation.nomor}}\r\n                                </div>\r\n                            </div><!-- end form-floating -->\r\n                            <div class=\"mb-3\">\r\n                                <input type=\"number\" class=\"form-control\" id=\"CompanyIndustryId\" placeholder=\"Company Industri\" v-model=\"registers.companyindustry\"/>\r\n                                <div v-if=\"validation.companyindustry\" class=\"text-danger\">\r\n                                   {{validation.companyindustry}}\r\n                                </div>\r\n                            </div><!-- end form-floating -->\r\n                           <div class=\"form-group mb-2\">\r\n                            <label>Provinsi :</label>\r\n                            <select class='form-control' v-model=\"registers.provinsiId\" @change='getProvinsis()' >\r\n                                <option v-for='data in provinsis.data' :value='data.id' :key='data.id' >{{ data.name }}</option>\r\n                            </select>\r\n                            </div>\r\n                            <div class=\"form-group mb-3\">\r\n                            <label>Kota :</label>\r\n                            <select class='form-control' v-model=\"registers.KotaId\">\r\n                                <option v-for='item in cities.data' :value='item.id' :key='item.id'>{{ item.name }}</option>\r\n                            </select>\r\n                             </div>\r\n                            <!-- <div class=\"form-group mb-3\" >\r\n                                <label>Select City :</label>\r\n                                <select id=\"cities\" v-model=\"registers.KotaId\" class=\"block w-full \">\r\n                                    <option  v-for=\"item in cities.data\" :value='item.id' :key=\"item.message\">{{ item.name }}</option>\r\n                                </select>\r\n                            </div> -->\r\n                            <!-- <div class=\"form mb-3\">\r\n                                <input type=\"number\" class=\"form-control\" id=\"KotaId\" placeholder=\"Kota\"  v-model=\"registers.KotaId\" />\r\n                                <div v-if=\"validation.KotaId\" class=\"text-danger\">\r\n                                   {{validation.KotaId}}\r\n                                </div>\r\n                            </div> -->\r\n                            <!-- end form-floating -->\r\n                            <div class=\"form mb-3\" >\r\n                                <input  type=\"number\" class=\"form-control\" id=\"WilayahId\" placeholder=\"Wilayah\" v-model=\"registers.WilayahId\"/>\r\n                                <div v-if=\"validation.WilayahId\" class=\"text-danger\">\r\n                                   {{validation.WilayahId}}\r\n                                </div>\r\n                            </div><!-- end form-floating -->\r\n                            <div class=\"form-floating mb-3\">\r\n                                <input type=\"text\" class=\"form-control\" id=\"BentukBadanUsaha\" placeholder=\"Bentuk Badan Usaha\" v-model=\"registers.bentukusaha\">\r\n                                <label for=\"BentukBadanUsaha\">Bentuk Badan Usaha</label>\r\n                                <div v-if=\"validation.bentukusaha\" class=\"text-danger\">\r\n                                   {{validation.bentukusaha}}\r\n                                </div>\r\n                            </div>\r\n                            <!-- end form-floating -->\r\n                             <div class=\"mb-4\">\r\n                                <label class=\"mb-2 form-label\">Pesan</label>\r\n                                <textarea type=\"input\" name=\"pesan\" class=\"form-control form-control-s1\" id=\"AlasanBergabung\" placeholder=\"Alasan Untuk Bergabung\" v-model=\"registers.alasan\"></textarea>\r\n                                <div v-if=\"validation.alasan\" class=\"text-danger\">\r\n                                   {{validation.alasan}}\r\n                                </div>\r\n                            </div>\r\n                            <!-- form checkbox  -->\r\n                            <div class=\"form-check mb-2\">\r\n                                <input id=\"agree\" type=\"checkbox\" value=\"agree\" v-model=\"checked\">\r\n                                <span class=\"form-check-label mb-1 ms-2 form-text\" for=\"terms\" > {{ SectionData.registerData.termText }} <a><router-link to=\"/terms\">terms of service</router-link></a> and <a><router-link to=\"/privacy\">privacy and policy</router-link></a></span>                                \r\n                            </div>\r\n                            <!-- end form checkbox -->\r\n                            <button :disabled=\"!checked\" class=\"btn btn-dark w-100\" type=\"submit\">{{ SectionData.registerData.btnText }}</button>\r\n                            <!-- <span class=\"d-block my-3\">— or sign up with —</span>\r\n                            <ul class=\"btns-group d-flex\">\r\n                                <li class=\"flex-grow-1\" v-for=\"(list, i) in SectionData.registerData.btns\" :key=\"i\"><router-link :to=\"list.path\" class=\"btn d-block\" :class=\"list.btnClass\"><em class=\"ni\" :class=\"list.icon\"></em> {{ list.title }} </router-link></li>\r\n                            </ul> -->\r\n                            <p class=\"mt-3 form-text\">{{ SectionData.registerData.haveAccountText }} <router-link :to=\"SectionData.registerData.btnTextLink\" class=\"btn-link\">{{ SectionData.registerData.btnTextTwo }}</router-link></p>\r\n                        </form>\r\n                        <div class=\"alert alert-success\" v-if=\"isSuccess\">Registrasi Member Berhasil, Tunggu beberapa saat untuk di konfirmasi</div>\r\n                    </div><!-- end col-lg-6 -->\r\n                </div><!-- end row -->\r\n            </div><!-- end container -->\r\n        </section><!-- end register-section -->\r\n    <!-- <ModalSection1></ModalSection1> -->\r\n</template>\r\n\r\n<script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\nimport SectionData from '@/store/store.js'\r\nimport { reactive, ref } from 'vue';\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    name: 'RegisterSection',\r\n    data () {\r\n      return {\r\n        SectionData,\r\n        checked : false,\r\n        provinsis: [],\r\n        cities: []\r\n      };\r\n    },\r\n     methods: {\r\n        getProvinsis : function(){\r\n            axios.get('http://127.0.0.1:8000/api/provinsi')\r\n            .then(function (response) {\r\n                        this.provinsis = response.data;\r\n                    }.bind(this));\r\n        },\r\n        getCities: function() {\r\n                axios.get('http://127.0.0.1:8000/api/cities',{\r\n                    params: {\r\n                        provinsiId_id: this.country\r\n                    }\r\n                })\r\n                .then(function(response){\r\n                    this.cities = response.data;\r\n                }.bind(this));\r\n            }\r\n     },\r\n        created: function(){\r\n            this.getProvinsis()\r\n            this.getCities()\r\n  },\r\n    setup(){\r\n        //data binding \r\n        const registers = reactive({\r\n            name : '',\r\n            email : '',\r\n            password: '',\r\n            username: '',\r\n            namaPerushaan: '',\r\n            nomor: '',\r\n            companyindustry:'',\r\n            provinsiId: '',\r\n            KotaId: '',\r\n            WilayahId:'',\r\n            bentukusaha: '',\r\n            alasan: '',\r\n        });\r\n\r\n        const validation = ref ([]);\r\n\r\n        function store() {\r\n            axios.post(\r\n                'http://127.0.0.1:8000/api/register',\r\n                registers\r\n            )\r\n            .then(()=> {\r\n                console.log('Sudah BeNAR');\r\n            }).catch((err) => {\r\n                validation.value = err.response.data\r\n        });\r\n\r\n        }\r\n        return {\r\n            isSuccess: false,\r\n            registers,\r\n            validation,\r\n            store\r\n        }\r\n    },\r\n  mounted () {\r\n    /*  ======== Show/Hide passoword ======== */\r\n    function showHidePassword(selector){\r\n        let elem = document.querySelectorAll(selector);\r\n        if(elem.length > 0){\r\n            elem.forEach(item => {\r\n            item.addEventListener(\"click\", function(e){\r\n                e.preventDefault();\r\n                let target = document.getElementById(item.getAttribute(\"href\"));\r\n                if(target.type == \"password\") {\r\n                target.type = \"text\";\r\n                item.classList.add(\"is-shown\");\r\n                }else{\r\n                target.type = \"password\";\r\n                item.classList.remove(\"is-shown\");\r\n                }\r\n            });\r\n\r\n            });\r\n        }\r\n    }\r\n\r\n    showHidePassword(\".password-toggle\");\r\n\r\n  }\r\n}\r\n</script>\r\n<!-- <script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\nimport SectionData from '@/store/store.js'\r\nimport { reactive, ref } from 'vue';\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    name: 'RegisterSection',\r\n    data () {\r\n      return {\r\n        SectionData,\r\n        checked : false,\r\n        cities: []\r\n      };\r\n    },\r\n     methods: {\r\n    loadData(){\r\n        axios.get('http://127.0.0.1:8000/api/cities').then(({data}) => (this.cities = data));\r\n    }\r\n  },\r\n  created (){\r\n    this.loadData();\r\n  },\r\n    setup(){\r\n        //data binding \r\n        const registers = reactive({\r\n            name : '',\r\n            email : '',\r\n            password: '',\r\n            username: '',\r\n            namaPerushaan: '',\r\n            nomor: '',\r\n            companyindustry:'',\r\n            provinsiId: '',\r\n            KotaId: '',\r\n            WilayahId:'',\r\n            bentukusaha: '',\r\n            alasan: '',\r\n        });\r\n\r\n        const validation = ref ([]);\r\n\r\n        function store() {\r\n            axios.post(\r\n                'http://127.0.0.1:8000/api/register',\r\n                registers\r\n            )\r\n            .then(()=> {\r\n                console.log('Sudah BeNAR');\r\n            }).catch((err) => {\r\n                validation.value = err.response.data\r\n        });\r\n\r\n        }\r\n        return {\r\n            isSuccess: false,\r\n            registers,\r\n            validation,\r\n            store\r\n        }\r\n    },\r\n  mounted () {\r\n    /*  ======== Show/Hide passoword ======== */\r\n    function showHidePassword(selector){\r\n        let elem = document.querySelectorAll(selector);\r\n        if(elem.length > 0){\r\n            elem.forEach(item => {\r\n            item.addEventListener(\"click\", function(e){\r\n                e.preventDefault();\r\n                let target = document.getElementById(item.getAttribute(\"href\"));\r\n                if(target.type == \"password\") {\r\n                target.type = \"text\";\r\n                item.classList.add(\"is-shown\");\r\n                }else{\r\n                target.type = \"password\";\r\n                item.classList.remove(\"is-shown\");\r\n                }\r\n            });\r\n\r\n            });\r\n        }\r\n    }\r\n\r\n    showHidePassword(\".password-toggle\");\r\n\r\n  }\r\n}\r\n</script> -->"]}]}