eveTheme()\r\n\r\n      if (window) {\r\n        window.addEventListener('storage', function () {\r\n          retrieveTheme()\r\n        }, false)\r\n      }\r\n    }\r\n\r\n    themeSwitcher('.theme-toggler')\r\n  }\r\n}\r\n</script>"]}]}