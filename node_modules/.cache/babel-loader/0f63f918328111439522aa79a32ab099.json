turan</router-link>\r\n        </li>\r\n        <li class=\"breadcrumb-item\">Data Kas</li>\r\n      </ol>\r\n    </nav>\r\n    <!-- {{ provinsi }} -->\r\n    <div\r\n      v-if=\"checkPrivilege('provinsi-store')\"\r\n      class=\"d-grid gap-2 d-md-block\"\r\n    >\r\n      <button\r\n        @click=\"resetnama()\"\r\n        type=\"button\"\r\n        class=\"btn btn-dark btn-sm mb-2\"\r\n        data-bs-toggle=\"modal\"\r\n        data-bs-target=\"#messageModal\"\r\n      >\r\n        Tambah Daftar Kas\r\n      </button>\r\n    </div>\r\n    <!-- {{ khas }} -->\r\n    <div class=\"profile-setting-panel-wrap\">\r\n      <div class=\"table-responsive\">\r\n        <table class=\"table mb-0 table-s2\" id=\"dataKhas\">\r\n          <thead class=\"fs-14\">\r\n            <tr>\r\n              <th\r\n                scope=\"col\"\r\n                v-for=\"(list, i) in SectionData.khasData.khasTableHead\"\r\n                :key=\"i\"\r\n              >\r\n                {{ list }}\r\n              </th>\r\n            </tr>\r\n          </thead>\r\n          <tbody class=\"fs-13\">\r\n            <tr v-for=\"item in khas.data\" :key=\"item.id\">\r\n              <td>{{ item.kode }}</td>\r\n              <td>{{ item.nama }}</td>\r\n              <td>{{ item.saldo_awal }}</td>\r\n              <td>{{ item.saldo_akhir }}</td>\r\n              <td>{{ item.keterangan }}</td>\r\n              <td>{{ item.tanggal }}</td>\r\n              <td>{{ item.edit_by }}</td>\r\n              <td >\r\n                <button\r\n                  v-if=\"checkPrivilege('provinsi-update')\"\r\n                  @click=\"showKhas(item.id)\"\r\n                  class=\"col- icon-btn p-0 m-0\"\r\n                  title=\"Edit\"\r\n                  data-bs-toggle=\"modal\"\r\n                  data-bs-target=\"#updateModal\"\r\n                >\r\n                  <em class=\"fa fa-pencil-square-o\"></em>\r\n                </button>\r\n                <!-- <button\r\n                  v-if=\"checkPrivilege('provinsi-delete')\"\r\n                  @click=\"showDelete(item.id)\"\r\n                  class=\"col- icon-btn p-0 m-0\"\r\n                  title=\"Delete\"\r\n                >\r\n                  <em class=\"ni ni-trash\"></em>\r\n                </button> -->\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <!-- end table-responsive -->\r\n      <!-- pagination -->\r\n      <div class=\"text-center mt-4 mt-md-5\">\r\n        <Pagination\r\n          :records=\"records.length\"\r\n          v-model=\"page\"\r\n          :per-page=\"perPage\"\r\n        ></Pagination>\r\n      </div>\r\n    </div>\r\n    <!-- end profile-setting-panel-wrap-->\r\n    <!-- Modal store -->\r\n    <form @submit.prevent=\"postKhas()\">\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"messageModal\"\r\n        tabindex=\"-1\"\r\n        aria-labelledby=\"reportModalLabel\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog modal-dialog-centered\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h4 class=\"modal-title\" id=\"reportModalLabel\">Tambah Kas</h4>\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn-close icon-btn\"\r\n                data-bs-dismiss=\"modal\"\r\n                aria-label=\"Close\"\r\n              >\r\n                <em class=\"ni ni-cross\"></em>\r\n              </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"form-floating mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"kodeakun\"\r\n                  placeholder=\"Kode\"\r\n                  v-model=\"kode\"\r\n                  required\r\n                />\r\n                <label for=\"kodeakun\">Kode</label>\r\n              </div>\r\n              <div class=\"form-floating mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"nama\"\r\n                  placeholder=\"Nama\"\r\n                  v-model=\"nama\"\r\n                  required\r\n                />\r\n                <label for=\"nama\">Nama</label>\r\n              </div>\r\n              <div class=\"form-floating mb-3\">\r\n                <input\r\n                  type=\"number\"\r\n                  class=\"form-control\"\r\n                  id=\"jumlah\"\r\n                  placeholder=\"Jumlah\"\r\n                  v-model=\"saldo_awal\"\r\n                  required\r\n                />\r\n                <label for=\"jumlah\">Jumlah</label>\r\n              </div>\r\n              <div class=\"form-group mb-3\">\r\n                <label>Kode Akun </label>\r\n                <v-select\r\n                  v-model=\"kode_akun\"\r\n                  required\r\n                  :options=\"kategori\"\r\n                  :reduce=\"(kategori) => kategori.value\"\r\n                  label=\"text\"\r\n                >\r\n                </v-select>\r\n              </div>\r\n              <div class=\"form-floating mb-3\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"keterangan\"\r\n                  placeholder=\"Keterangan\"\r\n                  v-model=\"keterangan\"\r\n                  required\r\n                />\r\n                <label for=\"keterangan\">Keterangan</label>\r\n              </div>\r\n              <!-- end form-floating -->\r\n              <button\r\n                class=\"btn btn-dark w-100\"\r\n                data-bs-dismiss=\"modal\"\r\n                type=\"submit\"\r\n              >\r\n                Tambah\r\n              </button>\r\n            </div>\r\n            <!-- end modal-body -->\r\n          