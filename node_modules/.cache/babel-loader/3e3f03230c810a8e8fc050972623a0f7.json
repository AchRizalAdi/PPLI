,iBAAV,EAA6B,EAA7B,EAAiC,MAAjC;QACF;MACD,CAdD;IAeD,CAjBM;;IAkBP;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACAU,MAAM,CAACjB,EAAD,EAAK;MACT;MACAX,KAAK,CAAC6B,GAAN,CAAU,0CAAV,EAAsDN,IAAtD,CACE,UAAUO,QAAV,EAAoB;QAClB,KAAKnB,EAAL,GAAUA,EAAV;QACA,KAAKC,eAAL,GAAuBkB,QAAQ,CAACxB,IAAhC;MACD,CAHD,CAGEyB,IAHF,CAGO,IAHP,CADF;IAMD,CAlCM;;IAmCPC,YAAY,EAAE,YAAY;MACxBhC,KAAK,CAAC6B,GAAN,CAAU,wCAAV,EAAoDN,IAApD,CACE,UAAUO,QAAV,EAAoB;QAClB,KAAKjB,SAAL,GAAiBiB,QAAQ,CAACxB,IAAT,CAAcA,IAA/B,CADkB,CAElB;;QACA2B,UAAU,CAAC,MAAM;UACfhC,CAAC,CAAC,UAAD,CAAD,CAAciC,SAAd;QACD,CAFS,EAEP,GAFO,CAAV;MAGD,CAND,CAMEH,IANF,CAMO,IANP,CADF;IASD,CA7CM;;IA+CPI,YAAY,CAACxB,EAAD,EAAK;MACf;MACAX,KAAI,CACDoC,IADH,CACQ,6CAA6CzB,EADrD,EACyD;QACrD0B,MAAM,EAAE,KAAKA;MADwC,CADzD,EAIGd,IAJH,CAISO,QAAD,IAAc;QAClB,KAAKE,YAAL;QACAM,OAAO,CAACC,GAAR,CAAYT,QAAZ;QACA,KAAKU,MAAL,CAAYC,IAAZ,CAAiBX,QAAQ,CAACxB,IAAT,CAAc+B,MAA/B;MACD,CARH,EASGK,KATH,CASUC,KAAD,IAAW;QAChB,KAAKH,MAAL,CAAYC,IAAZ,CAAiB,cAAjB;QACAH,OAAO,CAACC,GAAR,CAAYI,KAAZ;MACD,CAZH;MAaA,KAAKN,MAAL,GAAc,EAAd;IACD,CA/DM;;IAgEPO,cAAc,CAACjC,EAAD,EAAK;MACjB;MAEAX,KAAK,CAAC6C,MAAN,CAAa,+CAA+ClC,EAA5D,EAAgEY,IAAhE,CACE,YAAY;QACVtB,CAAC,CAAC,UAAD,CAAD,CAAciC,SAAd,GAA0BY,OAA1B;QACA1C,OAAO,CAAC2C,IAAR,CAAa,aAAb;QACA,KAAKP,MAAL,CAAYC,IAAZ,CAAiB,iBAAjB;MACD,CAJD,CAIEV,IAJF,CAIO,IAJP,CADF;IAOD,CA1EM;;IA2EPiB,SAAS,CAACrC,EAAD,EAAK;MACZ,KAAKA,EAAL,GAAUA,EAAV;IACD,CA7EM;;IA8EPsC,KAAK,CAACtC,EAAD,EAAK;MACRX,KAAI,CACDoC,IADH,CACQ,8CAA8CzB,EADtD,EAC0D;QACtDsC,KAAK,EAAE,KAAKnC;MAD0C,CAD1D,EAIGS,IAJH,CAISO,QAAD,IAAc;QAClB,KAAKE,YAAL;QACA,KAAKQ,MAAL,CAAYC,IAAZ,CAAiB,gBAAjB;QACAH,OAAO,CAACC,GAAR,CAAYT,QAAZ;MACD,CARH,EASGY,KATH,CASUC,KAAD,IAAW;QAChB,KAAKH,MAAL,CAAYC,IAAZ,CAAiB,sBAAjB;QACAH,OAAO,CAACC,GAAR,CAAYI,KAAZ;MACD,CAZH;MAaA,KAAK7B,SAAL,GAAiB,EAAjB;IACD;;EA7FM,CAvBI;EAsHboC,OAAO,EAAE,YAAY;IACnB,KAAKlB,YAAL;IACA5B,OAAO,CAAC+C,EAAR,CAAW,aAAX,EAA0B,MAAM;MAC9B,KAAKnB,YAAL;IACD,CAFD,EAFmB,CAKnB;EACD,CA5HY;EA6HboB,QAAQ,EAAE;IACRC,gBAAgB,GAAG;MACjB,MAAMC,UAAS,GAAI,KAAK7C,OAAL,IAAgB,KAAKD,IAAL,GAAY,CAA5B,CAAnB;MACA,MAAM+C,QAAO,GAAID,UAAS,GAAI,KAAK7C,OAAnC;MACA,OAAO,KAAKC,OAAL,CAAa8C,KAAb,CAAmBF,UAAnB,EAA+BC,QAA/B,CAAP;IACD;;EALO;AA7HG,CAAf","names":["SectionData","Pagination","axios","$","Swal","mitt","emitter","components","data","name","page","perPage","records","id","status_register","registers","isi_pesan","cekWilayah","methods","showDelete","fire","title","showDenyButton","confirmButtonText","denyButtonText","then","result","isConfirmed","deleteRegisters","isDenied","update","get","response","bind","getRegisters","setTimeout","DataTable","updateStatus","post","status","console","log","$toast","show","catch","error","deleteRgisters","delete","destroy","emit","sendEmail","pesan","created","on","computed","displayedRecords","startIndex","endIndex","slice"],"sourceRoot":"","sources":["E:\\Codeee\\PPLI FE BE\\PPLI\\src\\components\\section\\OfferSection.vue"],"sourcesContent":["<template>\r\n  <div class=\"col-lg-9 ps-xl-5\">\r\n    <div class=\"user-panel-title-box\">\r\n      <h3>{{ SectionData.registersData.mainTitle }}</h3>\r\n    </div>\r\n    <!-- {{kondisi}} -->\r\n    <!-- end user-panel-title-box -->\r\n    <div class=\"d-flex flex-row\">\r\n      <div class=\"d-grid ms-1 gap-2 d-md-block\">\r\n        <router-link\r\n          to=\"/log\"\r\n          type=\"button\"\r\n          class=\"btn btn-sm btn-dark mb-3\"\r\n        >\r\n          Log Register\r\n        </router-link>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"profile-setting-panel-wrap\">\r\n      <div class=\"table-responsive\">\r\n        <table class=\"table mb-0 table-s2\" id=\"dataReg\">\r\n          <thead class=\"fs-14\">\r\n            <tr>\r\n              <th\r\n                scope=\"col\"\r\n                v-for=\"(list, i) in SectionData.registersData\r\n                  .registersTableHead\"\r\n                :key=\"i\"\r\n              >\r\n                {{ list }}\r\n              </th>\r\n            </tr>\r\n          </thead>\r\n          <tbody class=\"fs-13\">\r\n            <tr\r\n              v-for=\"(item, index) in registers\"\r\n              :value=\"item.id\"\r\n              :key=\"item.id\"\r\n            >\r\n              <th scope=\"row\" id=\"id\">\r\n                <a href=\"#\">{{ index + 1 }}</a>\r\n              </th>\r\n              <td>{{ item.name }}</td>\r\n              <td>{{ item.nama_perusahaan }}</td>\r\n              <!-- <td>{{ item.PhoneNumber }}</td> -->\r\n              <!-- <td><span v-for=\"items in item.company_industry\" :key=\"items.id\">{{items.name}}<br></span></td> -->\r\n              <td>{{ item.wilayah }}</td>\r\n              <!-- <td>{{ item.cities.name }}</td> -->\r\n              <!-- <td>{{ item.BentukBadanUsaha }}</td> -->\r\n              <!-- <td>{{ item.cekWilayah }}</td> -->\r\n              <td>\r\n                <button\r\n                  :disabled=\"item.cekWilayah\"\r\n                  class=\"btn btn-warning btn-sm\"\r\n                  @click=\"update(item.id)\"\r\n                  id=\"statusss\"\r\n                  data-bs-toggle=\"modal\"\r\n                  data-bs-target=\"#statussModal\"\r\n                >\r\n                  {{ item.status }}\r\n                </button>\r\n              </td>\r\n              <td>\r\n                <button\r\n                  :disabled=\"item.cekStatus\"\r\n                  class=\"btn btn-primary btn-sm\"\r\n                  @click=\"sendEmail(item.id)\"\r\n                  id=\"pesan\"\r\n                  data-bs-toggle=\"modal\"\r\n                  data-bs-target=\"#pesanModal\"\r\n                >\r\n                  Kirim Pesan\r\n                </button>\r\n              </td>\r\n              <td class=\"row\">\r\n                <!-- v-if=\"checkPrivilege('kontak-update')\" -->\r\n                <router-link\r\n                  :to=\"{ name: 'show-register', params: { id: item.id } }\"\r\n                  class=\"col- p-0 m-0 icon-btn btn-sm\"\r\n                  title=\"Edit\"\r\n                  ><em class=\"fa fa-eye\"></em\r\n                ></router-link>\r\n                <button\r\n                  :disabled=\"item.cekWilayah\"\r\n                  type=\"button\"\r\n                  class=\"col- p-0 m-0 icon-btn btn-sm\"\r\n                  title=\"Delete\"\r\n                  @click=\"showDelete(item.id)\"\r\n                >\r\n                  <em class=\"ni ni-trash\"></em>\r\n                </button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <!-- end table-responsive -->\r\n      <!-- pagination -->\r\n      <div class=\"text-center mt-4 mt-md-2\">\r\n        <Pagination\r\n          :records=\"records.length\"\r\n          v-model=\"page\"\r\n          :per-page=\"perPage\"\r\n        ></Pagination>\r\n      </div>\r\n      <!-- Modal store -->\r\n      <form @submit.prevent=\"updateStatus(id)\">\r\n        <div\r\n          class=\"modal fade\"\r\n          id=\"statussModal\"\r\n          tabindex=\"-1\"\r\n          aria-labelledby=\"reportModalLabel\"\r\n          aria-hidden=\"true\"\r\n        >\r\n          <div class=\"modal-dialog modal-dialog-centered\">\r\n            <div class=\"modal-content\">\r\n              <div class=\"modal-header\">\r\n                <h4 class=\"modal-title\" id=\"reportModalLabel\">Permission</h4>\r\n                <button\r\n                  type=\"button\"\r\n                  class=\"btn-close icon-btn\"\r\n                  data-bs-dismiss=\"modal\"\r\n                  aria-label=\"Close\"\r\n                >\r\n                  <em class=\"ni ni-cross\"></em>\r\n                </button>\r\n              </div>\r\n              <div class=\"modal-body\">\r\n                <!-- {{status_register}} -->\r\n                <div class=\"form-group mb-2\">\r\n                  <label>status </label>\r\n                  <select v-model=\"status\" class=\"form-control\" required>\r\n                    <option\r\n                      v-for=\"data in status_register.data\"\r\n                      :value=\"data.name\"\r\n                      :key=\"data.id\"\r\n                    >\r\n                      {{ data.name }}\r\n                    </option>\r\n                  </select>\r\n                </div>\r\n                <button\r\n                  class=\"btn btn-dark w-100\"\r\n                  data-bs-dismiss=\"modal\"\r\n                  type=\"submit\"\r\n                >\r\n                  Select\r\n                </button>\r\n              </div>\r\n              <!-- end modal-body -->\r\n            </div>\r\n            <!-- end modal-content -->\r\n          </div>\r\n          <!-- end modal-dialog -->\r\n        </div>\r\n        <!-- end modal-->\r\n      </form>\r\n      <!-- Modal Pesan -->\r\n      <form @submit.prevent=\"pesan(id)\">\r\n        <div\r\n          class=\"modal fade\"\r\n          id=\"pesanModal\"\r\n          tabindex=\"-1\"\r\n          aria-labelledby=\"reportModalLabel\"\r\n          aria-hidden=\"true\"\r\n        >\r\n          <div class=\"modal-dialog modal-dialog-centered\">\r\n            <div class=\"modal-content\">\r\n              <div class=\"modal-header\">\r\n                <h4 class=\"modal-title\" id=\"reportModalLabel\">Mail</h4>\r\n                <button\r\n                  type=\"button\"\r\n                  class=\"btn-close icon-btn\"\r\n                  data-bs-dismiss=\"modal\"\r\n                  aria-label=\"Close\"\r\n                >\r\n                  <em class=\"ni ni-cross\"></em>\r\n                </button>\r\n              </div>\r\n              <div class=\"modal-body\">\r\n                <div class=\"form-group mb-4\">\r\n                  <label>Isi Pesan DIbawah</label>\r\n                  <div class=\"form-floating mb-3\">\r\n                    <textarea\r\n                      type=\"input\"\r\n                      class=\"form-control\"\r\n                      id=\"nama\"\r\n                      v-model=\"isi_pesan\"\r\n                      required\r\n                    ></textarea>\r\n                  </div>\r\n                  <!-- end form-floating -->\r\n                </div>\r\n                <button\r\n                  class=\"btn btn-dark w-100\"\r\n                  data-bs-dismiss=\"modal\"\r\n                  type=\"submit\"\r\n                >\r\n                  Send\r\n                </button>\r\n              </div>\r\n              <!-- end modal-body -->\r\n            </div>\r\n            <!-- end modal-content -->\r\n          </div>\r\n          <!-- end modal-dialog -->\r\n        </div>\r\n        <!-- end modal-->\r\n      </form>\r\n    </div>\r\n    <!-- end profile-setting-panel-wrap-->\r\n  </div>\r\n  <!-- end col-lg-8 -->\r\n</template>\r\n\r\n<script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\nimport SectionData from \"@/store/store.js\";\r\nimport Pagination from \"v-pagination-3\";\r\nimport axios from \"axios\";\r\nimport $ from \"jquery\";\r\nimport Swal from \"sweetalert2\";\r\nimport mitt from \"mitt\";\r\nconst emitter = mitt();\r\n\r\nexport default {\r\n  components: {\r\n    Pagination,\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      // checked: false,\r\n      name: \"OfferSection\",\r\n      SectionData,\r\n      page: 1,\r\n      perPage: 6,\r\n      records: [],\r\n      id: [],\r\n      status_register: [],\r\n      registers: [],\r\n      isi_pesan: [],\r\n      data: [],\r\n      cekWilayah: [],\r\n      // kondisi: \"\"\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    showDelete(id) {\r\n      Swal.fire({\r\n        title: \"Apakah anda ingin menghapus data ini?\",\r\n        showDenyButton: true,\r\n        // showCancelButton: true,\r\n        confirmButtonText: \"Iya\",\r\n        denyButtonText: `Tidak`,\r\n      }).then((result) => {\r\n        /* Read more about isConfirmed, isDenied below */\r\n        if (result.isConfirmed) {\r\n          this.deleteRegisters(id);\r\n          Swal.fire(\"Berhasil Terhapus!\", \"\", \"success\");\r\n        } else if (result.isDenied) {\r\n          Swal.fire(\"Tidak Tersimpan\", \"\", \"info\");\r\n        }\r\n      });\r\n    },\r\n    // checkWilayah(){\r\n    //   axios.get(\"http://127.0.0.1:8000/api/userRegister\").then(\r\n    //     function (response) {\r\n    //       this.cekWilayah = response.data.data;\r\n    //       // this.checked = cekWilayah.cekWilayah;\r\n    //     }.bind(this)\r\n    //   );\r\n    // },\r\n    update(id) {\r\n      // alert(document.getElementById('registers').value)\r\n      axios.get(\"http://127.0.0.1:8000/api/statusRegister\").then(\r\n        function (response) {\r\n          this.id = id;\r\n          this.status_register = response.data;\r\n        }.bind(this)\r\n      );\r\n    },\r\n    getRegisters: function () {\r\n      axios.get(\"http://127.0.0.1:8000/api/userRegister\").then(\r\n        function (response) {\r\n          this.registers = response.data.data;\r\n          // this.kondisi = response.data.data;\r\n          setTimeout(() => {\r\n            $(\"#dataReg\").DataTable();\r\n          }, 100);\r\n        }.bind(this)\r\n      );\r\n    },\r\n\r\n    updateStatus(id) {\r\n      // alert(id)\r\n      axios\r\n        .post(\"http://127.0.0.1:8000/api/update/member/\" + id, {\r\n          status: this.status,\r\n        })\r\n        .then((response) => {\r\n          this.getRegisters();\r\n          console.log(response);\r\n          this.$toast.show(response.data.status);\r\n        })\r\n        .catch((error) => {\r\n          this.$toast.show(\"gagal update\");\r\n          console.log(error);\r\n        });\r\n      this.status = \"\";\r\n    },\r\n    deleteRgisters(id) {\r\n      // alert(id);\r\n\r\n      axios.delete(\"http://127.0.0.1:8000/api/register/delete/\" + id).then(\r\n        function () {\r\n          $(\"#dataReg\").DataTable().destroy();\r\n          emitter.emit(\"refreshPage\");\r\n          this.$toast.show(\"berhasil delete\");\r\n        }.bind(this)\r\n      );\r\n    },\r\n    sendEmail(id) {\r\n      this.id = id;\r\n    },\r\n    pesan(id) {\r\n      axios\r\n        .post(\"http://127.0.0.1:8000/api/register/email/\" + id, {\r\n          pesan: this.isi_pesan,\r\n        })\r\n        .then((response) => {\r\n          this.getRegisters();\r\n          this.$toast.show(\"pesan terkirim\");\r\n          console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$toast.show(\"gagal mengirim pesan\");\r\n          console.log(error);\r\n        });\r\n      this.isi_pesan = \"\";\r\n    },\r\n  },\r\n  created: function () {\r\n    this.getRegisters();\r\n    emitter.on(\"refreshPage\", () => {\r\n      this.getRegisters();\r\n    });\r\n    // this.checkWilayah();\r\n  },\r\n  computed: {\r\n    displayedRecords() {\r\n      const startIndex = this.perPage * (this.page - 1);\r\n      const endIndex = startIndex + this.perPage;\r\n      return this.records.slice(startIndex, endIndex);\r\n    },\r\n  },\r\n};\r\n</script>\r\n//\r\n<!-- <select v-model=\"status\" id=\"statusss\" class='form-control w-100' @change=\"postStatus(item.id, $event)\" >\r\n//                                                 <option :value=\"null\" disabled>Mail Verified</option>\r\n//                                                 <option value=\"Approved by DPP\">Approved by DPP</option>\r\n//                                                 <option value=\"Approved by DPW\">Approved by DPW</option>\r\n//                                                 <option value=\"Rejected by DPP\">Rejected by DPP</option>\r\n//                                                 <option value=\"Rejected by DPW\">Rejected by DPW</option>\r\n//                                             </select> -->\r\n\r\n//\r\n<!-- <td><button class=\"icon-btn ms-auto\" title=\"Accept\" for=\"status\" ><em class=\"ni ni-check\"></em></button></td> -->\r\n"]}]}