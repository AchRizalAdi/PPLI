CAAW,aAAX,EAA0B,MAAM;MAC9B,KAAKT,QAAL;IACD,CAFD;EAGD,CAxHY;EAyHbU,QAAQ,EAAE;IACRC,gBAAgB,GAAG;MACjB,MAAMC,UAAS,GAAI,KAAKlD,OAAL,IAAgB,KAAKF,IAAL,GAAY,CAA5B,CAAnB;MACA,MAAMqD,QAAO,GAAID,UAAS,GAAI,KAAKlD,OAAnC;MACA,OAAO,KAAKC,OAAL,CAAamD,KAAb,CAAmBF,UAAnB,EAA+BC,QAA/B,CAAP;IACD;;EALO;AAzHG,CAAf","names":["SectionData","Pagination","axios","$","Swal","mitt","emitter","components","data","page","i","perPage","records","provinsis","name","cities","provinsi","roles","methods","resetnama","showPost","fire","position","icon","title","showConfirmButton","timer","showDelete","id","showDenyButton","confirmButtonText","denyButtonText","then","result","isConfirmed","deleteRoles","isDenied","postRoles","post","response","DataTable","destroy","emit","$toast","success","console","log","catch","error","getRoles","get","setTimeout","bind","putRoles","show","showRoles","delete","created","on","computed","displayedRecords","startIndex","endIndex","slice"],"sourceRoot":"","sources":["E:\\Codeee\\PPLI FE BE\\PPLI\\src\\components\\section\\GroupSection.vue"],"sourcesContent":["<template>\r\n  <div class=\"col-lg-9 ps-xl-5\">\r\n    <div class=\"user-panel-title-box\">\r\n      <h3>Group</h3>\r\n    </div>\r\n    <!-- end user-panel-title-box -->\r\n    <nav aria-label=\"breadcrumb\">\r\n      <ol class=\"breadcrumb breadcrumb-s1 justify-content-left mb-3\">\r\n        <li class=\"breadcrumb-item\">\r\n          <router-link to=\"/pengaturan\">Pengaturan</router-link>\r\n        </li>\r\n        <li class=\"breadcrumb-item\">Group</li>\r\n      </ol>\r\n    </nav>\r\n    <!-- {{ cities }} -->\r\n    <div class=\"d-grid gap-2 d-md-block\">\r\n      <button\r\n        @click=\"resetnama()\"\r\n        type=\"button\"\r\n        class=\"btn btn-dark btn-sm mb-2\"\r\n        data-bs-toggle=\"modal\"\r\n        data-bs-target=\"#addModal\"\r\n      >\r\n        Add Group\r\n      </button>\r\n    </div>\r\n    <div class=\"profile-setting-panel-wrap\">\r\n      <div class=\"table\">\r\n        <table class=\"table mb-0 table-s2\" id=\"dataRoles\">\r\n          <thead class=\"fs-14\">\r\n            <tr>\r\n              <th\r\n                scope=\"col\"\r\n                v-for=\"(list, i) in SectionData.groupData.groupTableHead\"\r\n                :key=\"i\"\r\n              >\r\n                {{ list }}\r\n              </th>\r\n            </tr>\r\n          </thead>\r\n          <tbody class=\"fs-13\">\r\n            <tr v-for=\"item in roles.data\" :key=\"item.id\">\r\n              <td>{{ item.name }}</td>\r\n              <td class=\"row\">\r\n                <router-link\r\n                  :to=\"{ name: 'privilege', params: { id: item.id } }\"\r\n                  class=\"col- p-0 m-0 icon-btn\"\r\n                  title=\"Show\"\r\n                  ><em class=\"fa-solid fa-gears\"></em\r\n                ></router-link>\r\n                <!-- <button  @click=\"showRoles(item.id)\" class=\"col- p-0 m-0 icon-btn\" title=\"Edit\" data-bs-toggle=\"modal\"  data-bs-target=\"#editModal\"><em class=\"ni ni-file\" ></em></button> -->\r\n                <button\r\n                 v-if=\"item.cekRoles\"\r\n                  @click=\"showDelete(item.id)\"\r\n                  class=\"col- p-0 m-0 icon-btn\"\r\n                  title=\"Delete\"\r\n                >\r\n                  <em class=\"ni ni-trash\"></em>\r\n                </button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <!-- end table-responsive -->\r\n      <!-- pagination -->\r\n      <div class=\"text-center mt-4 mt-md-5\">\r\n        <Pagination\r\n          :records=\"records.length\"\r\n          v-model=\"page\"\r\n          :per-page=\"perPage\"\r\n        ></Pagination>\r\n      </div>\r\n    </div>\r\n    <!-- end profile-setting-panel-wrap-->\r\n    <!-- Modal store -->\r\n    <form @submit.prevent=\"postRoles()\">\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"addModal\"\r\n        tabindex=\"-1\"\r\n        aria-labelledby=\"reportModalLabel\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog modal-dialog-centered\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h4 class=\"modal-title\" id=\"reportModalLabel\">Group</h4>\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn-close icon-btn\"\r\n                data-bs-dismiss=\"modal\"\r\n                aria-label=\"Close\"\r\n              >\r\n                <em class=\"ni ni-cross\"></em>\r\n              </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"form-floating mb-3 mt-4\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"nama\"\r\n                  placeholder=\"name\"\r\n                  v-model=\"name\"\r\n                  required\r\n                />\r\n                <label for=\"nama\">Name</label>\r\n              </div>\r\n              <!-- end form-floating -->\r\n              <button\r\n                class=\"btn btn-dark w-100\"\r\n                data-bs-dismiss=\"modal\"\r\n                type=\"submit\"\r\n              >\r\n                Add\r\n              </button>\r\n            </div>\r\n            <!-- end modal-body -->\r\n          </div>\r\n          <!-- end modal-content -->\r\n        </div>\r\n        <!-- end modal-dialog -->\r\n      </div>\r\n      <!-- end modal-->\r\n    </form>\r\n    <form @submit.prevent=\"putRoles(id)\">\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"editModal\"\r\n        tabindex=\"-1\"\r\n        aria-labelledby=\"reportModalLabel\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog modal-dialog-centered\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h4 class=\"modal-title\" id=\"reportModalLabel\">Update Group</h4>\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn-close icon-btn\"\r\n                data-bs-dismiss=\"modal\"\r\n                aria-label=\"Close\"\r\n              >\r\n                <em class=\"ni ni-cross\"></em>\r\n              </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"form-floating mb-3 mt-4\">\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"nama\"\r\n                  placeholder=\"name\"\r\n                  v-model=\"name\"\r\n                  required\r\n                />\r\n                <label for=\"nama\">Name</label>\r\n              </div>\r\n              <!-- end form-floating -->\r\n              <button\r\n                class=\"btn btn-dark w-100\"\r\n                data-bs-dismiss=\"modal\"\r\n                type=\"submit\"\r\n              >\r\n                update\r\n              </button>\r\n            </div>\r\n            <!-- end modal-body -->\r\n          </div>\r\n          <!-- end modal-content -->\r\n        </div>\r\n        <!-- end modal-dialog -->\r\n      </div>\r\n      <!-- end modal-->\r\n    </form>\r\n  </div>\r\n  <!-- end col-lg-8 -->\r\n</template>\r\n\r\n<script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\nimport SectionData from \"@/store/store.js\";\r\nimport Pagination from \"v-pagination-3\";\r\nimport axios from \"axios\";\r\nimport $ from \"jquery\";\r\nimport Swal from \"sweetalert2\";\r\nimport mitt from \"mitt\";\r\nconst emitter = mitt();\r\n// import { reactive } from 'vue';\r\n// import { onMounted, ref } from 'vue';\r\n\r\nexport default {\r\n  components: {\r\n    Pagination,\r\n  },\r\n  data() {\r\n    return {\r\n      SectionData,\r\n      page: 1,\r\n      i: 1,\r\n      perPage: 6,\r\n      records: [],\r\n      provinsis: [],\r\n      name: [],\r\n      cities: [],\r\n      provinsi: [],\r\n      roles: [],\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    resetnama() {\r\n      this.name = null;\r\n    },\r\n    showPost() {\r\n      Swal.fire({\r\n        position: \"center\",\r\n        icon: \"success\",\r\n        title: \"Data telah tersimpan!\",\r\n        showConfirmButton: false,\r\n        timer: 1500,\r\n      });\r\n    },\r\n    showDelete(id) {\r\n      Swal.fire({\r\n        title: \"Apakah anda ingin menghapus data ini?\",\r\n        showDenyButton: true,\r\n        // showCancelButton: true,\r\n        confirmButtonText: \"Iya\",\r\n        denyButtonText: `Tidak`,\r\n      }).then((result) => {\r\n        /* Read more about isConfirmed, isDenied below */\r\n        if (result.isConfirmed) {\r\n          this.deleteRoles(id);\r\n          Swal.fire(\"Data Terhapus!\", \"\", \"success\");\r\n        } else if (result.isDenied) {\r\n          Swal.fire(\"Tidak Tersimpan\", \"\", \"info\");\r\n        }\r\n      });\r\n    },\r\n    postRoles() {\r\n      axios\r\n        .post(\"http://127.0.0.1:8000/api/role/store\", {\r\n          name: this.name,\r\n        })\r\n        .then((response) => {\r\n          this.showPost();\r\n          $(\"#dataRoles\").DataTable().destroy();\r\n          emitter.emit(\"refreshPage\");\r\n          this.$toast.success(\"berhasil ditambahkan\");\r\n          console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$toast.error(\"gagal menambahkan data\");\r\n          console.log(error);\r\n        });\r\n      this.name = \"\";\r\n    },\r\n    getRoles: function () {\r\n      axios.get(\"http://127.0.0.1:8000/api/role/show\").then(\r\n        function (response) {\r\n          this.roles = response.data;\r\n          setTimeout(() => {\r\n            $(\"#dataRoles\").DataTable();\r\n          }, 100);\r\n        }.bind(this)\r\n      );\r\n    },\r\n    putRoles(id) {\r\n      axios\r\n        .post(\"http://127.0.0.1:8000/api/role/update/\" + id, {\r\n          name: this.name,\r\n        })\r\n        .then((response) => {\r\n          this.getRoles();\r\n          this.$toast.show(\"berhasil update\");\r\n          console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$toast.show(\"gagal update\");\r\n          console.log(error);\r\n        });\r\n      this.name = \"\";\r\n    },\r\n    showRoles(id) {\r\n      // alert(id);\r\n      axios.get(\"http://127.0.0.1:8000/api/role/show/\" + id).then(\r\n        function (response) {\r\n          // alert(response);\r\n          // console.log(response.data.data.id);\r\n          this.id = response.data.data.id;\r\n          this.name = response.data.data.name;\r\n        }.bind(this)\r\n      );\r\n    },\r\n    deleteRoles(id) {\r\n      axios.delete(\"http://127.0.0.1:8000/api/admin/delete/roles/\" + id).then(\r\n        function () {\r\n          $(\"#dataRoles\").DataTable().destroy();\r\n          emitter.emit(\"refreshPage\");\r\n        }.bind(this)\r\n      );\r\n    },\r\n\r\n    // batas\r\n  },\r\n  created: function () {\r\n    this.getRoles();\r\n    emitter.on(\"refreshPage\", () => {\r\n      this.getRoles();\r\n    });\r\n  },\r\n  computed: {\r\n    displayedRecords() {\r\n      const startIndex = this.perPage * (this.page - 1);\r\n      const endIndex = startIndex + this.perPage;\r\n      return this.records.slice(startIndex, endIndex);\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}