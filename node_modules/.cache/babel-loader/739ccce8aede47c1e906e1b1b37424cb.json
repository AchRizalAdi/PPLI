AAaiD,KAAb,CAAmBF,UAAnB,EAA+BC,QAA/B,CAAP;IACF;;EALQ;AAhHG,CAAf","names":["SectionData","Pagination","axios","$","Swal","components","data","page","i","perPage","records","provinsis","name","cities","provinsi","roles","methods","resetnama","showPost","fire","position","icon","title","showConfirmButton","timer","showDelete","id","showDenyButton","confirmButtonText","denyButtonText","then","result","isConfirmed","deleteRoles","isDenied","postRoles","post","response","getRoles","$toast","success","console","log","catch","error","get","document","ready","DataTable","bind","putRoles","show","showRoles","delete","created","computed","displayedRecords","startIndex","endIndex","slice"],"sourceRoot":"","sources":["E:\\Codeee\\PPLI FE BE\\PPLI\\src\\components\\section\\GroupSection.vue"],"sourcesContent":["<template>\r\n   <div class=\"col-lg-9 ps-xl-5\">\r\n                        <div class=\"user-panel-title-box\">\r\n                            <h3>Group</h3>\r\n                        </div><!-- end user-panel-title-box -->\r\n                        \r\n                <!-- {{ cities }} -->\r\n                        <div class=\"d-grid gap-2 d-md-block\">\r\n                            <button @click=\"resetnama()\" type=\"button\" class=\"btn btn-primary btn-sm mb-2\" data-bs-toggle=\"modal\" data-bs-target=\"#addModal\">Add Group</button>\r\n                        </div>\r\n                        <div class=\"profile-setting-panel-wrap\">\r\n                            <div class=\"table\">\r\n                                <table class=\"table mb-0 table-s2\" id=\"dataTable\">\r\n                                    <thead class=\"fs-14\">\r\n                                        <tr>\r\n                                            <th scope=\"col\" v-for=\"(list, i) in SectionData.groupData.groupTableHead\" :key=\"i\">{{ list }}</th>\r\n                                        </tr>\r\n                                    </thead>\r\n                                    <tbody class=\"fs-13\">\r\n                                        <tr v-for=\"(item) in roles.data\"  :key=\"item.id\">\r\n                                            <td>{{item.name}}</td>\r\n                                            <td class=\"row\">\r\n                                                <router-link  :to=\"{name: 'privilege', params: { id: item.id }}\" class=\"col- p-0 m-0 icon-btn\" title=\"Show\"><em class=\"fa-solid fa-gears\" ></em></router-link>\r\n                                                <!-- <button  @click=\"showRoles(item.id)\" class=\"col- p-0 m-0 icon-btn\" title=\"Edit\" data-bs-toggle=\"modal\"  data-bs-target=\"#editModal\"><em class=\"ni ni-file\" ></em></button> -->\r\n                                                <button  @click=\"showDelete(item.id)\" class=\"col- p-0 m-0 icon-btn\" title=\"Delete\" ><em class=\"ni ni-trash\"></em></button>\r\n                                            </td>\r\n                                        </tr> \r\n                                    </tbody>\r\n                                </table>\r\n                            </div><!-- end table-responsive -->\r\n                            <!-- pagination -->\r\n                            <div class=\"text-center mt-4 mt-md-5\">\r\n                                <Pagination :records=\"records.length\" v-model=\"page\" :per-page=\"perPage\"></Pagination>\r\n                            </div>\r\n                        </div><!-- end profile-setting-panel-wrap-->\r\n            <!-- Modal store -->\r\n             <form @submit.prevent=\"postRoles()\">\r\n                <div class=\"modal fade\" id=\"addModal\" tabindex=\"-1\" aria-labelledby=\"reportModalLabel\" aria-hidden=\"true\" >\r\n                    <div class=\"modal-dialog modal-dialog-centered\">\r\n                        <div class=\"modal-content\">\r\n                            <div class=\"modal-header\">\r\n                                <h4 class=\"modal-title\" id=\"reportModalLabel\">Group</h4>\r\n                                <button type=\"button\" class=\"btn-close icon-btn\" data-bs-dismiss=\"modal\" aria-label=\"Close\">\r\n                                    <em class=\"ni ni-cross\"></em>\r\n                                </button>\r\n                            </div>\r\n                            <div class=\"modal-body\">\r\n                               <div class=\"form-floating mb-3 mt-4\">\r\n                                <input type=\"text\" class=\"form-control\" id=\"nama\" placeholder=\"name\"  v-model=\"name\"  required>\r\n                                <label for=\"nama\">Name</label>\r\n                            </div><!-- end form-floating -->  \r\n                                <button class=\"btn btn-dark w-100\" type=\"submit\">Add</button>\r\n                            </div><!-- end modal-body -->\r\n                        </div><!-- end modal-content -->\r\n                    </div><!-- end modal-dialog -->\r\n                </div><!-- end modal-->\r\n            </form>\r\n               <form @submit.prevent=\"putRoles(id)\">\r\n                <div class=\"modal fade\" id=\"editModal\" tabindex=\"-1\" aria-labelledby=\"reportModalLabel\" aria-hidden=\"true\" >\r\n                    <div class=\"modal-dialog modal-dialog-centered\">\r\n                        <div class=\"modal-content\">\r\n                            <div class=\"modal-header\">\r\n                                <h4 class=\"modal-title\" id=\"reportModalLabel\">Update Group</h4>\r\n                                <button type=\"button\" class=\"btn-close icon-btn\" data-bs-dismiss=\"modal\" aria-label=\"Close\">\r\n                                    <em class=\"ni ni-cross\"></em>\r\n                                </button>\r\n                            </div>\r\n                            <div class=\"modal-body\">\r\n                               <div class=\"form-floating mb-3 mt-4\">\r\n                                <input type=\"text\" class=\"form-control\" id=\"nama\" placeholder=\"name\"  v-model=\"name\"  required>\r\n                                <label for=\"nama\">Name</label>\r\n                            </div><!-- end form-floating -->  \r\n                                <button class=\"btn btn-dark w-100\" data-bs-dismiss=\"modal\" type=\"submit\">update</button>\r\n                            </div><!-- end modal-body -->\r\n                        </div><!-- end modal-content -->\r\n                    </div><!-- end modal-dialog -->\r\n                </div><!-- end modal-->\r\n            </form>\r\n        </div><!-- end col-lg-8 -->          \r\n</template>\r\n\r\n<script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\nimport SectionData from '@/store/store.js'\r\nimport Pagination from 'v-pagination-3';\r\nimport axios from 'axios';\r\nimport $ from \"jquery\";\r\nimport Swal from \"sweetalert2\";\r\n// import { reactive } from 'vue';\r\n// import { onMounted, ref } from 'vue';\r\n\r\nexport default {\r\n    components: {\r\n    Pagination\r\n  },\r\n  data () {\r\n    return {\r\n      SectionData,\r\n      page: 1,\r\n      i:1,\r\n      perPage: 6,\r\n      records: [],\r\n      provinsis: [],\r\n      name:[],\r\n      cities:[],\r\n      provinsi:[],\r\n      roles: []\r\n    }\r\n  },\r\n  \r\n  methods: {\r\n    resetnama() {\r\n        this.name=null;\r\n    },\r\n    showPost() {\r\n      Swal.fire({\r\n        position: \"top-end\",\r\n        icon: \"success\",\r\n        title: \"Data telah tersimpan!\",\r\n        showConfirmButton: false,\r\n        timer: 1500,\r\n      });\r\n    },\r\n    showDelete(id) {\r\n      Swal.fire({\r\n        title: \"Apakah anda ingin menghapus data ini?\",\r\n        showDenyButton: true,\r\n        // showCancelButton: true,\r\n        confirmButtonText: \"Iya\",\r\n        denyButtonText: `Tidak`,\r\n      }).then((result) => {\r\n        /* Read more about isConfirmed, isDenied below */\r\n        if (result.isConfirmed) {\r\n          this.deleteRoles(id);\r\n          Swal.fire(\"Tersimpan!\", \"\", \"success\");\r\n        } else if (result.isDenied) {\r\n          Swal.fire(\"Tidak Tersimpan\", \"\", \"info\");\r\n        }\r\n      });\r\n    },\r\n    postRoles(){\r\n            axios.post('http://127.0.0.1:8000/api/role/store', {\r\n                name: this.name,\r\n            }).then(response => {\r\n            this.showPost();    \r\n            this.getRoles()\r\n            this.$toast.success(\"berhasil ditambahkan\");\r\n            console.log(response);\r\n            }).catch(error => {\r\n            this.$toast.error(\"gagal menambahkan data\");\r\n            console.log(error);\r\n            })\r\n            this.name = '';\r\n        },\r\n        getRoles : function(){\r\n            axios.get('http://127.0.0.1:8000/api/role/show')\r\n            .then(function (response) {\r\n                        this.roles = response.data;\r\n                        $(document).ready(function () {\r\n                                        $('#dataTable').DataTable();\r\n                                    });\r\n                    }.bind(this));\r\n        },\r\n        putRoles(id){\r\n            axios.post('http://127.0.0.1:8000/api/role/update/'+id, {\r\n                name: this.name,\r\n            }).then(response => {\r\n            \r\n            this.getRoles()\r\n            this.$toast.show(\"berhasil update\");\r\n            console.log(response);\r\n            }).catch(error => {\r\n            this.$toast.show(\"gagal update\");\r\n            console.log(error);\r\n            })\r\n            this.name = '';\r\n        },\r\n        showRoles (id){\r\n            // alert(id);\r\n            axios.get('http://127.0.0.1:8000/api/role/show/'+id)\r\n            .then(function (response) {\r\n                // alert(response);\r\n                // console.log(response.data.data.id);\r\n                this.id = response.data.data.id;\r\n                this.name = response.data.data.name;\r\n             }.bind(this));\r\n        },\r\n        deleteRoles (id){\r\n    // alert(id);\r\n            axios.delete('http://127.0.0.1:8000/api/role/'+id)\r\n            .then(function () {\r\n                     \r\n                         this.getRoles()\r\n                          this.$toast.show(\"berhasil delete\");\r\n                    }.bind(this));\r\n        },\r\n\r\n// batas\r\n        },\r\n created: function(){\r\n             this.getRoles()\r\n             \r\n  },\r\n  computed: {\r\n      displayedRecords() {\r\n      const startIndex = this.perPage * (this.page - 1);\r\n      const endIndex = startIndex + this.perPage;\r\n      return this.records.slice(startIndex, endIndex);\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n       "]}]}