V,oBAGG,KAHH,eAGG,iBAFFA,oBAA+F,OAA/F,EAA+F;IAAxFI,IAAI,EAAC,MAAmF;IAA5EL,KAAK,EAAC,cAAsE;IAAvDE,EAAE,EAAC,MAAoD;IAA7CiC,WAAW,EAAC,MAAiC;+DAAhBxB,aAAIY,OAAY;IAATS,QAAQ,EAAR;EAAS,CAA/F;;EAAA,kBAA+ErB,aAE7E,EADF6B,WACE,CAHH,CAKG,EAFAjC,0CAEA,EADFkC,WACE,CAZN,CAaE,EADIlC,uCACJ,CApBN,CAqBE,EADIA,0CACJ,CAtBN,CAuBE,EADIA,yCACJ,CAxBN,GAwBMA,kCAzBP;;EAAA,CA2BD,CAzFX,GAyFWA","names":["class","_createElementVNode","id","for","tabindex","type","_hoisted_2","_createCommentVNode","_createElementBlock","_Fragment","_renderList","$data","privilegeData","privilegeTableHead","list","i","scope","key","data","item","_toDisplayString","name","_ctx","$event","_createVNode","_component_Pagination","records","length","onSubmit","$options","_hoisted_14","_hoisted_17","required","value","_hoisted_18","placeholder","_hoisted_20","_hoisted_21","_hoisted_25","_hoisted_28","_hoisted_30","_hoisted_31"],"sourceRoot":"","sources":["E:\\Codeee\\PPLI FE BE\\PPLI\\src\\components\\section\\PrivilageSection.vue"],"sourcesContent":["<template>\r\n   <div class=\"col-lg-9 ps-xl-5\">\r\n                        <div class=\"user-panel-title-box\">\r\n                            <h3>Privilege</h3>\r\n                        </div><!-- end user-panel-title-box -->\r\n                        \r\n                <!-- {{ cities }} -->\r\n                        <!-- <div class=\"d-grid gap-2 d-md-block\">\r\n                            <button @click=\"resetnama()\" type=\"button\" class=\"btn btn-primary btn-sm mb-2\" data-bs-toggle=\"modal\" data-bs-target=\"#addModal\">Add Group</button>\r\n                        </div> -->\r\n                        <div class=\"profile-setting-panel-wrap\">\r\n                            <div class=\"table-responsive\">\r\n                                <table class=\"table mb-0 table-s2\" id=\"dataTable\">\r\n                                    <thead class=\"fs-14\">\r\n                                        <tr>\r\n                                            <th scope=\"col\" v-for=\"(list, i) in SectionData.privilegeData.privilegeTableHead\" :key=\"i\">{{ list }}</th>\r\n                                        </tr>\r\n                                    </thead>\r\n                                    <tbody class=\"fs-13\">\r\n                                        <tr v-for=\"(item) in cities.data\"  :key=\"item.id\">\r\n                                            <td>{{item.name}}</td>\r\n                                            <td class=\"row\">\r\n                                                <input type=\"checkbox\" id=\"checkbox\" v-model=\"checked\" />\r\n                                                <label for=\"checkbox\">{{ checked }}</label>\r\n                                                <!-- <button  @click=\"showCities(item.id)\" class=\"col-sm-1 icon-btn\" title=\"Show\" data-bs-toggle=\"modal\"  data-bs-target=\"#editModal\"><em class=\"fa fa-eye\" ></em></button> -->\r\n                                            </td>\r\n                                        </tr> \r\n                                    </tbody>\r\n                                </table>\r\n                            </div><!-- end table-responsive -->\r\n                            <!-- pagination -->\r\n                            <div class=\"text-center mt-4 mt-md-5\">\r\n                                <Pagination :records=\"records.length\" v-model=\"page\" :per-page=\"perPage\"></Pagination>\r\n                            </div>\r\n                        </div><!-- end profile-setting-panel-wrap-->\r\n            <!-- Modal store -->\r\n             <form @submit.prevent=\"postCities()\">\r\n                <div class=\"modal fade\" id=\"addModal\" tabindex=\"-1\" aria-labelledby=\"reportModalLabel\" aria-hidden=\"true\" >\r\n                    <div class=\"modal-dialog modal-dialog-centered\">\r\n                        <div class=\"modal-content\">\r\n                            <div class=\"modal-header\">\r\n                                <h4 class=\"modal-title\" id=\"reportModalLabel\">Add Provinsi</h4>\r\n                                <button type=\"button\" class=\"btn-close icon-btn\" data-bs-dismiss=\"modal\" aria-label=\"Close\">\r\n                                    <em class=\"ni ni-cross\"></em>\r\n                                </button>\r\n                            </div>\r\n                            <div class=\"modal-body\">\r\n                                 <div class=\"form-group mb-2\">\r\n                                    <label>Provinsi </label>\r\n                                    <select v-model=\"provinsi\" class='form-control' required>\r\n                                        <option  v-for='data in provinsis.data' :value='data.id' :key='data.id' >{{ data.name }}</option>\r\n                                    </select>\r\n                                 </div>\r\n                               <div class=\"form-floating mb-3 mt-4\">\r\n                                <input type=\"text\" class=\"form-control\" id=\"nama\" placeholder=\"name\"  v-model=\"name\"  required>\r\n                                <label for=\"nama\">city</label>\r\n                            </div><!-- end form-floating -->  \r\n                                <button class=\"btn btn-dark w-100\" type=\"submit\">Add</button>\r\n                            </div><!-- end modal-body -->\r\n                        </div><!-- end modal-content -->\r\n                    </div><!-- end modal-dialog -->\r\n                </div><!-- end modal-->\r\n            </form>\r\n               <form @submit.prevent=\"putCities(id)\">\r\n                <div class=\"modal fade\" id=\"editModal\" tabindex=\"-1\" aria-labelledby=\"reportModalLabel\" aria-hidden=\"true\" >\r\n                    <div class=\"modal-dialog modal-dialog-centered\">\r\n                        <div class=\"modal-content\">\r\n                            <div class=\"modal-header\">\r\n                                <h4 class=\"modal-title\" id=\"reportModalLabel\">Update City</h4>\r\n                                <button type=\"button\" class=\"btn-close icon-btn\" data-bs-dismiss=\"modal\" aria-label=\"Close\">\r\n                                    <em class=\"ni ni-cross\"></em>\r\n                                </button>\r\n                            </div>\r\n                            <div class=\"modal-body\">\r\n                                 <div class=\"form-group mb-2\">\r\n                                    <label>Provinsi {{id}} </label>\r\n                                    <select v-model=\"provinsi\" class='form-control' required>\r\n                                        <option  v-for='data in provinsis.data' :value='data.id' :key='data.id' >{{ data.name }}</option>\r\n                                    </select>\r\n                                 </div>\r\n                               <div class=\"form-floating mb-3 mt-4\">\r\n                                <input type=\"text\" class=\"form-control\" id=\"nama\" placeholder=\"name\"  v-model=\"name\"  required>\r\n                                <label for=\"nama\">city</label>\r\n                            </div><!-- end form-floating -->  \r\n                                <button class=\"btn btn-dark w-100\" data-bs-dismiss=\"modal\" type=\"submit\">update</button>\r\n                            </div><!-- end modal-body -->\r\n                        </div><!-- end modal-content -->\r\n                    </div><!-- end modal-dialog -->\r\n                </div><!-- end modal-->\r\n            </form>\r\n        </div><!-- end col-lg-8 -->          \r\n</template>\r\n\r\n<script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\nimport SectionData from '@/store/store.js'\r\nimport Pagination from 'v-pagination-3';\r\nimport axios from 'axios';\r\nimport $ from \"jquery\";\r\n// import { reactive } from 'vue';\r\n// import { onMounted, ref } from 'vue';\r\n\r\nexport default {\r\n    components: {\r\n    Pagination\r\n  },\r\n  data () {\r\n    return {\r\n      SectionData,\r\n      page: 1,\r\n      i:1,\r\n      perPage: 6,\r\n      records: [],\r\n      provinsis: [],\r\n      name:[],\r\n      cities:[],\r\n      provinsi:[]\r\n    }\r\n  },\r\n  \r\n  methods: {\r\n    resetnama() {\r\n        this.name=null;\r\n    },\r\n      postCities(){\r\n            axios.post('http://127.0.0.1:8000/api/cities', {\r\n                name: this.name,\r\n                provinsiId:this.provinsi,\r\n            }).then(response => {\r\n                \r\n            this.getCities()\r\n            this.$toast.show(\"berhasil ditambahkan\");\r\n            console.log(response);\r\n            }).catch(error => {\r\n            this.$toast.show(\"Cities sudah ada\");\r\n            console.log(error);\r\n            })\r\n            this.name = '';\r\n            this.provinsi='';\r\n        },\r\n        deleteCities (id){\r\n    // alert(id);\r\n            axios.delete('http://127.0.0.1:8000/api/cities/'+id)\r\n            .then(function () {\r\n                     \r\n                         this.getCities()\r\n                          this.$toast.show(\"berhasil delete\");\r\n                    }.bind(this));\r\n        },\r\n       putCities(id){\r\n            axios.post('http://127.0.0.1:8000/api/cities/'+id, {\r\n                name: this.name,\r\n                provinsiId:this.provinsi,\r\n            }).then(response => {\r\n                \r\n            this.getCities()\r\n            this.$toast.show(\"berhasil update\");\r\n            console.log(response);\r\n            }).catch(error => {\r\n            this.$toast.show(\"gagal update\");\r\n            console.log(error);\r\n            })\r\n            this.name = '';\r\n            this.provinsi='';\r\n        },\r\n       \r\n         getProvinsis : function(){\r\n            axios.get('http://127.0.0.1:8000/api/provinsi')\r\n            .then(function (response) {\r\n                        this.provinsis = response.data;\r\n                    }.bind(this));\r\n        },\r\n          showCities (){\r\n            // alert(id);\r\n            axios.get('http://127.0.0.1:8000/api/cities/9')\r\n            .then(function (response) {\r\n                // alert(response);\r\n                // console.log(response.data.data.id);\r\n                this.id = response.data.data.id;\r\n                this.provinsi = response.data.data.provinsi.id;\r\n                this.name = response.data.data.name;\r\n             }.bind(this));\r\n        },\r\n          getCities : function(){\r\n            axios.get('http://127.0.0.1:8000/api/cities')\r\n            .then(function (response) {\r\n                        this.cities = response.data;\r\n                        $(document).ready(function () {\r\n                                        $('#dataTable').DataTable();\r\n                                    });\r\n                    }.bind(this));\r\n        },\r\n        },\r\n created: function(){\r\n            this.getProvinsis()\r\n             this.getCities()\r\n             \r\n  },\r\n  computed: {\r\n      displayedRecords() {\r\n      const startIndex = this.perPage * (this.page - 1);\r\n      const endIndex = startIndex + this.perPage;\r\n      return this.records.slice(startIndex, endIndex);\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n       "]}]}