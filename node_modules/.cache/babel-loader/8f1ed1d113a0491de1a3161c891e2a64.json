g :src=\"previewimg\" v-if=\"previewimg\" class=\"mb-2\" width=\"200\"  />\r\n        <input type=\"file\" class=\"form-control\" id=\"logo\" @change=\"upload\" />\r\n      </div>\r\n      {{ gambar }}\r\n      <!-- {{}} -->\r\n      <div class=\"form-floating mb-3\">\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          id=\"NamaPerushaan\"\r\n          placeholder=\"Nama Perusahaan\"\r\n          v-model=\"NamaPerushaan\"\r\n        />\r\n        <label for=\"NamaPerushaan\">Nama Perusahaan</label>\r\n      </div>\r\n\r\n      <!-- end form-floating -->\r\n      <div class=\"form-floating mb-3\">\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          id=\"name\"\r\n          v-model=\"name\"\r\n          placeholder=\"Name\"\r\n        />\r\n        <label for=\"name\">Name</label>\r\n      </div>\r\n      <!-- end form-floating -->\r\n      <div class=\"form-floating mb-3\">\r\n        <input\r\n          type=\"number\"\r\n          class=\"form-control\"\r\n          id=\"nohp\"\r\n          placeholder=\"Nomor Handphone\"\r\n          v-model=\"PhoneNumber\"\r\n        />\r\n        <label for=\"nohp\">Nomor Handphone</label>\r\n      </div>\r\n      <!-- end form-floating -->\r\n      <div class=\"form-floating mb-3\">\r\n        <input\r\n          type=\"email\"\r\n          class=\"form-control\"\r\n          id=\"email\"\r\n          placeholder=\"Email\"\r\n          v-model=\"email\"\r\n        />\r\n        <label for=\"email\">Email</label>\r\n      </div>\r\n      <!-- end form-floating -->\r\n      <div class=\"form-floating mb-3\">\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          id=\"alamat\"\r\n          placeholder=\"Alamat\"\r\n          v-model=\"alamat\"\r\n        />\r\n        <label for=\"alamat\">Alamat</label>\r\n      </div>\r\n      <!-- end form-floating -->\r\n      <div class=\"form-floating mb-3\">\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          id=\"BentukBadanUsaha\"\r\n          placeholder=\"Bentuk Badan Usaha\"\r\n          v-model=\"BentukBadanUsaha\"\r\n        />\r\n        <label for=\"BentukBadanUsaha\">Bentuk Badan Usaha</label>\r\n      </div>\r\n      <!-- end form-floating -->\r\n      <label for=\"status\">Status</label>\r\n      <!-- <div class=\"form-select mb-3\"> -->\r\n      <select class=\"form-select mb-3\" v-model=\"status\">\r\n        <option value=\"aktif\">Aktif</option>\r\n        <option value=\"tidak aktif\">Tidak Aktif</option>\r\n      </select>\r\n      <!-- {{status}} -->\r\n      <!-- </div> -->\r\n      <router-link to=\"/member\" type=\"button\" class=\"btn btn-sm btn-dark\"\r\n        >Kembali</router-link\r\n      >\r\n      <button class=\"btn btn-sm btn-success w-20 ms-2\" type=\"submit\">\r\n        Simpan\r\n      </button>\r\n    </form>\r\n  </div>\r\n  <!-- end col-lg-8 -->\r\n</template>\r\n\r\n<script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\nimport SectionData from \"@/store/store.js\";\r\nimport axios from \"axios\";\r\n// import $ from \"jquery\";\r\n// import Swal from \"sweetalert2\";\r\n\r\nexport default {\r\n  name: \"AccountSection\",\r\n  data() {\r\n    return {\r\n      SectionData,\r\n      member: [],\r\n      tanggal_bayar: \"\",\r\n      status: \"\",\r\n      gambar: [],\r\n      NamaPerushaan: \"\",\r\n      PhoneNumber: \"\",\r\n      alamat: \"\",\r\n      name: \"\",\r\n      BentukBadanUsaha: \"\",\r\n      email: \"\",\r\n      previewimg: \"\",\r\n      file:[]\r\n    };\r\n  },\r\n  created() {\r\n    this.getMember();\r\n  },\r\n  methods: {\r\n    getMember() {\r\n      axios\r\n        .get(`http://127.0.0.1:8000/api/member/show/${this.$route.params.id}`)\r\n        .then((res) => {\r\n          this.NamaPerushaan = res.data.data.NamaPerushaan;\r\n          this.gambar = res.data.data.gambar;\r\n          this.name = res.data.data.name;\r\n          this.PhoneNumber = res.data.data.PhoneNumber;\r\n          this.email = res.data.data.email;\r\n          this.alamat = res.data.data.alamat;\r\n          this.BentukBadanUsaha = res.data.data.BentukBadanUsaha;\r\n          this.status = res.data.data.status;\r\n        });\r\n    },\r\n    updateMember() {\r\n      axios\r\n        .post(\r\n          `http://127.0.0.1:8000/api/member/update/${this.$route.params.id}`,\r\n          {\r\n             NamaPerushaan: this.NamaPerushaan,\r\n             gambar: this.gambar,\r\n             name: this.name,\r\n             PhoneNumber: this.PhoneNumber,\r\n             email: this.email,\r\n             alamat: this.alamat,\r\n             BentukBadanUsaha: this.BentukBadanUsaha,\r\n             status: this.status,\r\n          }\r\n        )\r\n        .then((res) => {\r\n          this.$router.push({ name: \"member\" });\r\n          this.$toast.success(\"Member berhasil di update\");\r\n          console.log(res);\r\n        });\r\n    },\r\n    upload(event) {\r\n      this.gambar = event.target.files[0];\r\n      this.previewimg = URL.createObjectURL(event.target.files[0]);\r\n      console.log(this.gambar)\r\n    },\r\n  },\r\n  mounted() {\r\n    /* =========== Show/Hide passoword ============== */\r\n    function showHidePassword(selector) {\r\n      let elem = document.querySelectorAll(selector);\r\n      if (elem.length > 0) {\r\n        elem.forEach((item) => {\r\n          item.addEventListener(\"click\", function (e) {\r\n            e.preventDefault();\r\n            let target = document.getElementById(item.getAttribute(\"href\"));\r\n            if (target.type == \"password\") {\r\n              target.type = \"text\";\r\n              item.classList.add(\"is-shown\");\r\n            } else {\r\n              target.type = \"password\";\r\n              item.classList.remove(\"is-shown\");\r\n            }\r\n          });\r\n        });\r\n      }\r\n    }\r\n\r\n    showHidePassword(\".password-toggle\");\r\n  },\r\n};\r\n</script>\r\n"]}]}