AAc,CAACC,SAAD,EAAY;MAC7B,MAAMC,UAAS,GAAIC,YAAY,CAACC,OAAb,CAAqB,YAArB,CAAnB;MACA,IAAIC,MAAK,GAAI,KAAb;MACAC,IAAI,CAACC,KAAL,CAAWL,UAAX,EAAuBM,OAAvB,CAAgC/B,IAAD,IAAU;QACvC,IAAIA,IAAG,KAAMwB,SAAb,EAAwB;UACtBI,MAAK,GAAI,IAAT;QACF;MACD,CAJD;MAKA,OAAOA,MAAP;IACD;;EA/BM,CAfI;EAgDdI,OAAO,EAAE,YAAU;IACP,KAAKvB,WAAL;EAEV,CAnDY;EAoDbwB,QAAQ,EAAE;IACNC,gBAAgB,GAAG;MACnB,MAAMC,UAAS,GAAI,KAAKhC,OAAL,IAAgB,KAAKF,IAAL,GAAY,CAA5B,CAAnB;MACA,MAAMmC,QAAO,GAAID,UAAS,GAAI,KAAKhC,OAAnC;MACA,OAAO,KAAKC,OAAL,CAAaiC,KAAb,CAAmBF,UAAnB,EAA+BC,QAA/B,CAAP;IACF;;EALQ;AApDG,CAAf","names":["SectionData","Pagination","axios","$","components","data","page","i","perPage","records","kegiatan","methods","resetnama","name","getKegiatan","get","then","response","document","ready","DataTable","bind","deleteKegiatan","id","confirm","delete","$toast","error","checkPrivilege","privilege","permission","localStorage","getItem","status","JSON","parse","forEach","created","computed","displayedRecords","startIndex","endIndex","slice"],"sourceRoot":"","sources":["E:\\Codeee\\PPLI FE BE\\PPLI\\src\\components\\section\\KegiatanSection.vue"],"sourcesContent":["<template>\r\n   <div class=\"col-lg-9 ps-xl-5\">\r\n                        <div class=\"user-panel-title-box\">\r\n                            <h3>Jadwal Acara</h3>\r\n                        </div><!-- end user-panel-title-box -->\r\n                        \r\n                <!-- {{ kontak }} -->\r\n                        <div class=\"row\">\r\n                            <div class=\"col d-grid gap-2 d-md-block\" v-if=\"checkPrivilege('provinsi-store')\">\r\n                                <router-link to=\"/add-kegiatan\" type=\"button\" class=\"btn btn-dark btn-sm mb-2\">Tambah Kegiatan</router-link>\r\n                                <!-- <button type=\"button\" class=\"btn btn-primary btn-sm mb-2 ms-2\">Card Name</button>     -->\r\n                            </div>\r\n                            <!-- <div class=\"col\">\r\n                                <router-link to=\"\" type=\"button\" class=\"btn btn-dark btn-sm\">List Daftar Kegiatan</router-link>\r\n                            </div> -->\r\n                        </div>\r\n                        <!-- {{ kegiatan }} -->\r\n                        <div class=\"profile-setting-panel-wrap\">\r\n                            <div class=\"table-responsive\">\r\n                                <table class=\"table mb-0 table-s2\" id=\"dataTable\">\r\n                                    <thead class=\"fs-14\">\r\n                                        <tr>\r\n                                            <th scope=\"col\" v-for=\"(list, i) in SectionData.kegiatanData.kegiatanTableHead\" :key=\"i\">{{ list }}</th>\r\n                                        </tr>\r\n                                    </thead>\r\n                                    <tbody class=\"fs-13\">\r\n                                        <tr v-for=\"(item,index) in kegiatan\"  :key=\"item.id\">\r\n                                             <td scope=\"row\"  ><a href=\"#\">{{index+1}}</a>  </td> \r\n                                            <td>{{item.nama_kegiatan}}</td>\r\n                                            <td>{{item.sifat_kegiatan}}</td>\r\n                                            <td>{{item.lokasi_kegiatan}}</td>\r\n                                            <td>{{item.tanggal_rencana}}</td>\r\n                                            <td>{{item.tanggal_realisasi}}</td>\r\n                                            <td>{{item.keterangan}}</td>\r\n                                            <td class=\"row\">\r\n                                                <!-- <router-link :to=\"{name: 'show-contact', params: { id: item.id }}\" class=\"col- icon-btn  p-0 m-0\" title=\"Show\"><em class=\"fa fa-eye\" ></em></router-link> -->\r\n                                                <!-- <button  @click=\"showCities(item.id)\" class=\"col-sm-1 icon-btn \" title=\"Show\" data-bs-toggle=\"modal\"  data-bs-target=\"#editModal\"><em class=\"fa fa-eye\" ></em></button> -->\r\n                                                <router-link :to=\"{name: 'edit-kegiatan', params: { id: item.id }}\" class=\"col- p-0 m-0 icon-btn \" title=\"Edit\"><em class=\"fa fa-pencil-square-o\" ></em></router-link>\r\n                                                <button  @click=\"deleteKegiatan(item.id)\" class=\"col- p-0 m-0 icon-btn \" title=\"Delete\" ><em class=\"ni ni-trash\"></em></button>\r\n                                            </td>\r\n                                        </tr> \r\n                                    </tbody>\r\n                                </table>\r\n                            </div><!-- end table-responsive -->\r\n                            <!-- pagination -->\r\n                            <div class=\"text-center mt-4 mt-md-5\">\r\n                                <Pagination :records=\"records.length\" v-model=\"page\" :per-page=\"perPage\"></Pagination>\r\n                            </div>\r\n                        </div><!-- end profile-setting-panel-wrap-->\r\n        </div><!-- end col-lg-8 -->          \r\n</template>\r\n\r\n<script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\nimport SectionData from '@/store/store.js'\r\nimport Pagination from 'v-pagination-3';\r\nimport axios from 'axios';\r\nimport $ from \"jquery\";\r\n// import { reactive } from 'vue';\r\n// import { onMounted, ref } from 'vue';\r\n\r\nexport default {\r\n    components: {\r\n    Pagination\r\n  },\r\n  data () {\r\n    return {\r\n      SectionData,\r\n      page: 1,\r\n      i:1,\r\n      perPage: 6,\r\n      records: [],\r\n      kegiatan: [],\r\n    }\r\n  },\r\n  \r\n  methods: {\r\n    resetnama() {\r\n        this.name=null;\r\n    },\r\n            getKegiatan(){\r\n            axios.get('http://127.0.0.1:8000/api/kegiatan')\r\n            .then(function(response){\r\n                this.kegiatan = response.data;\r\n                 $(document).ready(function () {\r\n                $('#dataTable').DataTable();\r\n            });\r\n            }.bind(this));\r\n        },\r\n        deleteKegiatan (id){\r\n            if(confirm(\"Apakah Kamu Yakin?\")){\r\n        axios.delete('http://127.0.0.1:8000/api/kegiatan/'+id)\r\n        .then(function () {\r\n                this.getKegiatan()\r\n                this.$toast.error(\"Berhasil delete\");\r\n        }.bind(this));\r\n            }\r\n        },\r\n         checkPrivilege(privilege) {\r\n      const permission = localStorage.getItem(\"permission\");\r\n      let status = false;\r\n      JSON.parse(permission).forEach((data) => {\r\n        if (data === privilege) {\r\n          status = true;\r\n        }\r\n      });\r\n      return status;\r\n    },\r\n        },\r\n created: function(){\r\n             this.getKegiatan()\r\n             \r\n  },\r\n  computed: {\r\n      displayedRecords() {\r\n      const startIndex = this.perPage * (this.page - 1);\r\n      const endIndex = startIndex + this.perPage;\r\n      return this.records.slice(startIndex, endIndex);\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n       "]}]}