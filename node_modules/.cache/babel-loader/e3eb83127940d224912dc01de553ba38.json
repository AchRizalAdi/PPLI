isplayString","$data","provinsisData","mainTitle","_createCommentVNode","onClick","$options","_createElementBlock","_Fragment","_renderList","provinsisTableHead","list","i","key","data","item","name","title","_createVNode","_component_Pagination","records","length","$event","onSubmit","_hoisted_22","placeholder","required","_hoisted_25","_hoisted_26","_ctx","_hoisted_30","_hoisted_33"],"sourceRoot":"","sources":["E:\\Codeee\\PPLI FE BE\\PPLI\\src\\components\\section\\ProvSection.vue"],"sourcesContent":["<template>\r\n   <div class=\"col-lg-9 ps-xl-5\">\r\n                        <div class=\"user-panel-title-box\">\r\n                            <h3>{{SectionData.provinsisData.mainTitle }}</h3>\r\n                        </div><!-- end user-panel-title-box -->\r\n                        \r\n                <!-- {{ provinsi }} -->\r\n                        <div class=\"d-grid gap-2 d-md-block\">\r\n                            <button @click=\"resetnama()\" type=\"button\" class=\"btn btn-primary btn-sm mb-2\" data-bs-toggle=\"modal\" data-bs-target=\"#messageModal\">Add Provinsi</button>\r\n                        </div>\r\n                        \r\n                        <div class=\"profile-setting-panel-wrap\">\r\n                            <div class=\"table-responsive\">\r\n                                <table class=\"table mb-0 table-s2\" id=\"dataTable\">\r\n                                    <thead class=\"fs-14\">\r\n                                        <tr>\r\n                                            <th scope=\"col\" v-for=\"(list, i) in SectionData.provinsisData.provinsisTableHead\" :key=\"i\">{{ list }}</th>\r\n                                        </tr>\r\n                                    </thead>\r\n                                    <tbody class=\"fs-13\">\r\n                                        <tr v-for=\"item in provinsis.data\"  :key=\"item.id\">\r\n                                            <td scope=\"row\"  ><a href=\"#\">{{ item.id }}</a>  </td>\r\n                                            <td>{{ item.name }}</td>\r\n                                            <td class=\"row\">\r\n                                                <button  @click=\"showProvinsi(item.id)\" class=\"col-sm-1 icon-btn ms-auto\" title=\"Edit\" data-bs-toggle=\"modal\"  data-bs-target=\"#updateModal\"><em class=\"fa fa-pencil-square-o\" ></em></button>\r\n                                                <button  @click=\"deleteProvinsi(item.id)\" class=\"col-sm icon-btn ms-auto\" title=\"Delete\" ><em class=\"ni ni-trash\"></em></button></td>\r\n                                        </tr>\r\n                                    </tbody>\r\n                                </table>\r\n                            </div><!-- end table-responsive -->\r\n                            <!-- pagination -->\r\n                            <div class=\"text-center mt-4 mt-md-5\">\r\n                                <Pagination :records=\"records.length\" v-model=\"page\" :per-page=\"perPage\"></Pagination>\r\n                            </div>\r\n                        </div><!-- end profile-setting-panel-wrap-->\r\n            <!-- Modal store -->\r\n             <form @submit.prevent=\"postProvinsis()\">\r\n                <div class=\"modal fade\" id=\"messageModal\" tabindex=\"-1\" aria-labelledby=\"reportModalLabel\" aria-hidden=\"true\" >\r\n                    <div class=\"modal-dialog modal-dialog-centered\">\r\n                        <div class=\"modal-content\">\r\n                            <div class=\"modal-header\">\r\n                                <h4 class=\"modal-title\" id=\"reportModalLabel\">Add Provinsi</h4>\r\n                                <button type=\"button\" class=\"btn-close icon-btn\" data-bs-dismiss=\"modal\" aria-label=\"Close\">\r\n                                    <em class=\"ni ni-cross\"></em>\r\n                                </button>\r\n                            </div>\r\n                            <div class=\"modal-body\">\r\n                               <div class=\"form-floating mb-3\">\r\n                                <input type=\"text\" class=\"form-control\" id=\"nama\" placeholder=\"name\"  v-model=\"name\"  required>\r\n                                <label for=\"nama\">Nama</label>\r\n                            </div><!-- end form-floating -->  \r\n                                <button class=\"btn btn-dark w-100\" type=\"submit\">Add</button>\r\n                            </div><!-- end modal-body -->\r\n                        </div><!-- end modal-content -->\r\n                    </div><!-- end modal-dialog -->\r\n                </div><!-- end modal-->\r\n            </form>\r\n              <form @submit.prevent=\"putProvinsis(edit)\">\r\n                <div class=\"modal fade\" id=\"updateModal\" tabindex=\"-1\" aria-labelledby=\"reportModalLabel\" aria-hidden=\"true\" >\r\n                    <div class=\"modal-dialog modal-dialog-centered\">\r\n                        <div class=\"modal-content\">\r\n                            <div class=\"modal-header\">\r\n                                <h4 class=\"modal-title\" id=\"reportModalLabel\">update provinsi</h4>\r\n                                <button type=\"button\" class=\"btn-close icon-btn\" data-bs-dismiss=\"modal\" aria-label=\"Close\">\r\n                                    <em class=\"ni ni-cross\"></em>\r\n                                </button>\r\n                            </div>\r\n                            <div class=\"modal-body\">\r\n                               <div class=\"form-floating mb-3\" >\r\n                                <input type=\"text\"  class=\"form-control\" id=\"namaedit\" placeholder=\"dfghjk\"  v-model=\"name\"  required>\r\n                                <!-- <label for=\"nama\">{{edit.data.name}}</label> -->\r\n                            </div><!-- end form-floating -->  \r\n                                <button class=\"btn btn-dark w-100\" type=\"submit\">update</button>\r\n                            </div><!-- end modal-body -->\r\n                        </div><!-- end modal-content -->\r\n                    </div><!-- end modal-dialog -->\r\n                </div><!-- end modal-->\r\n            </form>\r\n        </div><!-- end col-lg-8 -->          \r\n</template>\r\n\r\n<script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\nimport SectionData from '@/store/store.js'\r\nimport Pagination from 'v-pagination-3';\r\nimport axios from 'axios';\r\nimport $ from \"jquery\";\r\n// import { reactive } from 'vue';\r\n// import { onMounted, ref } from 'vue';\r\n\r\nexport default {\r\n    components: {\r\n    Pagination\r\n  },\r\n  data () {\r\n    return {\r\n      SectionData,\r\n      page: 1,\r\n      perPage: 6,\r\n      records: [],\r\n      provinsis: [],\r\n    //   edit:'',\r\n      name: '',\r\n    }\r\n  },\r\n  \r\n  methods: {\r\n    resetnama() {\r\n        this.name=null;\r\n    },    \r\n    getProvinsis : function(){\r\n            axios.get('http://127.0.0.1:8000/api/provinsi')\r\n            .then(function (response) {\r\n                        this.provinsis = response.data;\r\n                         $(document).ready(function () {\r\n                                        $('#dataTable').DataTable();\r\n                                    });\r\n                    }.bind(this));\r\n  },\r\n  deleteProvinsi (id){\r\n    // alert(id);\r\n            axios.delete('http://127.0.0.1:8000/api/provinsi/'+id)\r\n            .then(function () {\r\n                        alert('delete succes');\r\n                         this.getProvinsis()\r\n                    }.bind(this));\r\n  },\r\n  showProvinsi (id){\r\n    // alert(id);\r\n            axios.get('http://127.0.0.1:8000/api/provinsi/'+id)\r\n            .then(function (response) {\r\n                this.edit = response.data.data.id;\r\n                this.name=response.data.data.name;\r\n             }.bind(this));\r\n    },\r\n  \r\n    putProvinsis(id){\r\n        // alert(id);\r\n        axios.post('http://127.0.0.1:8000/api/provinsi/'+id,{\r\n             name: this.name\r\n             }).then(function () {\r\n             this.getProvinsis()\r\n             }.bind(this));\r\n    },\r\n        postProvinsis(){\r\n            axios.post('http://127.0.0.1:8000/api/provinsi', {\r\n                name: this.name\r\n            }).then(response => {\r\n                \r\n            this.getProvinsis()\r\n            alert('provinsi berhasil ditambahkan');\r\n            console.log(response);\r\n            }).catch(error => {\r\n            alert('provinsi sudah ada');\r\n            console.log(error);\r\n            })\r\n            this.name = '';\r\n        }\r\n   },\r\n        created: function(){\r\n            this.getProvinsis()\r\n  },\r\n  computed: {\r\n      displayedRecords() {\r\n      const startIndex = this.perPage * (this.page - 1);\r\n      const endIndex = startIndex + this.perPage;\r\n      return this.records.slice(startIndex, endIndex);\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n        //   setup(){\r\n        //         const provinsi = reactive({\r\n        //             name : '',\r\n        //         });\r\n        \r\n        //         function store() {\r\n        //             axios.post(\r\n        //                 'http://127.0.0.1:8000/api/provinsi',\r\n        //                 provinsi\r\n        //             )\r\n        //             .then((response)=> {\r\n        //                 console.log(response);\r\n        //             }).catch((err) => {\r\n        //                 console.log(err);\r\n        //         });\r\n        \r\n        \r\n        //         }\r\n                \r\n        //         return {\r\n        //             provinsi,\r\n        //             store\r\n        //         }\r\n        //     },"]}]}