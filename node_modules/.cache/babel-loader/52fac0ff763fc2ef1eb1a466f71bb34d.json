AK,EAAC,sBAkBL;IAlB6BM,UAAQ,yCAAEC,6CAAF;EAkBrC,CAlBN,GACIC,oCACAA,gDACAA,qCACAP,oBAaK,IAbL,cAaK,CAZCO,8LAYD,EAXCP,oBASK,IATL,cASK,CARDQ,UAQC,EAPDR,oBAMK,IANL,cAMK,CALDA,oBAAyD,IAAzD,EAAyD,IAAzD,EAAyD,CAArDA,oBAAgD,IAAhD,cAAgDS,iBAAjBC,WAAKC,IAAY,CAAhD,EAAwC;EAAA;EAAxC,CAAqD,CAAzD,CAKC,qBAJDC,oBAA6MC,SAA7M,EAA6M,IAA7M,EAA6MC,YAA1LJ,kBAAYK,SAA8K,EAAlMC,IAA6B,IAAzB;yBAAfJ,oBAA6M,IAA7M,EAA6M;MAAlKK,GAAG,EAAED,IAAI,CAACE;IAAwJ,CAA7M,GAAyDC,aAA+IC,sBAA/I,EAA+I;MAAlIrB,KAAK,EAAC,iCAA4H;MAAzFsB,EAAE,EAAEL,IAAI,CAACM;IAAgF,CAA/I;wBAAqE,MAA4C,CAA5CtB,oBAA4C,IAA5C,EAA4C;QAAxCD,KAAK,mBAAC,SAAD,EAAmBiB,IAAI,CAACO,IAAxB;MAAmC,CAA5C;;MAAA,CAA4C,oCAAGP,IAAI,CAACb,QAAK;MAAA;OAAb;;;;KAAjH;;IAAA,UAAzD;GAA6M,CAA7M;;EAAA,CAIC,GAHDqB,UAGC,EAFDC,UAEC,EADDzB,oBAA2H,IAA3H,EAA2H,IAA3H,EAA2H,CAAvHA,oBAAkH,GAAlH,EAAkH;IAA/GD,KAAK,EAAC,iCAAyG;IAAvEG,IAAI,EAAC,GAAkE;IAA7DwB,OAAK,sCAAEpB,iBAAF;EAAwD,CAAlH,cAAuH,CAA3H,CACC,CANL,CAOC,CATL,CAWD,EADDC,uLACC,CAbL,EAJJ;;EAAA,GAkBMA","names":["class","_createElementVNode","type","href","title","_hoisted_8","onLoggedIn","$options","_createCommentVNode","_hoisted_3","_toDisplayString","$data","name","_createElementBlock","_Fragment","_renderList","authorNav","list","key","id","_createVNode","_component_router_link","to","path","icon","_hoisted_6","_hoisted_7","onClick"],"sourceRoot":"","sources":["E:\\Codeee\\PPLI FE BE\\PPLI\\src\\components\\common\\MenuTwo.vue"],"sourcesContent":["<template>\r\n  <nav class=\"header-menu menu nav\" @loggedIn=\"setUser\">\r\n      <!-- menu list -->\r\n      <!-- <MenuList></MenuList> -->\r\n      <!-- header btn -->\r\n      <ul class=\"menu-btns menu-btns-2 ms-lg-auto \"> \r\n            <!-- <li class=\"d-none d-lg-inline-block\"><router-link to=\"wallet\" class=\"icon-btn icon-btn-s1\" title=\"Wallet\"><em class=\"ni ni-wallet\"></em></router-link></li> -->\r\n            <li class=\"d-none d-lg-inline-block dropdown\">\r\n                <button type=\"button\" class=\"icon-btn icon-btn-s1\" data-bs-toggle=\"dropdown\"><em class=\"ni ni-user\"></em></button>\r\n                <ul class=\"dropdown-menu card-generic card-generic-s3 dropdown-menu-end mt-2\">\r\n                    <li><h6 class=\"dropdown-header\">{{ data.name }}</h6></li>\r\n                    <li v-for=\"list in SectionData.authorNav\" :key=\"list.id\"><router-link class=\"dropdown-item card-generic-item\" :to=\"list.path\"><em class=\"ni me-2\" :class=\"list.icon\"></em>{{ list.title }}</router-link></li>\r\n                    <li><a href=\"#\" class=\"dropdown-item card-generic-item theme-toggler\" title=\"Toggle Dark/Light mode\"><em class=\"ni ni-moon me-2\"></em> Dark Mode</a></li>\r\n                    <li><hr class=\"dropdown-divider\"></li>\r\n                    <li><a class=\"dropdown-item card-generic-item\" href=\"#\" @click=\"logout()\"><em class=\"ni ni-power me-2\"></em>Logout</a></li>\r\n                </ul>\r\n            </li>\r\n          <!-- <li class=\"d-lg-none\"><ButtonLink :text=\"SectionData.headerData.btnText\" link=\"/wallet\" classname=\"btn btn-lg\" :class=\"classname\"></ButtonLink></li> -->\r\n      </ul>\r\n  </nav><!-- .header-menu -->\r\n</template>\r\n\r\n<script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\nimport SectionData from '@/store/store.js'\r\nimport axios from 'axios'\r\n\r\n// @ is an alias to /src\r\n// import MenuList from '@/components/common/MenuList.vue'\r\n\r\nexport default {\r\n  name: 'MenuTwo',\r\n  props: ['classname'],\r\n  // components: {\r\n  //   MenuList\r\n  // },\r\n  data () {\r\n    return {\r\n      SectionData,\r\n      data: '',\r\n      // loginType: ''\r\n      user: null,\r\n      isLoggedIn: false\r\n    }\r\n  },\r\n  created() {\r\n      axios.defaults.headers.common['Content-Type'] = 'application/json'\r\n      axios.defaults.headers.common['Authorization'] = 'Bearer ' + localStorage.getItem('token') \r\n      // axios.defaults.headers.common['Permission'] = 'Bearer ' + localStorage.getItem('permission')\r\n      \r\n\r\n      axios.get(`http://127.0.0.1:8000/api/me`)\r\n        .then(response => {\r\n          this.data = response.data\r\n          // this.loginType = response.data.roles[0].name\r\n        })\r\n        .catch(error => {\r\n          if (error.response.status === 401) {\r\n            localStorage.clear();\r\n            \r\n            this.$router.push('/login')\r\n          }\r\n          console.error(error);\r\n        })\r\n    },\r\n  mounted () {\r\n    this.setUser()\r\n    /*  ==========================================\r\n      Dark/Light mode configaration\r\n    ========================================== */\r\n    function themeSwitcher (selector) {\r\n      let themeToggler = document.querySelectorAll(selector)\r\n      if (themeToggler.length > 0) {\r\n        themeToggler.forEach(item => {\r\n          item.addEventListener('click', function (e) {\r\n            e.preventDefault()\r\n            document.body.classList.toggle('dark-mode')\r\n            if (document.body.classList.contains('dark-mode')) {\r\n              localStorage.setItem('website_theme', 'dark-mode')\r\n            } else {\r\n              localStorage.setItem('website_theme', 'default')\r\n            }\r\n          })\r\n        })\r\n      }\r\n\r\n      function retrieveTheme () {\r\n        var theme = localStorage.getItem('website_theme')\r\n        if (theme != null) {\r\n          document.body.classList.remove('default', 'dark-mode')\r\n          document.body.classList.add(theme)\r\n        }\r\n      }\r\n\r\n      retrieveTheme()\r\n\r\n      if (window) {\r\n        window.addEventListener('storage', function () {\r\n          retrieveTheme()\r\n        }, false)\r\n      }\r\n    }\r\n\r\n    themeSwitcher('.theme-toggler')\r\n  },\r\n  methods: {\r\n      setUser() {\r\n          this.user = JSON.parse(localStorage.getItem('user'))\r\n          this.isLoggedIn = localStorage.getItem('token') != null\r\n\r\n        },\r\n        logout() {\r\n          localStorage.removeItem('token')\r\n          localStorage.removeItem('user')\r\n          localStorage.removeItem('permission')\r\n          // localStorage.clear();\r\n          this.setUser()\r\n\r\n          this.$router.push('/login')\r\n        }\r\n    }\r\n}\r\n</script>\r\n"]}]}