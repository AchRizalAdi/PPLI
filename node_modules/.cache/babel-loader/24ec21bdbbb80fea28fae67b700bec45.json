rEach","created","mounted","uploadImage","selector","elem","querySelectorAll","length","item","addEventListener","files","img","getElementById","dataset","target","onload","URL","revokeObjectURL","src","createObjectURL","allowedExtensions","fileExtension","value","split","pop","lastDot","lastIndexOf","ext","substring","extTxt","includes","alert","showHidePassword","e","preventDefault","getAttribute","type","classList","add","remove"],"sourceRoot":"","sources":["E:\\Codeee\\PPLI FE BE\\PPLI\\src\\components\\section\\PengaturanSection.vue"],"sourcesContent":["<template>\r\n  <div class=\"col-lg-9 ps-xl-5\">\r\n        <div class=\"user-panel-title-box\">\r\n            <h3>{{SectionData.pengaturanProfileData.title }}</h3>\r\n        </div><!-- end user-panel-title-box -->\r\n        <div class=\"profile-setting-panel-wrap\">\r\n            <ul class=\"nav nav-tabs nav-tabs-s1 nav-tabs-mobile-size\" id=\"myTab\" role=\"tablist\">\r\n                <li class=\"nav-item\" role=\"presentation\" v-for=\"list in SectionData.pengaturanProfileData.pengaturanProfileTabNav\" :key=\"list.id\">\r\n                    <button class=\"nav-link\" :class=\"list.isActive\" :id=\"list.slug\" data-bs-toggle=\"tab\" :data-bs-target=\"list.bsTarget\" type=\"button\">{{ list.title }}</button>\r\n                </li>\r\n            </ul>\r\n            <div class=\"tab-content mt-4\" id=\"myTabContent\">\r\n                <div class=\"tab-pane fade show active\" id=\"pengaturan-umum\" role=\"tabpanel\" aria-labelledby=\"pengaturan-umum-tab\">\r\n                    <div class=\"profile-setting-panel\">\r\n                        <!-- <h5 class=\"mb-4\">Umum</h5> -->\r\n                        \r\n                        <section :v-if=\"item.if\" class=\"category-section\" :class=\"classname\">\r\n                                    <div class=\"container mb-5\">\r\n                                        <!-- section heading -->\r\n                                        <SectionHeading classname=\"\" :text=\"SectionData.settingUmumData.title\" :content=\"SectionData.settingUmumData.content\" isMargin=\"\"></SectionHeading>\r\n                                        <div class=\"row g-gs\">\r\n                                            <div class=\"col-lg-3 col-6\" v-for=\"item in SectionData.settingUmumData.settingUmumList\" :key=\"item.id\">\r\n                                                <router-link :to=\"item.path\" class=\"card card-cat text-center h-100\" :class=\"item.class\">\r\n                                                    <div class=\"card-body card-body-s1\">\r\n                                                        <span :class=\"item.icon\"></span>\r\n                                                        <h5 class=\"card-cat-title\">{{ item.title }}</h5>\r\n                                                    </div>\r\n                                                </router-link>\r\n                                            </div>\r\n                                        </div>\r\n                                    </div>\r\n                                </section>\r\n                               \r\n                                <section class=\"category-section\" :class=\"classname\">\r\n                                    <div class=\"container\">\r\n                                        <!-- section heading -->\r\n                                        <SectionHeading classname=\"\" :text=\"SectionData.settingOrgData.title\" :content=\"SectionData.settingOrgData.content\" isMargin=\"\"></SectionHeading>\r\n                                        <div class=\"row g-gs\">\r\n                                            <div class=\"col-lg-3 col-6\" v-for=\"item in SectionData.settingOrgData.settingOrgList\" :key=\"item.id\">\r\n                                                <router-link :to=\"item.path\" class=\"card card-cat text-center h-100\" :class=\"item.class\">\r\n                                                    <div class=\"card-body card-body-s1\">\r\n                                                        <span :class=\"item.icon\"></span>\r\n                                                        <h5 class=\"card-cat-title\">{{ item.title }}</h5>\r\n                                                    </div>\r\n                                                </router-link>\r\n                                            </div><!-- end col -->\r\n                                        </div><!-- end row -->\r\n                                    </div><!-- end container -->\r\n                                </section><!-- end category-section -->\r\n                            </div><!-- end profile-setting-panel -->\r\n                        </div><!-- end tab-pane -->\r\n                        \r\n                    <!-- ISI USER MANAGEMENT -->\r\n                    <!-- {{ userManajemen }} -->\r\n                <div class=\"tab-pane fade\" id=\"change-password\" role=\"tabpanel\" aria-labelledby=\"change-password-tab\">\r\n                    <div class=\"profile-setting-panel\">\r\n                         <div class=\"d-grid gap-2 d-md-block\">\r\n                            <router-link to=\"/user-mana\" type=\"button\" class=\"btn btn-dark btn-sm mb-2\" >Add User Management</router-link>\r\n                            <router-link to=\"/group\" type=\"button\" class=\"btn btn-dark btn-sm mb-2 ms-2 \">Group</router-link>\r\n                        </div>\r\n                       <div class=\"profile-setting-panel-wrap\">\r\n                            <div class=\"table-responsive\">\r\n                                <table class=\"table mb-0 table-s2\" id=\"dataTable\">\r\n                                    <thead class=\"fs-14\">\r\n                                        <tr>\r\n                                            <th scope=\"col\" v-for=\"(list, i) in SectionData.userMData.userMTableHead\" :key=\"i\">{{ list }}</th>\r\n                                        </tr>\r\n                                    </thead>\r\n                                    \r\n                                    <!-- isi tabel -->\r\n                                <tbody class=\"fs-13\">\r\n                                        <tr v-for=\"(item,index) in userManajemen.data\"  :key=\"item.id\">\r\n                                            <td scope=\"row\" ><a href=\"#\">{{ index+1 }}</a>  </td>\r\n                                            <td>{{ item.name }}</td>\r\n                                            <td>{{ item.email }}</td>\r\n                                            <td>{{ item.PhoneNumber }}</td>\r\n                                            <td>{{ item.status }}</td>\r\n                                            <td>{{ item.roles }}</td>\r\n                                            <td class=\"row\">\r\n                                               <router-link  :to=\"{name: 'privilage', params: { id: item.id }}\" class=\"col-sm-1 icon-btn\" title=\"Show\"><em class=\"fa-solid fa-gears\" ></em></router-link>\r\n                                                <button  @click=\"showProvinsi(item.id)\" class=\"col-sm-1 icon-btn\" title=\"Edit\"><em class=\"fa fa-pencil-square-o\" ></em></button>\r\n                                                <button  @click=\"deleteProvinsi(item.id)\" class=\"col-sm-1 icon-btn\" title=\"Delete\" ><em class=\"ni ni-trash\"></em></button>\r\n                                            </td>\r\n                                        </tr>\r\n                                    </tbody>\r\n                                </table>\r\n                            </div><!-- end table-responsive -->\r\n                            <!-- pagination -->\r\n                            <div class=\"text-center mt-4 mt-md-5\">\r\n                                <!-- <Pagination :records=\"records.length\" v-model=\"page\" :per-page=\"perPage\"></Pagination> -->\r\n                            </div>\r\n                        </div><!-- end profile-setting-panel-wrap-->\r\n                    </div><!-- end profile-setting-panel -->\r\n                </div><!-- end tab-pane -->\r\n            </div><!-- end tab-content -->\r\n            <form @submit.prevent=\"putProvinsis(edit)\">\r\n                <div class=\"modal fade\" id=\"updateModal\" tabindex=\"-1\" aria-labelledby=\"reportModalLabel\" aria-hidden=\"true\" >\r\n                    <div class=\"modal-dialog modal-dialog-centered\">\r\n                        <div class=\"modal-content\">\r\n                            <div class=\"modal-header\">\r\n                                <h4 class=\"modal-title\" id=\"reportModalLabel\">update provinsi</h4>\r\n                                <button type=\"button\" class=\"btn-close icon-btn\" data-bs-dismiss=\"modal\" aria-label=\"Close\">\r\n                                    <em class=\"ni ni-cross\"></em>\r\n                                </button>\r\n                            </div>\r\n                            <div class=\"modal-body\">\r\n                               <div class=\"form-floating mb-3\" >\r\n                                <input type=\"text\"  class=\"form-control\" id=\"namaedit\" placeholder=\"\"  v-model=\"name\"  required>\r\n                                <!-- <label for=\"nama\">{{edit.data.name}}</label> -->\r\n                            </div><!-- end form-floating -->  \r\n                                <button class=\"btn btn-dark w-100\" type=\"submit\">update</button>\r\n                            </div><!-- end modal-body -->\r\n                        </div><!-- end modal-content -->\r\n                    </div><!-- end modal-dialog -->\r\n                </div><!-- end modal-->\r\n            </form>\r\n        </div><!-- end profile-setting-panel-wrap-->\r\n    </div><!-- end col-lg-8 -->\r\n</template>\r\n\r\n<script>\r\n// Import component data. You can change the data in the store to reflect in all component\r\nimport SectionData from '@/store/store.js'\r\nimport axios from 'axios'\r\nimport $ from \"jquery\";\r\n\r\nexport default {\r\n  name: 'AccountSection',\r\n  data () {\r\n    return {\r\n      SectionData,\r\n      userManajemen: [],\r\n    }\r\n  },\r\n  methods: {\r\n    resetnama() {\r\n        this.name=null;\r\n    },\r\n    getUserManajemen: function(){\r\n        axios.get('http://127.0.0.1:8000/api/userManajemen')\r\n        .then(function (response) {\r\n                        this.userManajemen = response.data;\r\n                        $(document).ready(function () {\r\n                                        $('#dataTable').DataTable();\r\n                                    });\r\n                    }.bind(this));\r\n    },\r\n    checkPrivilege(privilege) {\r\n      const permission = localStorage.getItem(\"permission\");\r\n      let status = false;\r\n      JSON.parse(permission).forEach((data) => {\r\n        if (data === privilege) {\r\n          status = true;\r\n        }\r\n      });\r\n      return status;\r\n    },\r\n  },\r\n    created: function (){\r\n         this.getUserManajemen()\r\n  },\r\n  mounted () {\r\n    /*===========SHOW UPLOADED IMAGE ================== */\r\n    function uploadImage (selector) {\r\n      let elem = document.querySelectorAll(selector)\r\n      if (elem.length > 0) {\r\n        elem.forEach(item => {\r\n          item.addEventListener('change', function () {\r\n            if (item.files && item.files[0]) {\r\n              let img = document.getElementById(item.dataset.target)\r\n              img.onload = function () {\r\n                URL.revokeObjectURL(img.src)\r\n              }\r\n              img.src = URL.createObjectURL(item.files[0])\r\n\r\n              let allowedExtensions = ['jpg', 'JPEG', 'JPG', 'png']\r\n              let fileExtension = this.value.split('.').pop()\r\n              var lastDot = this.value.lastIndexOf('.')\r\n              var ext = this.value.substring(lastDot + 1)\r\n              var extTxt = img.value = ext\r\n              if (!allowedExtensions.includes(fileExtension)) {\r\n                alert(extTxt + ' file type not allowed, Please upload jpg, JPG, JPEG, or png file')\r\n                img.src = ' '\r\n              }\r\n            }\r\n          })\r\n        })\r\n      }\r\n    }\r\n\r\n    uploadImage('.upload-image')\r\n\r\n    /* =========== Show/Hide passoword ============== */\r\n    function showHidePassword(selector){\r\n    let elem = document.querySelectorAll(selector);\r\n    if(elem.length > 0){\r\n        elem.forEach(item => {\r\n        item.addEventListener(\"click\", function(e){\r\n            e.preventDefault();\r\n            let target = document.getElementById(item.getAttribute(\"href\"));\r\n            if(target.type == \"password\") {\r\n            target.type = \"text\";\r\n            item.classList.add(\"is-shown\");\r\n            }else{\r\n            target.type = \"password\";\r\n            item.classList.remove(\"is-shown\");\r\n            }\r\n        });\r\n\r\n        });\r\n    }\r\n    }\r\n\r\n    showHidePassword(\".password-toggle\");\r\n\r\n  }\r\n}\r\n</script>\r\n <!-- <h5 class=\"mb-4\">Change Password</h5> -->\r\n                        <!-- <div class=\"mb-3\">\r\n                            <label for=\"oldPassword\" class=\"form-label\">Old Password</label>\r\n                            <div class=\"position-relative\">\r\n                                <input type=\"password\" class=\"form-control form-control-s1\" id=\"oldPassword\" placeholder=\"Old password\">\r\n                                <a href=\"oldPassword\" class=\"password-toggle\" title=\"Toggle show/hide pasword\">\r\n                                    <em class=\"password-shown ni ni-eye-off\"></em>\r\n                                    <em class=\"password-hidden ni ni-eye\"></em>\r\n                                </a>\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label for=\"newPassword\" class=\"form-label\">New Password</label>\r\n                            <div class=\"position-relative\">\r\n                                <input type=\"password\" class=\"form-control form-control-s1\" id=\"newPassword\" placeholder=\"New password\">\r\n                                <a href=\"newPassword\" class=\"password-toggle\" title=\"Toggle show/hide pasword\">\r\n                                    <em class=\"password-shown ni ni-eye-off\"></em>\r\n                                    <em class=\"password-hidden ni ni-eye\"></em>\r\n                                </a>\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label for=\"confirmNewPassword\" class=\"form-label\">Confirm New Password</label>\r\n                            <div class=\"position-relative\">\r\n                                <input type=\"password\" class=\"form-control form-control-s1\" id=\"confirmNewPassword\" placeholder=\"Confirm new password\">\r\n                                <a href=\"confirmNewPassword\" class=\"password-toggle\" title=\"Toggle show/hide pasword\">\r\n                                    <em class=\"password-shown ni ni-eye-off\"></em>\r\n                                    <em class=\"password-hidden ni ni-eye\"></em>\r\n                                </a>\r\n                            </div>\r\n                        </div> -->\r\n                        <!-- <button class=\"btn btn-dark mt-3\" type=\"button\">Update Password</button> -->"]}]}